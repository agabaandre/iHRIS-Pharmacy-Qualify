<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="form-limits">
  <metadata>
    <displayName>Form Limits</displayName>
    <className>I2CE_Module_FormLimits</className>
    <category>Base System Component</category>
    <description>A module that enables limits for forms.</description>
    <creator>Carl Leitner</creator>
    <email>litlfred@ibiblio.org</email>
    <link>https://launchpad.net/i2ce</link>
    <version>4.3.3.0</version>
    <requirement name="forms">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
    <requirement name="field-limits">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
    <path name="classes">
      <value>./lib</value>
    </path>
    <path name="templates">
      <value>./templates</value>
    </path>
  </metadata>
</I2CEConfiguration>