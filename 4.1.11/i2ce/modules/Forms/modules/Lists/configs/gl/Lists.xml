<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Lists" path="/I2CE/">
  <metadata>
    <version>4.3.3.0.1</version>
  </metadata>
  <configurationGroup name="Lists" path="/I2CE/" locale="gl">
    <version>4.3.0.1</version>
    <configurationGroup name="auto_list" path="/I2CE/page/auto_list">
      <configuration name="title" locale="gl" values="single" type="string">
        <value>Administrar as listas da base de datos</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="Lists" path="/modules/Lists">
      <configuration name="messages" type="delimited" values="many" locale="gl">
        <value>other_lists:Outras listas</value>
        <value>title:Mostrar %s</value>
        <value>edit_title:Engadir/Actualizar %s</value>
        <value>edit_text:Actualizar esta información</value>
        <value>add_text:Engadir un novo %s</value>
        <value>add_new:Engadir un novo</value>
        <value>select_text:Seleccionar outro %s</value>
        <value>menu_title:Administrar as listas da base de datos</value>
        <value>remap_field_text:Reorganizar o campo %1$s en %2$s (%3$s coincidencias)</value>
        <value>remap_text:Definir os datos de reorganización</value>
        <value>view_remap_text:Mostrar os datos de reorganización</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="invalid_messages" path="/modules/forms/invalid_field_messages" locale="gl">
      <configuration name="unique_fields" values="single" type="string">
        <value>Este debe ser único e outro rexistro ten o mesmo valor para os valores especificados dos campos:</value>
      </configuration>
      <configuration name="unique_field" values="single" type="string">
        <value>Este debe ser único e outro rexistro ten o mesmo valor para o valor especificado do campo:</value>
      </configuration>
      <configuration name="unique" values="single" type="string">
        <value>Este debe ser único e outro rexistro ten o mesmo valor.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="template_text" path="/modules/forms/template_text">
      <configuration name="blank" values="single" locale="gl" type="string">
        <value>Seleccione un</value>
      </configuration>
      <configuration name="loading" values="single" locale="gl" type="string"></configuration>
      <configuration name="no_results" values="single" locale="gl" type="string"></configuration>
      <configuration name="default_display" values="single" locale="gl" type="string"></configuration>
      <configuration name="clear_display" values="single" locale="gl" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="formClasses" path="/modules/forms/formClasses">
      <configurationGroup name="I2CE_List">
        <configurationGroup name="fields">
          <configurationGroup name="remap">
            <configuration name="headers" type="delimited" locale="gl" values="single">
              <value>default:Valor reorganizado</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="i2ce_hidden">
            <configuration name="headers" type="delimited" locale="gl" values="single">
              <value>default:Agochar</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="I2CE_SimpleList">
        <configurationGroup name="fields">
          <configurationGroup name="name">
            <configuration name="headers" type="delimited" locale="gl" values="single">
              <value>default:Nome</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="I2CE_SimpleCodedList">
        <configurationGroup name="fields">
          <configurationGroup name="code">
            <configuration name="headers" type="delimited" locale="gl" values="single">
              <value>default:Código</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="gl">
      <configuration name="can_hide_list_members" values="single" type="string">
        <value>Pode agochar os membros da lista</value>
      </configuration>
      <configuration name="can_edit_database_lists" values="single" type="string">
        <value>Pode editar algunhas das listas almacenadas na base de datos</value>
      </configuration>
      <configuration name="can_edit_all_database_lists" values="single" type="string">
        <value>Pode editar todas as listas almacenadas na base de datos</value>
      </configuration>
      <configuration name="can_view_database_lists" values="single" type="string">
        <value>Pode ollar algunhas das listas almacenadas na base de datos</value>
      </configuration>
      <configuration name="can_view_all_database_lists" values="single" type="string">
        <value>Pode ollar todas as listas almacenadas na base de datos</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="view_list">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="gl" type="string">
            <value>Ver a lista</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>