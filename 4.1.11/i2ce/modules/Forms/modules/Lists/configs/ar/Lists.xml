<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Lists" path="/I2CE/">
  <metadata>
    <version>4.3.3.0.1</version>
  </metadata>
  <configurationGroup name="Lists" path="/I2CE/" locale="ar">
    <version>4.3.0.1</version>
    <configurationGroup name="auto_list" path="/I2CE/page/auto_list">
      <configuration name="title" locale="ar" values="single" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="Lists" path="/modules/Lists">
      <configuration name="messages" type="delimited" values="many" locale="ar">
        <value>add_text:أضف %s جديدا</value>
        <value>add_new:إضافة جديدة</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="invalid_messages" path="/modules/forms/invalid_field_messages" locale="ar">
      <configuration name="unique_fields" values="single" type="string"></configuration>
      <configuration name="unique_field" values="single" type="string"></configuration>
      <configuration name="unique" values="single" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="template_text" path="/modules/forms/template_text">
      <configuration name="blank" values="single" locale="ar" type="string"></configuration>
      <configuration name="loading" values="single" locale="ar" type="string"></configuration>
      <configuration name="no_results" values="single" locale="ar" type="string"></configuration>
      <configuration name="default_display" values="single" locale="ar" type="string"></configuration>
      <configuration name="clear_display" values="single" locale="ar" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="formClasses" path="/modules/forms/formClasses">
      <configurationGroup name="I2CE_List">
        <configurationGroup name="fields">
          <configurationGroup name="remap">
            <configuration name="headers" type="delimited" locale="ar" values="single"></configuration>
          </configurationGroup>
          <configurationGroup name="i2ce_hidden">
            <configuration name="headers" type="delimited" locale="ar" values="single">
              <value>default:إخفاء</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="I2CE_SimpleList">
        <configurationGroup name="fields">
          <configurationGroup name="name">
            <configuration name="headers" type="delimited" locale="ar" values="single">
              <value>default:اسم</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="I2CE_SimpleCodedList">
        <configurationGroup name="fields">
          <configurationGroup name="code">
            <configuration name="headers" type="delimited" locale="ar" values="single"></configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="ar">
      <configuration name="can_hide_list_members" values="single" type="string">
        <value>يمكن إخفاء&#160;أعضاء القائمة</value>
      </configuration>
      <configuration name="can_edit_database_lists" values="single" type="string">
        <value>يمكن&#160;تعديل&#160;بعض&#160;القوائم&#160;المخزنة في قاعدة البيانات</value>
      </configuration>
      <configuration name="can_edit_all_database_lists" values="single" type="string">
        <value>يمكن&#160;تعديل&#160;جميع&#160;القوائم&#160;المخزنة في قاعدة البيانات</value>
      </configuration>
      <configuration name="can_view_database_lists" values="single" type="string">
        <value>يمكن عرض&#160;بعض القوائم&#160;المخزنة في قاعدة البيانات</value>
      </configuration>
      <configuration name="can_view_all_database_lists" values="single" type="string">
        <value>يمكن عرض&#160;كل القوائم&#160;المخزنة في قاعدة البيانات</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="view_list">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ar" type="string">
            <value>عرض&#160;القائمة</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>