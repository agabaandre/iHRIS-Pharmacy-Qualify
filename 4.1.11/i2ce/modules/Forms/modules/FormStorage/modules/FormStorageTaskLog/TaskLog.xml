<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="form-task-log">
  <metadata>
    <displayName>Form Task Logger</displayName>
    <className>I2CE_Module_TaskLog_FormStorage</className>
    <category>Base System Component</category>
    <description>A module that enables logging of data changes related to tasks</description>
    <creator>Carl Leitner</creator>
    <email>litlfred@ibiblio.org</email>
    <link>https://launchpad.net/i2ce</link>
    <version>4.3.0</version>
    <requirement name="forms-storage">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
    <path name="classes">
      <value>./lib</value>
    </path>
    <path name="sql">
      <value>./sql</value>
    </path>
  </metadata>
</I2CEConfiguration>