<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="forms-storage-entry">
  <metadata>
    <displayName>Form Storage - Entry</displayName>
    <className>I2CE_Module_FormStorageEntry</className>
    <category>Base System Component</category>
    <description>A module that enables storage of Forms to a entry database structure that enables historical tracking and automatic extension for new fields.</description>
    <creator>Luke Duncan</creator>
    <email>lduncan@intrahealth.org</email>
    <link>https://launchpad.net/i2ce</link>
    <version>4.3.3.0</version>
    <requirement name="forms-storage">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
    <path name="classes">
      <value>./lib</value>
    </path>
    <path name="sql">
      <value>./sql</value>
    </path>
  </metadata>
</I2CEConfiguration>