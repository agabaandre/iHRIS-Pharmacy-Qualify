<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="forms-storage-SDMXHD">
  <metadata>
    <displayName>Form Storage - SDMX-HD</displayName>
    <category>Base System Component</category>
    <description>A module that enables reading storage of Forms from a SDMX CodeList or CrossSectionalData</description>
    <creator>Luke Duncan</creator>
    <email>lduncan@intrahealth.org</email>
    <link>https://launchpad.net/i2ce</link>
    <version>4.3.0</version>
    <requirement name="forms-storage">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
    <requirement name="forms-storage-xml">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
    <path name="classes">
      <value>./lib</value>
    </path>
  </metadata>
</I2CEConfiguration>