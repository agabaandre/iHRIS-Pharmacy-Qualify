<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="formRelationship-viewer">
  <metadata>
    <displayName>Form Relationship Viewer</displayName>
    <description>Provides Viewing and Edit of instances of a Form Relationships for use by a Swiss Factory</description>
    <link>https://launchpad.net/i2ce</link>
    <version>4.3.0</version>
    <requirement name="formRelationships">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
    <requirement name="forms-storage">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
    <requirement name="swissfactory">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
    <path name="classes">
      <value>./lib</value>
    </path>
    <path name="templates">
      <value>./templates</value>
    </path>
  </metadata>
  <configurationGroup name="formRelationship-viwer" path="/" />
</I2CEConfiguration>