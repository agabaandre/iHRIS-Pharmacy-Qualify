<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="MooTools">
  <metadata>
    <displayName>MooTools</displayName>
    <category>External Software</category>
    <description>MooTools javascript library</description>
    <creator>Intrahealth Informatics</creator>
    <email>hris@capacityproject.org</email>
    <link>https://launchpad.net/i2ce</link>
    <version>1.4.5</version>
    <path name="scripts">
      <value>./scripts</value>
    </path>
    <path name="modules">
      <value>./modules</value>
    </path>
    <requirement name="I2CE">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
  </metadata>
</I2CEConfiguration>