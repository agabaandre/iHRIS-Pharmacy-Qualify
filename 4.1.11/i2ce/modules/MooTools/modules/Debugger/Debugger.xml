<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="fancyDebug">
  <metadata>
    <displayName>Fancy Debugger</displayName>
    <className>I2CE_Module_Debugging</className>
    <category>Errors</category>
    <description>A fancy error displaying system</description>
    <creator>Carl Leitner</creator>
    <email>litlfred@ibiblio.org</email>
    <link>http://www.capacityproject.org</link>
    <version>4.3.0</version>
    <path name="scripts">
      <value>./scripts</value>
    </path>
    <path name="images">
      <value>./images</value>
    </path>
    <path name="css">
      <value>./css</value>
    </path>
    <requirement name="pages">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
    <requirement name="MooTools">
      <atLeast version="1.4" />
      <lessThan version="1.5" />
    </requirement>
  </metadata>
</I2CEConfiguration>