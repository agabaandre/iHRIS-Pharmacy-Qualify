<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="FormWorm">
  <metadata>
    <displayName>Form Worm</displayName>
    <className>I2CE_Module_FormWorm</className>
    <category>Base System Component</category>
    <description>A collection of javascript utilities to handle form verification and submission of forms with multiple actions</description>
    <creator>Carl Leitner</creator>
    <email>litlfred@ibiblio.org</email>
    <link>https://launchpad.net/i2ce</link>
    <version>4.3.0</version>
    <requirement name="pages">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
    <requirement name="MooTools-I2CE">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
    <path name="scripts">
      <value>./scripts</value>
    </path>
    <path name="css">
      <value>./css</value>
    </path>
  </metadata>
</I2CEConfiguration>