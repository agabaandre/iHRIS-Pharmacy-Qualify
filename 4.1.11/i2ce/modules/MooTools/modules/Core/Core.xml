<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="MooTools-I2CE">
  <metadata>
    <displayName>I2CE Library</displayName>
    <className>I2CE_MootoolsCore</className>
    <category>Base System Component</category>
    <description>I2CE MooTools core library</description>
    <creator>Carl Leitner</creator>
    <email>litlfred@ibiblio.org</email>
    <link>https://launchpad.net/i2ce</link>
    <version>4.3.0</version>
    <requirement name="MooTools">
      <atLeast version="1.4" />
      <lessThan version="1.5" />
    </requirement>
    <path name="scripts">
      <value>./scripts</value>
    </path>
    <path name="css">
      <value>./css</value>
    </path>
  </metadata>
</I2CEConfiguration>