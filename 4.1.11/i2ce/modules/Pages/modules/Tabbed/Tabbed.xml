<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="tabbed-pages">
  <metadata>
    <displayName>Tabbed Pages</displayName>
    <category>Pages</category>
    <description>The I2CE module pages system</description>
    <creator>Intrahealth Informatics</creator>
    <email>hris@capacityproject.org</email>
    <link>https://launchpad.net/i2ce</link>
    <version>4.3.0</version>
    <path name="classes">
      <value>./lib</value>
    </path>
    <path name="templates">
      <value>./templates</value>
    </path>
    <path name="css">
      <value>./css</value>
    </path>
    <path name="scripts">
      <value>./scripts</value>
    </path>
    <requirement name="pages">
      <atLeast version="4.3" />
      <lessThan version="4.4" />
    </requirement>
    <requirement name="MooTools">
      <atLeast version="1.4" />
      <lessThan version="1.5" />
    </requirement>
  </metadata>
</I2CEConfiguration>