<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="web-services" path="/modules/web-services">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="web-services" path="/modules/web-services" locale="fr">
    <version>4.3.0.1</version>
    <configurationGroup name="messages" locale="fr">
      <configuration name="unknown_error" values="single" type="string">
        <value>Une erreur inconnue s'est produite.</value>
      </configuration>
      <configuration name="noaccess" values="single" type="string">
        <value>Vous n'avez pas accès à ce service. Veuillez vous connecter ou contacter l'administrateur.</value>
      </configuration>
      <configuration name="noaccess_page" values="single" type="string">
        <value>Vous n'avez pas accès au service '%s'.</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>