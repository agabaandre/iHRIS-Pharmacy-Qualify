<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="DHIS_Metadata" path="/">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="DHIS_Metadata" path="/" locale="nl">
    <version>4.3.0.1</version>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="dhis_submit">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Indienen bij DHIS</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="dhis_metadata">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Medata uploaden naar DHIS</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="nl">
      <configuration name="can_edit_database_list_dhis_orgunit" locale="nl">
        <value>Kan de lijst met organisatorische eenheden voor DHIS bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_dhis_orgunit" locale="nl">
        <value>Kan de lijst met organisatorische eenheden voor DHIS bekijken</value>
      </configuration>
      <configuration name="can_edit_database_list_dhis_data_element" locale="nl">
        <value>Kan de lijst met gegevenselementen voor DHIS bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_dhis_data_element" locale="nl">
        <value>Kan de lijst met gegevenselementen voor DHIS bekijken</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="dhis_data_element">
          <configuration name="display" values="single" locale="nl">
            <value>Gegevenselement</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="dhis_orgunit">
          <configuration name="display" values="single" locale="nl">
            <value>DHIS organisatorische eenheid</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_DHIS_CodedList">
          <configurationGroup name="fields">
            <configurationGroup name="dhis_uid">
              <configuration name="headers" type="delimited" locale="nl"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>