<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="DHIS_Metadata" path="/">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="DHIS_Metadata" path="/" locale="sco">
    <version>4.2.0.1</version>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="dhis_submit">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="sco">
            <value>Haun in til DHIS</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="dhis_metadata">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="sco">
            <value>DHIS Metadata Uplaid</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="sco">
      <configuration name="can_edit_database_list_dhis_orgunit" locale="sco">
        <value>Can eedit the DHIS Organisational unit leet</value>
      </configuration>
      <configuration name="can_view_database_list_dhis_orgunit" locale="sco">
        <value>Can see the DHIS Organisational unit leet</value>
      </configuration>
      <configuration name="can_edit_database_list_dhis_data_element" locale="sco">
        <value>Can eedit the DHIS data element leet</value>
      </configuration>
      <configuration name="can_view_database_list_dhis_data_element" locale="sco">
        <value>Can see DHIS data element leet</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="dhis_data_element">
          <configuration name="display" values="single" locale="sco">
            <value>The Data Element</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="dhis_orgunit">
          <configuration name="display" values="single" locale="sco">
            <value>The DHIS Organisational Unit</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_DHIS_CodedList">
          <configurationGroup name="fields">
            <configurationGroup name="dhis_uid">
              <configuration name="headers" type="delimited" locale="sco">
                <value>default:The DHIS UID</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>