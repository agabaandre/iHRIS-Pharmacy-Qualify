<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="DHIS_Metadata" path="/">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="DHIS_Metadata" path="/" locale="es">
    <version>4.3.0.1</version>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="dhis_submit">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="es">
            <value>Enviar a DHIS</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="dhis_metadata">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="es">
            <value>Subir metadatos de DHIS</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="es">
      <configuration name="can_edit_database_list_dhis_orgunit" locale="es">
        <value>Puede editar la lista de unidad organizativa de DHIS</value>
      </configuration>
      <configuration name="can_view_database_list_dhis_orgunit" locale="es">
        <value>Puede ver la lista de unidad organizativa de DHIS</value>
      </configuration>
      <configuration name="can_edit_database_list_dhis_data_element" locale="es">
        <value>Puede editar la lista de elementos de datos de DHIS</value>
      </configuration>
      <configuration name="can_view_database_list_dhis_data_element" locale="es">
        <value>Puede ver la lista de elementos de datos de DHIS</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="dhis_data_element">
          <configuration name="display" values="single" locale="es">
            <value>Elemento de datos</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="dhis_orgunit">
          <configuration name="display" values="single" locale="es">
            <value>Unidad organizativa de DHIS</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_DHIS_CodedList">
          <configurationGroup name="fields">
            <configurationGroup name="dhis_uid">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:UID de DHIS</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>