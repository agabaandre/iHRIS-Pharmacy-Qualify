<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="DHIS_Metadata" path="/">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="DHIS_Metadata" path="/" locale="fr">
    <version>4.3.0.1</version>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="dhis_submit">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Soumettre à DHIS</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="dhis_metadata">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Téléchargement de métadonnées DHIS</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="can_edit_database_list_dhis_orgunit" locale="fr">
        <value>Peut modifier la liste des unités d’organisation DHIS</value>
      </configuration>
      <configuration name="can_view_database_list_dhis_orgunit" locale="fr">
        <value>Peut afficher la liste des unités d’organisation DHIS</value>
      </configuration>
      <configuration name="can_edit_database_list_dhis_data_element" locale="fr">
        <value>Peut modifier la liste des éléments de données DHIS</value>
      </configuration>
      <configuration name="can_view_database_list_dhis_data_element" locale="fr">
        <value>Peut afficher la liste des éléments de données DHIS</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="dhis_data_element">
          <configuration name="display" values="single" locale="fr">
            <value>Élément de données</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="dhis_orgunit">
          <configuration name="display" values="single" locale="fr">
            <value>Unité d’organisation DHIS</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_DHIS_CodedList">
          <configurationGroup name="fields">
            <configurationGroup name="dhis_uid">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:UID DHIS</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>