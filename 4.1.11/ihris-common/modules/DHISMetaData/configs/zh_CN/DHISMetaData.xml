<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="DHIS_Metadata" path="/">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="DHIS_Metadata" path="/" locale="zh_CN">
    <version>4.2.0.1</version>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="dhis_submit">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="zh_CN">
            <value>向DHIS提交</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="dhis_metadata">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="zh_CN">
            <value>DHIS元数据上传</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="zh_CN">
      <configuration name="can_edit_database_list_dhis_orgunit" locale="zh_CN">
        <value>可以编辑DHIS组织单位列表</value>
      </configuration>
      <configuration name="can_view_database_list_dhis_orgunit" locale="zh_CN">
        <value>可以查看DHIS组织单位列表</value>
      </configuration>
      <configuration name="can_edit_database_list_dhis_data_element" locale="zh_CN">
        <value>可以编辑DHIS数据元素列表</value>
      </configuration>
      <configuration name="can_view_database_list_dhis_data_element" locale="zh_CN">
        <value>可以查看DHIS数据元素列表</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="dhis_data_element">
          <configuration name="display" values="single" locale="zh_CN">
            <value>数据元素</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="dhis_orgunit">
          <configuration name="display" values="single" locale="zh_CN">
            <value>DHIS组织单位</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_DHIS_CodedList">
          <configurationGroup name="fields">
            <configurationGroup name="dhis_uid">
              <configuration name="headers" type="delimited" locale="zh_CN"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>