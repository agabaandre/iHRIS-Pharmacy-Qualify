<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="DHIS_Metadata" path="/">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="DHIS_Metadata" path="/" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="dhis_submit">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de">
            <value>Senden an DHIS</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="dhis_metadata">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de">
            <value>DHIS-Metadatenversand</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="de">
      <configuration name="can_edit_database_list_dhis_orgunit" locale="de">
        <value>Liste von DHIS-Organisationseinheiten bearbeiten können</value>
      </configuration>
      <configuration name="can_view_database_list_dhis_orgunit" locale="de">
        <value>Liste von DHIS-Organisationseinheiten betrachten können</value>
      </configuration>
      <configuration name="can_edit_database_list_dhis_data_element" locale="de">
        <value>Liste von DHIS-Datenelementen bearbeiten können</value>
      </configuration>
      <configuration name="can_view_database_list_dhis_data_element" locale="de">
        <value>Liste von DHIS-Datenelementen betrachten können</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="dhis_data_element">
          <configuration name="display" values="single" locale="de">
            <value>Datenelement</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="dhis_orgunit">
          <configuration name="display" values="single" locale="de">
            <value>DHIS-Organisationseinheit</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_DHIS_CodedList">
          <configurationGroup name="fields">
            <configurationGroup name="dhis_uid">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:DHIS-UID</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>