<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="DHIS-Dashboard" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="DHIS-Dashboard" path="/I2CE" locale="fr">
    <version>4.3.0.1</version>
    <configurationGroup name="forms" path="/modules/forms/forms/dataelement">
      <configuration name="display" values="single" locale="fr">
        <value>Élément de donnée</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/forms/dataset">
      <configuration name="display" values="single" locale="fr">
        <value>Ensemble de données</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/formClasses/">
      <configurationGroup name="iHRIS_Facility">
        <configurationGroup name="fields">
          <configurationGroup name="facility_code">
            <configuration name="headers" type="delimited" locale="fr">
              <value>default:Code d’établissement</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="iHRIS_DataElement">
        <configurationGroup name="fields">
          <configurationGroup name="name">
            <configuration name="headers" type="delimited" locale="fr">
              <value>default:Nom</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="shortname">
            <configuration name="headers" type="delimited" locale="fr">
              <value>default:Nom court</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="dataelement_code">
            <configuration name="headers" type="delimited" locale="fr"></configuration>
          </configurationGroup>
          <configurationGroup name="dataset">
            <configuration name="headers" type="delimited" locale="fr">
              <value>default:jeu de données</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="iHRIS_DataSet">
        <configurationGroup name="fields">
          <configurationGroup name="name">
            <configuration name="headers" type="delimited" locale="fr">
              <value>default:Nom</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="shortname">
            <configuration name="headers" type="delimited" locale="fr">
              <value>default:Nom court</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="dataset_code">
            <configuration name="headers" type="delimited" locale="fr"></configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="can_view_DHISDashboard" locale="fr">
        <value>afficher le tableau de bord DHIS</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="DHISDashboardExport">
        <configurationGroup name="args">
          <configuration name="title" locale="fr">
            <value>Afficher les rapports</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="dashboard">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Afficher le tableau de bord</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="dataelement">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Ajouter/Mettre à jour un élément de donnée</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="dataset">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Ajouter/Mettre à jour un ensemble de données</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>