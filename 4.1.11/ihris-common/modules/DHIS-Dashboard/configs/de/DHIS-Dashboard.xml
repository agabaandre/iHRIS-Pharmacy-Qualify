<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="DHIS-Dashboard" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="DHIS-Dashboard" path="/I2CE" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="forms" path="/modules/forms/forms/dataelement">
      <configuration name="display" values="single" locale="de">
        <value>Datenelement</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/forms/dataset">
      <configuration name="display" values="single" locale="de">
        <value>Datensatz</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/formClasses/">
      <configurationGroup name="iHRIS_Facility">
        <configurationGroup name="fields">
          <configurationGroup name="facility_code">
            <configuration name="headers" type="delimited" locale="de">
              <value>default:Einrichtungskurzzeichen</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="iHRIS_DataElement">
        <configurationGroup name="fields">
          <configurationGroup name="name">
            <configuration name="headers" type="delimited" locale="de"></configuration>
          </configurationGroup>
          <configurationGroup name="shortname">
            <configuration name="headers" type="delimited" locale="de">
              <value>default:Kurzname</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="dataelement_code">
            <configuration name="headers" type="delimited" locale="de"></configuration>
          </configurationGroup>
          <configurationGroup name="dataset">
            <configuration name="headers" type="delimited" locale="de">
              <value>default:Datensatz</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="iHRIS_DataSet">
        <configurationGroup name="fields">
          <configurationGroup name="name">
            <configuration name="headers" type="delimited" locale="de"></configuration>
          </configurationGroup>
          <configurationGroup name="shortname">
            <configuration name="headers" type="delimited" locale="de">
              <value>default:Kurzname</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="dataset_code">
            <configuration name="headers" type="delimited" locale="de"></configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="de">
      <configuration name="can_view_DHISDashboard" locale="de">
        <value>DHIS-Dashboard anzeigen</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="DHISDashboardExport">
        <configurationGroup name="args">
          <configuration name="title" locale="de">
            <value>Berichte ansehen</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="dashboard">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de">
            <value>Dashboard ansehen</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="dataelement">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de">
            <value>Datenelement hinzufügen/aktualisieren</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="dataset">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de">
            <value>Datensatz hinzufügen/aktualisieren</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>