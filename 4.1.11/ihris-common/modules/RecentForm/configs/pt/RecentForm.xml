<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-common-RecentForm" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="ihris-common-RecentForm" path="/I2CE" locale="pt">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_view_recent_forms" locale="pt">
        <value>Pode visualizar a lista das fichas que foram recentemente editadas/adicionadas.</value>
      </configuration>
