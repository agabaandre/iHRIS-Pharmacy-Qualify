<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-common-RecentForm" path="/I2CE">
  <metadata>
    <version>4.3.0</version>
  </metadata>
  <configurationGroup name="ihris-common-RecentForm" path="/I2CE" locale="en_US">
    <version>4.3.0</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_view_recent_forms" locale="en_US">
        <value>Can view the list of recent forms that have been edited/added.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="recent">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="en_US">
            <value>View Recent Forms</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>