              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Formulaire mappé</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>