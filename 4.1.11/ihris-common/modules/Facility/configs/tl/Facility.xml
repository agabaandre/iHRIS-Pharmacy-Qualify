<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Facility" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="Facility" path="/I2CE" locale="tl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_facility" locale="tl">
        <value>Baguhin ang tala ng pasilidad</value>
      </configuration>
      <configuration name="can_view_database_list_facility" locale="tl">
        <value>Tingnan ang tala ng pasilidad</value>
      </configuration>
      <configuration name="can_edit_database_list_facility_type" locale="tl">
        <value>Baguhin ang tala ng uri ng pasilidad</value>
      </configuration>
      <configuration name="can_view_database_list_facility_type" locale="tl">
        <value>Tingnan ang tala ng uri ng pasilidad</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="facility">
          <configurationGroup name="meta">
            <configuration name="description" locale="tl">
              <value>Ang pormularyong ito ay ginagagamit upang mailarawan ang saligang kabatiran patungkol sa isang pasilidad</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="facility_type">
          <configuration name="display" values="single" locale="tl">
            <value>Uri ng Pasilidad</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Facility">
          <configurationGroup name="fields">
            <configurationGroup name="facility_type">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Uri ng Pasilidad</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="tl"></configuration>
            </configurationGroup>
            <configurationGroup name="latitude">
              <configuration name="headers" type="delimited" locale="tl"></configuration>
            </configurationGroup>
            <configurationGroup name="longitude">
              <configuration name="headers" type="delimited" locale="tl"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>