<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Facility" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="Facility" path="/I2CE" locale="es">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_facility" locale="es">
        <value>Editar la lista de la institución</value>
      </configuration>
      <configuration name="can_view_database_list_facility" locale="es">
        <value>Ver la lista de la institución</value>
      </configuration>
      <configuration name="can_edit_database_list_facility_type" locale="es">
        <value>Editar el tipo de lista de la institución</value>
      </configuration>
      <configuration name="can_view_database_list_facility_type" locale="es">
        <value>Ver el tipo de lista de la institución</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="facility">
          <configurationGroup name="meta">
            <configuration name="description" locale="es">
              <value>Este formulario es usado para describir la información básica de una institución</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="facility_type">
          <configuration name="display" values="single" locale="es">
            <value>Tipo de Institución</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Facility">
          <configurationGroup name="fields">
            <configurationGroup name="facility_type">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Tipo de Institución</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="es"></configuration>
            </configurationGroup>
            <configurationGroup name="latitude">
              <configuration name="headers" type="delimited" locale="es"></configuration>
            </configurationGroup>
            <configurationGroup name="longitude">
              <configuration name="headers" type="delimited" locale="es"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>