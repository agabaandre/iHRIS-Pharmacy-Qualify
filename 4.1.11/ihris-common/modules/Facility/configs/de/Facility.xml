<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Facility" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="Facility" path="/I2CE" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_facility" locale="de">
        <value>Die Einrichtungsliste bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_facility" locale="de">
        <value>Die Einrichtungsliste betrachten</value>
      </configuration>
      <configuration name="can_edit_database_list_facility_type" locale="de">
        <value>Die Einrichtungsartenliste bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_facility_type" locale="de">
        <value>Die Einrichtungsartenliste betrachten</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="facility">
          <configurationGroup name="meta">
            <configuration name="description" locale="de">
              <value>Dieses Feld wird genutzt, um die Grundinformationen einer Einrichtung zu beschreiben</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="facility_type">
          <configuration name="display" values="single" locale="de">
            <value>Einrichtungsart</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Facility">
          <configurationGroup name="fields">
            <configurationGroup name="facility_type">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Einrichtungsart</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
            <configurationGroup name="latitude">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
            <configurationGroup name="longitude">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>