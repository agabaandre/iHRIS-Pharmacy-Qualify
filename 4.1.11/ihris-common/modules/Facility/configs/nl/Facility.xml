<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Facility" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="Facility" path="/I2CE" locale="nl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_facility" locale="nl">
        <value>Faciliteitenlijst bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_facility" locale="nl">
        <value>Faciliteitenlijst bekijken</value>
      </configuration>
      <configuration name="can_edit_database_list_facility_type" locale="nl">
        <value>Faciliteittypenlijst bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_facility_type" locale="nl">
        <value>Faciliteittypenlijst bekijken</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="facility">
          <configurationGroup name="meta">
            <configuration name="description" locale="nl">
              <value>Dit formulier wordt gebruikt om standaard gegevens over een faciliteit te beschrijven</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="facility_type">
          <configuration name="display" values="single" locale="nl">
            <value>Type instelling</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Facility">
          <configurationGroup name="fields">
            <configurationGroup name="facility_type">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Type instelling</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="nl"></configuration>
            </configurationGroup>
            <configurationGroup name="latitude">
              <configuration name="headers" type="delimited" locale="nl"></configuration>
            </configurationGroup>
            <configurationGroup name="longitude">
              <configuration name="headers" type="delimited" locale="nl"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>