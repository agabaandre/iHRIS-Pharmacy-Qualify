<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="CEUs" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="CEUs" path="/I2CE" locale="pl">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_continuing_education_course" locale="pl">
        <value>Edytuj listę kształcenia ustawicznego</value>
      </configuration>
      <configuration name="can_view_database_list_continuing_education_course" locale="pl">
        <value>Wyświetl listę kształcenia ustawicznego</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="continuing_education">
          <configuration name="display" values="single" locale="pl">
            <value>Kształcenie ustawiczne</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="continuing_education_course">
          <configuration name="display" values="single" locale="pl">
            <value>Kurs kształcenia ustawicznego</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ContinuingEducationCourse">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="pl">
                <value>default:Nazwa</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="credit_hours">
              <configuration name="headers" type="delimited" locale="pl">
                <value>default:Godziny kredytowe</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_ContinuingEducation">
          <configurationGroup name="fields">
            <configurationGroup name="continuing_education_course">
              <configuration name="headers" type="delimited" locale="pl">
                <value>default:Kurs kształcenia ustawicznego</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="credit_hours">
              <configuration name="headers" type="delimited" locale="pl">
                <value>default:Godziny kredytowe</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="pl">
                <value>default:Data rozpoczęcia</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="pl">
                <value>default:Data zakończenia</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>