        <configurationGroup name="continuing_education">
          <configuration name="display" values="single" locale="lt">
            <value>Tęstinis švietimas</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="continuing_education_course">
          <configuration name="display" values="single" locale="lt">
            <value>Tęsiamas mokymo kursas</value>
