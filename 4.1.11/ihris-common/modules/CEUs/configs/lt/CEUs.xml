            <configurationGroup name="credit_hours">
              <configuration name="headers" type="delimited" locale="lt">
                <value>default:Kreditų valandos</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="lt">
                <value>default:Pradžios data</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="lt">
                <value>default:Pabaigos data</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
