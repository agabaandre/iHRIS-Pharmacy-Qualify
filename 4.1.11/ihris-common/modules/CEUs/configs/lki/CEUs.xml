<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="CEUs" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="CEUs" path="/I2CE" locale="lki">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_continuing_education_course" locale="lki">
        <value>دسکاری فئرست ادامۀ ئآموزشی</value>
      </configuration>
      <configuration name="can_view_database_list_continuing_education_course" locale="lki">
        <value>سئرکردن فئرست ادامۀ ئآموزشی</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="continuing_education">
          <configuration name="display" values="single" locale="lki">
            <value>ادامۀ ئامووزش</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="continuing_education_course">
          <configuration name="display" values="single" locale="lki">
            <value>دورۀ ئآموزشی ادامۀ دۀ</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ContinuingEducationCourse">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="lki">
                <value>default:نۆم</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="credit_hours">
              <configuration name="headers" type="delimited" locale="lki">
                <value>default:ساتۀل-ساعتۀل اعتبارِی</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_ContinuingEducation">
          <configurationGroup name="fields">
            <configurationGroup name="continuing_education_course">
              <configuration name="headers" type="delimited" locale="lki">
                <value>default:دورۀ ئآموزشی ادامۀ دۀ</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="credit_hours">
              <configuration name="headers" type="delimited" locale="lki">
                <value>default:ساتۀل-ساعتۀل اعتبارِی</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="lki">
                <value>default:تاریخ شؤروع</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="lki">
                <value>default:دؤما تاریخ -تاریخ پایان</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>