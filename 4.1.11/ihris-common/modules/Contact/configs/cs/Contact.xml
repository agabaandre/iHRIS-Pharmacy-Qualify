<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Contact" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="Contact" path="/I2CE" locale="cs">
    <version>4.3.0.1</version>
    <configurationGroup name="/modules/forms/forms/contact_group">
      <configuration name="display" values="single" locale="cs"></configuration>
      <configurationGroup name="meta">
        <configuration name="description" locale="cs"></configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="/modules/forms/formClasses/iHRIS_Contact/fields/contact_group">
      <configuration name="headers" type="delimited" locale="cs"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Contact">
          <configurationGroup name="fields">
            <configurationGroup name="address">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Poštovní adresa</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="telephone">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Telefonní číslo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="mobile_phone">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Číslo mobilního telefonu</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="alt_telephone">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Náhradní telefonní číslo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="fax">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Číslo faxu</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="email">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Adresa elektronické pošty</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" values="many" locale="cs">
                <value>default:Poznámky</value>
                <value>contact_person:Poznámky ( hlavní kontaktní osoba)</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_NamedContact">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Jméno</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>