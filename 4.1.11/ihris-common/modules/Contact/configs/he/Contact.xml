<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Contact" path="/I2CE">
  <metadata>
    <version>4.2.1.2.1</version>
  </metadata>
  <configurationGroup name="Contact" path="/I2CE" locale="he">
    <version>4.2.1.2.1</version>
    <configurationGroup name="/modules/forms/forms/contact_group">
      <configuration name="display" values="single" locale="he"></configuration>
      <configurationGroup name="meta">
        <configuration name="description" locale="he"></configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="/modules/forms/formClasses/iHRIS_Contact/fields/contact_group">
      <configuration name="headers" type="delimited" locale="he"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Contact">
          <configurationGroup name="fields">
            <configurationGroup name="address">
              <configuration name="headers" type="delimited" locale="he">
                <value>default:כתובת למשלוח</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="telephone">
              <configuration name="headers" type="delimited" locale="he">
                <value>default:מםפר טלפון</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="mobile_phone">
              <configuration name="headers" type="delimited" locale="he">
                <value>default:מספר הטלפון הנייד</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="alt_telephone">
              <configuration name="headers" type="delimited" locale="he">
                <value>default:מספר טלפון חלופי</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="fax">
              <configuration name="headers" type="delimited" locale="he">
                <value>default:מספר פקס</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="email">
              <configuration name="headers" type="delimited" locale="he">
                <value>default:כתובת דוא"ל</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" values="many" locale="he">
                <value>default:הערות</value>
                <value>contact_person:הערות (איש הקשר העיקרי)</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_NamedContact">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="he">
                <value>default:שם</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>