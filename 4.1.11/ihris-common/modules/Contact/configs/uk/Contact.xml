<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Contact" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="Contact" path="/I2CE" locale="uk">
    <version>4.3.0.1</version>
    <configurationGroup name="/modules/forms/forms/contact_group">
      <configuration name="display" values="single" locale="uk">
        <value>Контактна група</value>
      </configuration>
      <configurationGroup name="meta">
        <configuration name="description" locale="uk">
          <value>Список контактних груп</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="/modules/forms/formClasses/iHRIS_Contact/fields/contact_group">
      <configuration name="headers" type="delimited" locale="uk">
        <value>default:Контактні групи</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Contact">
          <configurationGroup name="fields">
            <configurationGroup name="address">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Поштова адреса</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="telephone">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Номер телефону</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="mobile_phone">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Номер мобільного телефону</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="alt_telephone">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Альтернативний телефонний номер</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="fax">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Номер факсу</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="email">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Адреса електронної пошти</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" values="many" locale="uk">
                <value>default:Примітки</value>
                <value>contact_person:Примітки (Основної контактної особи)</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_NamedContact">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Ім'я</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>