              </configuration>
            </configurationGroup>
            <configurationGroup name="midpoint">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:نقطة الوسط</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:ملحوظات</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Job">
          <configurationGroup name="fields">
            <configurationGroup name="title">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:لقب</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="cadre">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:إطار&#160;(العاملون في قطاع الصحة&#160;فقط)</value>
