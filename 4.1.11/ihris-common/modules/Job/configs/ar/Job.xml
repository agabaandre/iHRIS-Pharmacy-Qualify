            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:رمز</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:وصف</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="classification">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:تصنيف</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
