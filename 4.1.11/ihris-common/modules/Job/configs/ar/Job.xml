      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="job">
          <configuration name="display" values="single" locale="ar">
            <value>وظيفة</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="classification">
          <configuration name="display" values="single" locale="ar">
            <value>تصنيف</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Classification">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:اسم</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:وصف</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:رمز</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_SalaryGrade">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:اسم</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:بداية</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end">
              <configuration name="headers" type="delimited" locale="ar">
