<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-common-Job" path="/I2CE">
  <metadata>
    <version>4.1.0.1</version>
  </metadata>
  <configurationGroup name="ihris-common-Job" path="/I2CE" locale="tl">
    <version>4.1.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_classification" locale="tl">
        <value>Baguhin ang tala ng klasipikasyon</value>
      </configuration>
      <configuration name="can_view_database_list_classification" locale="tl">
        <value>Tingnan ang tala ng klasipikasyon</value>
      </configuration>
      <configuration name="can_edit_database_list_job" locale="tl">
        <value>Baguhin ang tala ng hanapbuhay</value>
      </configuration>
      <configuration name="can_view_database_list_job" locale="tl">
        <value>Tingnan ang tala ng hanapbuhay</value>
      </configuration>
      <configuration name="can_edit_job_structure_database_lists" locale="tl">
        <value>Mababago ang ilang</value>
      </configuration>
      <configuration name="can_view_job_structure_database_lists" locale="tl">
        <value>Makikita ang ilang mga tala ng kalipunan ng dato na para sa kayarian ng hanapbuhay</value>
      </configuration>
      <configuration name="can_edit_all_job_structure_database_lists" locale="tl">
        <value>Mababago ang lahat ng tala ng kalipunan ng dato na para sa kayarian ng hanapbuhay</value>
      </configuration>
      <configuration name="can_view_all_job_structure_database_lists" locale="tl">
        <value>Makikita ang lahat ng mga tala ng kalipunan ng dato na para sa kayarian ng hanapbuhay</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="job">
          <configuration name="display" values="single" locale="tl">
            <value>Hanapbuhay</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="classification">
          <configuration name="display" values="single" locale="tl">
            <value>Klasipikasyon</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Classification">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pangalan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Paglalarawan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Kodigo</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_SalaryGrade">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pangalan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Simula</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Katapusan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="midpoint">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Kalagitnaan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Mga tala</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Job">
          <configurationGroup name="fields">
            <configurationGroup name="title">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pamagat</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="cadre">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Tagapagsanay (Mga Dalubhasang Pangkalusugan Lang)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Kodigo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Paglalarawan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="classification">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Klasipikasyon</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>