          </configuration>
        </configurationGroup>
        <configurationGroup name="classification">
          <configuration name="display" values="single" locale="cs">
            <value>Klasifikace</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Classification">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Název</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Popis</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Kód</value>
              </configuration>
