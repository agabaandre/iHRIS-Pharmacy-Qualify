      <configuration name="can_edit_database_list_job" locale="cs">
        <value>Upravit seznam práce</value>
      </configuration>
      <configuration name="can_view_database_list_job" locale="cs">
        <value>Zobrazit seznam práce</value>
      </configuration>
      <configuration name="can_edit_job_structure_database_lists" locale="cs">
        <value>Může upravit některé databázové seznamy struktury práce</value>
      </configuration>
      <configuration name="can_view_job_structure_database_lists" locale="cs">
