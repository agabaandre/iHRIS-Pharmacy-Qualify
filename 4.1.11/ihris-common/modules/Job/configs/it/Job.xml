                <value>default:Quadri(Solo i professionisti sanitari)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Codice</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Descrizione</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="classification">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Qualifica</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
