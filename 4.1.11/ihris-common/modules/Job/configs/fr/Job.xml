<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-common-Job" path="/I2CE">
  <metadata>
    <version>4.1.0.1</version>
  </metadata>
  <configurationGroup name="ihris-common-Job" path="/I2CE" locale="fr">
    <version>4.1.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_classification" locale="fr">
        <value>Éditer la liste de classification</value>
      </configuration>
      <configuration name="can_view_database_list_classification" locale="fr">
        <value>Vue la liste de classification</value>
      </configuration>
      <configuration name="can_edit_database_list_job" locale="fr">
        <value>Éditer la liste d'emploi</value>
      </configuration>
      <configuration name="can_view_database_list_job" locale="fr">
        <value>Vue la liste d'emploi</value>
      </configuration>
      <configuration name="can_edit_job_structure_database_lists" locale="fr">
        <value>Peut éditer quelques données dans la liste de classification des emplois</value>
      </configuration>
      <configuration name="can_view_job_structure_database_lists" locale="fr">
        <value>Peut voir quelques données dans la liste de classification des emplois</value>
      </configuration>
      <configuration name="can_edit_all_job_structure_database_lists" locale="fr">
        <value>Peut éditer toutes les données dans la liste de classification des emplois</value>
      </configuration>
      <configuration name="can_view_all_job_structure_database_lists" locale="fr">
        <value>Peut voir toutes les données dans la liste de classification des emplois</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="job">
          <configuration name="display" values="single" locale="fr">
            <value>Travail</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="classification">
          <configuration name="display" values="single" locale="fr"></configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Classification">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Nom</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="fr"></configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="fr"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_SalaryGrade">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Nom</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Démarrer</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Fin</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="midpoint">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Milieu</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="fr"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Job">
          <configurationGroup name="fields">
            <configurationGroup name="title">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Titre</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="cadre">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Cadre (Professionnel en santé seulement)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="fr"></configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="fr"></configuration>
            </configurationGroup>
            <configurationGroup name="classification">
              <configuration name="headers" type="delimited" locale="fr"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>