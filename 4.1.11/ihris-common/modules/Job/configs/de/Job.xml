<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-common-Job" path="/I2CE">
  <metadata>
    <version>4.1.0.1</version>
  </metadata>
  <configurationGroup name="ihris-common-Job" path="/I2CE" locale="de">
    <version>4.1.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_classification" locale="de">
        <value>Die Klassifikationsliste bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_classification" locale="de">
        <value>Die Klassifikationsliste einsehen</value>
      </configuration>
      <configuration name="can_edit_database_list_job" locale="de">
        <value>Die Tätigkeitenliste bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_job" locale="de">
        <value>Die Tätigkeitenliste einsehen</value>
      </configuration>
      <configuration name="can_edit_job_structure_database_lists" locale="de">
        <value>Kann einige Listen in der Tätigkeitsstrukturdatenbank bearbeiten</value>
      </configuration>
      <configuration name="can_view_job_structure_database_lists" locale="de">
        <value>Kann einige Listen in der Tätigkeitsstrukturdatenbank einsehen</value>
      </configuration>
      <configuration name="can_edit_all_job_structure_database_lists" locale="de">
        <value>Kann alle Listen in der Tätigkeitsstrukturdatenbank bearbeiten</value>
      </configuration>
      <configuration name="can_view_all_job_structure_database_lists" locale="de">
        <value>Kann alle Listen in der Tätigkeitsstrukturdatenbank einsehen</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="job">
          <configuration name="display" values="single" locale="de">
            <value>Tätigkeit</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="classification">
          <configuration name="display" values="single" locale="de">
            <value>Einstufung</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Classification">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Beschreibung</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_SalaryGrade">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
            <configurationGroup name="start">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Anfang</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Ende</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="midpoint">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Mittelwert</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Anmerkungen</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Job">
          <configurationGroup name="fields">
            <configurationGroup name="title">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Titel</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="cadre">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Mitarbeitergruppe (nur Gesundheitspersonal)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Beschreibung</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="classification">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Einstufung</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>