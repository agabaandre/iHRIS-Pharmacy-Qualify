        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Classification">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Nome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Descrição</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Código</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
