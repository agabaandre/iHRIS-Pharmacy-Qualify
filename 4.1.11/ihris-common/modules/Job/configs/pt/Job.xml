            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Notas</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Job">
          <configurationGroup name="fields">
