<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Currency" path="/I2CE">
  <metadata>
    <version>4.3.3.0.1</version>
  </metadata>
  <configurationGroup name="Currency" path="/I2CE" locale="ar">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="ar">
      <configuration name="can_edit_database_list_currency" locale="ar">
        <value>يمكن&#160;تعديل معلومات&#160;العملة</value>
      </configuration>
      <configuration name="can_view_database_list_currency" locale="ar">
        <value>يمكن عرض&#160;معلومات حول العملة</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="currency">
          <configuration name="display" values="single" locale="ar">
            <value>عملة</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Currency">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:رمز العملة</value>
                <value>title:رمز&#160;العملة الدولية&#160;(مثلا:&#160;"الدولار"، "الجنيه الاسترليني"، الخ.)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:اسم</value>
                <value>title:مثلا&#160;"دولار&#160;الولايات المتحدة"، "الجنيه البريطاني"</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="country">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:بلد</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="symbol">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:رمز</value>
                <value>title:مثلا&#160;"$"</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>