<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Currency" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="Currency" path="/I2CE" locale="my">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="my">
      <configuration name="can_edit_database_list_currency" locale="my">
        <value>ငွေကြေးအကြောင်းရာအား တည်းဖြတ်နိုင်သည်</value>
      </configuration>
      <configuration name="can_view_database_list_currency" locale="my">
        <value>ငွေကြေးအကြောင်းရာအား ရှု့နိုင်သည်</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="currency">
          <configuration name="display" values="single" locale="my">
            <value>ငွေကြေး</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Currency">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="my">
                <value>default:ငွေကြေးအမှတ်အသား</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="my">
                <value>default:အမည်</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="country">
              <configuration name="headers" type="delimited" locale="my">
                <value>default:နိုင်ငံ</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="symbol">
              <configuration name="headers" type="delimited" locale="my">
                <value>default:အမှတ်အသား</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>