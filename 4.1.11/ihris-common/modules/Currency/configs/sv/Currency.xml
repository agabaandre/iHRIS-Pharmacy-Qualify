<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Currency" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="Currency" path="/I2CE" locale="sv">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="sv">
      <configuration name="can_edit_database_list_currency" locale="sv">
        <value>Kan redigera valutainformation</value>
      </configuration>
      <configuration name="can_view_database_list_currency" locale="sv">
        <value>Kan titta p√• valutainformation</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="currency">
          <configuration name="display" values="single" locale="sv">
            <value>Valuta</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Currency">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="sv">
                <value>default:Valutakod</value>
                <value>title:Den internationella valutakoden (Ex. "USD", "GBP", osv.)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="sv">
                <value>default:Namn</value>
                <value>title:Ex. "Amerikanska dollar", "Brittiska pund"</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="country">
              <configuration name="headers" type="delimited" locale="sv">
                <value>default:Land</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="symbol">
              <configuration name="headers" type="delimited" locale="sv"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>