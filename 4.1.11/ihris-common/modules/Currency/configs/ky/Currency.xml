<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Currency" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="Currency" path="/I2CE" locale="ky">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="ky">
      <configuration name="can_edit_database_list_currency" locale="ky">
        <value>Акча бирдиги тууралуу маалыматты өзгөртсө болот</value>
      </configuration>
      <configuration name="can_view_database_list_currency" locale="ky">
        <value>Акча бирдиги тууралуу маалыматты көрсө болот</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="currency">
          <configuration name="display" values="single" locale="ky">
            <value>Акча бирдиги</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Currency">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="ky">
                <value>default:Акча бирдигинин коду</value>
                <value>title:Акча бирдигинин Эл аралык коду (Мис. "USD", "GBP" ж.б.)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="ky">
                <value>default:Ат</value>
                <value>title:Мис."Кошмо Штаттар Доллары", "Кыргыз сому"</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="country">
              <configuration name="headers" type="delimited" locale="ky">
                <value>default:Өлкө</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="symbol">
              <configuration name="headers" type="delimited" locale="ky">
                <value>default:Символ</value>
                <value>title:Мис. "$"</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>