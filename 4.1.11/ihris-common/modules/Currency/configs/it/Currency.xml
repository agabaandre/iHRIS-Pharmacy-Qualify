<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Currency" path="/I2CE">
  <metadata>
    <version>4.3.3.0.1</version>
  </metadata>
  <configurationGroup name="Currency" path="/I2CE" locale="it">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="it">
      <configuration name="can_edit_database_list_currency" locale="it">
        <value>Modifica le informazioni sulla valuta</value>
      </configuration>
      <configuration name="can_view_database_list_currency" locale="it">
        <value>Visualizza le informazioni sulla valuta</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="currency">
          <configuration name="display" values="single" locale="it">
            <value>Valuta</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Currency">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Codice valuta</value>
                <value>title:Il codice internazionale delle valute (Es. "USD", "GBP", etc.)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Nome</value>
                <value>title:Es. "Dollaro Statunitense", "Sterlina Britannica"</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="country">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Nazione</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="symbol">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Simbolo</value>
                <value>title:Es. "$"</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>