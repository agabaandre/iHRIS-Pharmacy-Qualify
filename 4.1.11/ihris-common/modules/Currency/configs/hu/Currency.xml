<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Currency" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="Currency" path="/I2CE" locale="hu">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="hu">
      <configuration name="can_edit_database_list_currency" locale="hu">
        <value>Szerkesztheti a devizanem információit</value>
      </configuration>
      <configuration name="can_view_database_list_currency" locale="hu">
        <value>Megtekintheti a devizanem információit</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="currency">
          <configuration name="display" values="single" locale="hu">
            <value>Pénznem</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Currency">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="hu">
                <value>default:Pénznem kód</value>
                <value>title:A nemzetközi devizanem (Pl.: "USD", "GBP", "HUF", stb.)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="hu">
                <value>default:Név</value>
                <value>title:Pl.: " United States Dollar" , "British Pound", "HUngarian Forint"</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="country">
              <configuration name="headers" type="delimited" locale="hu">
                <value>default:Ország</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="symbol">
              <configuration name="headers" type="delimited" locale="hu">
                <value>default:Szimbólum</value>
                <value>title:Pl.: "$"</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>