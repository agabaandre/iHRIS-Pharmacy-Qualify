<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Currency" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="Currency" path="/I2CE" locale="lrc">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="lrc">
      <configuration name="can_edit_database_list_currency" locale="lrc">
        <value>تونیت دونسمنیا پیل ویرایشت بکید</value>
      </configuration>
      <configuration name="can_view_database_list_currency" locale="lrc">
        <value>تونیت دونسمنیا پیل بوینیت</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="currency">
          <configuration name="display" values="single" locale="lrc">
            <value>پیل</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Currency">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="lrc">
                <value>default:کد پیل</value>
                <value>title:کد جهونی پیل(دولار امرکا،پوند بریتانیاو هنی)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="lrc">
                <value>default:نوم</value>
                <value>title:دمایی دولار امرکا ، پوند بریتانیا</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="country">
              <configuration name="headers" type="delimited" locale="lrc">
                <value>default:ولات</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="symbol">
              <configuration name="headers" type="delimited" locale="lrc">
                <value>default:نشون ون</value>
                <value>title:دمایی $</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>