<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Currency" path="/I2CE">
  <metadata>
    <version>4.3.3.0.1</version>
  </metadata>
  <configurationGroup name="Currency" path="/I2CE" locale="uk">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="uk">
      <configuration name="can_edit_database_list_currency" locale="uk">
        <value>Можете редагувати інформацію про валюту</value>
      </configuration>
      <configuration name="can_view_database_list_currency" locale="uk">
        <value>Можна переглянути відомості про валюту</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="currency">
          <configuration name="display" values="single" locale="uk">
            <value>Валюта</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Currency">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Код валюти</value>
                <value>title:Міжнародний код валюти (Напр. "USD", "UAH"та ін.)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Назва</value>
                <value>title:Ex. "Долар США", "Українська гривня"</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="country">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Країна</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="symbol">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Символ</value>
                <value>title:Напр. „₴“</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>