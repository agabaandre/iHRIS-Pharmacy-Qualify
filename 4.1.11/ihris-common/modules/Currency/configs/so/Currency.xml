<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Currency" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="Currency" path="/I2CE" locale="so">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="so">
      <configuration name="can_edit_database_list_currency" locale="so">
        <value>Waxaa suura gal ah inaad badashid macluumaadka lacagta</value>
      </configuration>
      <configuration name="can_view_database_list_currency" locale="so">
        <value>Waxaad soo bandhigi kartaa macluumaad ku saabsan lacagta</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="currency">
          <configuration name="display" values="single" locale="so">
            <value>Lacag</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Currency">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="so">
                <value>default:Sumadda lacagta</value>
                <value>title:Sumadda lacagta dibadaha (tusaale:"USD", "GBP", IWL.)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="so">
                <value>default:Magac</value>
                <value>title:Tusaale. "Doollar mareeykan" "Gani Biritin"</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="country">
              <configuration name="headers" type="delimited" locale="so">
                <value>default:Dalka</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="symbol">
              <configuration name="headers" type="delimited" locale="so">
                <value>default:Sumad</value>
                <value>title:Tusaale. "$"</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>