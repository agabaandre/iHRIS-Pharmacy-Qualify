<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="establishment" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="establishment" path="/I2CE" locale="tl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="tl">
      <configuration name="can_edit_database_list_establishment_type" locale="tl">
        <value>Mababago ang tala ng kalipunan ng dato na para sa uri ng establisamyento</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_type" locale="tl">
        <value>Matatanaaw ang tala ng kalipunan na para sa uri ng pagtatatag</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment_period" locale="tl">
        <value>Mababago ang tala ng kalipunan na para sa panahon ng pagtatatag</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_period" locale="tl">
        <value>Matatanaw ang tala ng kalipunan ng dato na para sa panahon ng establisamyento</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment" locale="tl">
        <value>Mababago ang tala ng kalipunan ng dato na para sa pagtatatag</value>
      </configuration>
      <configuration name="can_view_database_list_establishment" locale="tl">
        <value>Makikita ang tala ng kalipunan ng dato para sa establisamyento</value>
      </configuration>
      <configuration name="establishment_can_access" locale="tl">
        <value>Mapupuntahan ang kabatiran ng establisamyento</value>
      </configuration>
      <configuration name="establishment_can_edit" locale="tl">
        <value>Mababago ang kabatiran ng establisamyento</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="establishment">
          <configuration name="display" values="single" locale="tl">
            <value>Pagtatatag</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_period">
          <configuration name="display" values="single" locale="tl">
            <value>Panahon ng Pagtatatag</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_type">
          <configuration name="display" values="single" locale="tl">
            <value>Uri ng Pagtatatag</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_EstablishmentPeriod">
          <configurationGroup name="fields">
            <configurationGroup name="establishment_type">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Uri ng Pagtatatag</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Panahon ng Pagkanagagamit</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Establishment">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="tl">
              <value>%s sa %s (%s)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="establishment_period">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Panahon ng Pagtatatag</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_cadre">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Hanapbuhay o Tagapagsanay</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pasilidad o Uri ng Pasilidad</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Bilang mga Manggagawang Pangkalusugan</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>