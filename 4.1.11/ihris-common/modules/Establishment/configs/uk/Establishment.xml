<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="establishment" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="establishment" path="/I2CE" locale="uk">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="uk">
      <configuration name="can_edit_database_list_establishment_type" locale="uk">
        <value>Можна редагувати базу даних типів закладів</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_type" locale="uk">
        <value>Можна переглядати базу даних типів закладів</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment_period" locale="uk">
        <value>Можна редагувати базу даних періодів закладів</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_period" locale="uk">
        <value>Можна переглядати базу даних періодів закладів</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment" locale="uk">
        <value>Можна редагувати базу даних закладів</value>
      </configuration>
      <configuration name="can_view_database_list_establishment" locale="uk">
        <value>Можна переглядати базу даних закладів</value>
      </configuration>
      <configuration name="establishment_can_access" locale="uk">
        <value>Можна отримувати доступ до інформації про заклад</value>
      </configuration>
      <configuration name="establishment_can_edit" locale="uk">
        <value>Можна редагувати інформацію про заклад</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="establishment">
          <configuration name="display" values="single" locale="uk">
            <value>Заклад</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_period">
          <configuration name="display" values="single" locale="uk">
            <value>Період закладу</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_type">
          <configuration name="display" values="single" locale="uk">
            <value>Тип закладу</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_EstablishmentPeriod">
          <configurationGroup name="fields">
            <configurationGroup name="establishment_type">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Тип закладу</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Рік застосовності</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Establishment">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="uk">
              <value>%s у %s (%s)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="establishment_period">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Період закладу</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_cadre">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Робота або кадри</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Об'єкт або тип об'єкту</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Кількість працівників охорони здоров'я</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>