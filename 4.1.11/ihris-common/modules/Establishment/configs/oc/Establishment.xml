<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="establishment" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="establishment" path="/I2CE" locale="oc">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="oc">
      <configuration name="can_edit_database_list_establishment_type" locale="oc">
        <value>Pòt editar la lista de las bancas de donadas de tipe de las nòrmas</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_type" locale="oc">
        <value>Pòt afichar la banca de donadas de la lista de tipe de las nòrmas</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment_period" locale="oc">
        <value>Configurar las nòrmas</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_period" locale="oc"></configuration>
      <configuration name="can_edit_database_list_establishment" locale="oc"></configuration>
      <configuration name="can_view_database_list_establishment" locale="oc"></configuration>
      <configuration name="establishment_can_access" locale="oc"></configuration>
      <configuration name="establishment_can_edit" locale="oc"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="establishment">
          <configuration name="display" values="single" locale="oc">
            <value>Las nòrmas</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_period">
          <configuration name="display" values="single" locale="oc">
            <value>Lo periòde d'aplicacion de las nòrmas</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_type">
          <configuration name="display" values="single" locale="oc">
            <value>Tipe de las nòrmas</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_EstablishmentPeriod">
          <configurationGroup name="fields">
            <configurationGroup name="establishment_type">
              <configuration name="headers" type="delimited" locale="oc">
                <value>default:Tipe de las nòrmas</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="oc">
                <value>default:L'annada aplicabla</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Establishment">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="oc">
              <value>%s a %s (%s)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="establishment_period">
              <configuration name="headers" type="delimited" locale="oc">
                <value>default:Lo periòde d'aplicacion de las nòrmas</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_cadre">
              <configuration name="headers" type="delimited" locale="oc">
                <value>default:Emplec o quadre</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="oc">
                <value>default:Installacion o Tipe d’installacion</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="oc">
                <value>default:Nombre dels agents de santat</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>