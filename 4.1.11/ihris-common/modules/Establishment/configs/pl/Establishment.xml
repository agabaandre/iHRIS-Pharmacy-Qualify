<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="establishment" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="establishment" path="/I2CE" locale="pl">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pl">
      <configuration name="can_edit_database_list_establishment_type" locale="pl"></configuration>
      <configuration name="can_view_database_list_establishment_type" locale="pl"></configuration>
      <configuration name="can_edit_database_list_establishment_period" locale="pl"></configuration>
      <configuration name="can_view_database_list_establishment_period" locale="pl"></configuration>
      <configuration name="can_edit_database_list_establishment" locale="pl"></configuration>
      <configuration name="can_view_database_list_establishment" locale="pl"></configuration>
      <configuration name="establishment_can_access" locale="pl"></configuration>
      <configuration name="establishment_can_edit" locale="pl"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="establishment">
          <configuration name="display" values="single" locale="pl">
            <value>Ustanowienie</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_period">
          <configuration name="display" values="single" locale="pl">
            <value>Ustanowienie okresu</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_type">
          <configuration name="display" values="single" locale="pl">
            <value>Typ ustanowienia</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_EstablishmentPeriod">
          <configurationGroup name="fields">
            <configurationGroup name="establishment_type">
              <configuration name="headers" type="delimited" locale="pl">
                <value>default:Typ ustanowienia</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="pl">
                <value>default:Rok stosowania</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Establishment">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="pl">
              <value>%s na %s (%s)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="establishment_period">
              <configuration name="headers" type="delimited" locale="pl">
                <value>default:Ustanowienie okresu</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_cadre">
              <configuration name="headers" type="delimited" locale="pl">
                <value>default:Stanowisko lub personel</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="pl">
                <value>default:Typ obiektu lub pomieszczenia</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="pl">
                <value>default:Liczba pracowników służby zdrowia</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>