<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="establishment" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="establishment" path="/I2CE" locale="it">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="it">
      <configuration name="can_edit_database_list_establishment_type" locale="it">
        <value>È possibile modificare la lista del database del tipo di istituzione</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_type" locale="it">
        <value>È possibile visualizzare la lista del database del tipo di istituzione</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment_period" locale="it">
        <value>È possibile modificare la lista del database del periodo di istituzione</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_period" locale="it">
        <value>È possibile visualizzare la lista del database del periodo di istituzione</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment" locale="it">
        <value>È possibile modificare la lista del database dell'istituzione</value>
      </configuration>
      <configuration name="can_view_database_list_establishment" locale="it">
        <value>È possibile visualizzare la lista del database dell'istituzione</value>
      </configuration>
      <configuration name="establishment_can_access" locale="it">
        <value>È possibile accedere alle informazioni dell'istituzione</value>
      </configuration>
      <configuration name="establishment_can_edit" locale="it">
        <value>È possibile modificare le informazioni dell'istituzione</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="establishment">
          <configuration name="display" values="single" locale="it">
            <value>Istituzione</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_period">
          <configuration name="display" values="single" locale="it">
            <value>Periodo di Istituzione</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_type">
          <configuration name="display" values="single" locale="it">
            <value>Tipo di Istituzione</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_EstablishmentPeriod">
          <configurationGroup name="fields">
            <configurationGroup name="establishment_type">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Tipo di Istituzione</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Anno di applicabilità</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Establishment">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="it">
              <value>%s a %s (%s)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="establishment_period">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Periodo di Istituzione</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_cadre">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Mansione o quadro</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Struttura o tipologia struttura</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Numero di operatori sanitari</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>