<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="establishment" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="establishment" path="/I2CE" locale="cs">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="cs">
      <configuration name="can_edit_database_list_establishment_type" locale="cs">
        <value>Může upravit databázový seznam typu zařízení</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_type" locale="cs">
        <value>Může zobrazit databázový seznam typu zařízení</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment_period" locale="cs">
        <value>Může upravit databázový seznam období zařízení</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_period" locale="cs">
        <value>Může zobrazit databázový seznam období zařízení</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment" locale="cs">
        <value>Může upravit databázový seznam zařízení</value>
      </configuration>
      <configuration name="can_view_database_list_establishment" locale="cs">
        <value>Může zobrazit databázový seznam zařízení</value>
      </configuration>
      <configuration name="establishment_can_access" locale="cs">
        <value>Má přístup k informacím o zařízení</value>
      </configuration>
      <configuration name="establishment_can_edit" locale="cs">
        <value>Může upravit informace o zařízení</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="establishment">
          <configuration name="display" values="single" locale="cs">
            <value>Zařízení</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_period">
          <configuration name="display" values="single" locale="cs">
            <value>Období zařízení</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_type">
          <configuration name="display" values="single" locale="cs">
            <value>Typ zařízení</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_EstablishmentPeriod">
          <configurationGroup name="fields">
            <configurationGroup name="establishment_type">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Typ zařízení</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Rok uplatnění</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Establishment">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="cs">
              <value>%s ve %s (%s)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="establishment_period">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Období zařízení</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_cadre">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Práce nebo kádr</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Zařízení nebo typ zařízení</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Počet zdravotnických pracovníků</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>