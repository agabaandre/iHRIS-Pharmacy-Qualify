<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="establishment" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="establishment" path="/I2CE" locale="nl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="nl">
      <configuration name="can_edit_database_list_establishment_type" locale="nl">
        <value>Kan de lijst met typen betrekkingen bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_type" locale="nl">
        <value>Kan de lijst met typen betrekkingen bekijken</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment_period" locale="nl">
        <value>Kan de aanstellingsperiodedatabaselijst bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_period" locale="nl">
        <value>Kan de aanstellingsperiodedatabaselijst bekijken</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment" locale="nl">
        <value>Kan de aanstellingsdatabaselijst bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_establishment" locale="nl">
        <value>Kan de aanstellingsdatabaselijst bekijken</value>
      </configuration>
      <configuration name="establishment_can_access" locale="nl">
        <value>Heeft toegang tot de aanstellingsgegevens</value>
      </configuration>
      <configuration name="establishment_can_edit" locale="nl">
        <value>Kan de aanstellingsgegevens bewerken</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="establishment">
          <configuration name="display" values="single" locale="nl">
            <value>Aanstelling</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_period">
          <configuration name="display" values="single" locale="nl">
            <value>Aanstellingsperiode</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_type">
          <configuration name="display" values="single" locale="nl">
            <value>Type aanstelling</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_EstablishmentPeriod">
          <configurationGroup name="fields">
            <configurationGroup name="establishment_type">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Type aanstelling</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Jaar van toepasselijkheid</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Establishment">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="nl">
              <value>%s om %s (%s)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="establishment_period">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Aanstellingsperiode</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_cadre">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Functie of kader</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Faciliteit of faciliteittype</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Aantal werknemers in de gezondheidszorg</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>