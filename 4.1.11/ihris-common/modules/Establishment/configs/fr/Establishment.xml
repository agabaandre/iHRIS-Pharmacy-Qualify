<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="establishment" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="establishment" path="/I2CE" locale="fr">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="can_edit_database_list_establishment_type" locale="fr">
        <value>Peut éditer la base de donnée de la liste de type des normes</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_type" locale="fr">
        <value>Peut regarder la base de donnée de la liste de type des normes</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment_period" locale="fr">
        <value>Peut éditer la base de donnée de la période d'application des normes</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_period" locale="fr">
        <value>Peut regarder la base de donnée de la période d'application des normes</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment" locale="fr">
        <value>Peut éditer la base de donnée des normes</value>
      </configuration>
      <configuration name="can_view_database_list_establishment" locale="fr">
        <value>Peut regarder la base de donnée des normes</value>
      </configuration>
      <configuration name="establishment_can_access" locale="fr">
        <value>Peut avoir accès des information sur les normes</value>
      </configuration>
      <configuration name="establishment_can_edit" locale="fr">
        <value>Peut éditer des information sur les normes</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="establishment">
          <configuration name="display" values="single" locale="fr">
            <value>Les normes</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_period">
          <configuration name="display" values="single" locale="fr">
            <value>La période d'application des normes</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_type">
          <configuration name="display" values="single" locale="fr">
            <value>Type des normes</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_EstablishmentPeriod">
          <configurationGroup name="fields">
            <configurationGroup name="establishment_type">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Type des normes</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:L'année applicable</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Establishment">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="fr">
              <value>%s à %s (%s)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="establishment_period">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:La période d'application des normes</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_cadre">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Emploi ou cadre</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Installation ou Type d’installation</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Nombre des agents de santé</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>