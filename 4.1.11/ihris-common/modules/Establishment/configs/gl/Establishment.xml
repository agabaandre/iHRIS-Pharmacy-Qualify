<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="establishment" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="establishment" path="/I2CE" locale="gl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="gl">
      <configuration name="can_edit_database_list_establishment_type" locale="gl">
        <value>Pode editar a lista da base de datos do tipo de efectivos</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_type" locale="gl">
        <value>Pode ollar a lista da base de datos do tipo de efectivos</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment_period" locale="gl">
        <value>Pode editar a lista da base de datos do período de efectivos</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_period" locale="gl">
        <value>Pode ollar a lista da base de datos do período de efectivos</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment" locale="gl">
        <value>Pode editar a lista da base de datos dos efectivos</value>
      </configuration>
      <configuration name="can_view_database_list_establishment" locale="gl">
        <value>Pode ollar a lista da base de datos dos efectivos</value>
      </configuration>
      <configuration name="establishment_can_access" locale="gl">
        <value>Pode acceder a información dos efectivos</value>
      </configuration>
      <configuration name="establishment_can_edit" locale="gl">
        <value>Pode editar a información dos efectivos</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="establishment">
          <configuration name="display" values="single" locale="gl">
            <value>Efectivos</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_period">
          <configuration name="display" values="single" locale="gl">
            <value>Período de efectivos</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_type">
          <configuration name="display" values="single" locale="gl">
            <value>Tipo de efectivos</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_EstablishmentPeriod">
          <configurationGroup name="fields">
            <configurationGroup name="establishment_type">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Tipo de efectivos</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Ano aplicable</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Establishment">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="gl">
              <value>%s en %s (%s)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="establishment_period">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Período de efectivos</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_cadre">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Emprego ou cadro</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Establecemento ou tipo de establecemento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Número de traballadores da saúde</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>