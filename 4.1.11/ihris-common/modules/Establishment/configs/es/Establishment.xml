<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="establishment" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="establishment" path="/I2CE" locale="es">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="es">
      <configuration name="can_edit_database_list_establishment_type" locale="es">
        <value>Puede editar la lista de bases de datos de Tipos de Establecimiento</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_type" locale="es">
        <value>Puede ver la lista de Bases de Datos de Tipos de Establecimientos</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment_period" locale="es">
        <value>Puede editar la Lista Bases de Datos de Tipos de Establecimientos</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_period" locale="es">
        <value>Puede ver la Lista de Bases de Datos de Períodos de Establecimientos</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment" locale="es">
        <value>Puede editar la Lista de Bases de Datos de Establecimientos</value>
      </configuration>
      <configuration name="can_view_database_list_establishment" locale="es">
        <value>Puede ver la Lista de bases de datos de Establecimientos</value>
      </configuration>
      <configuration name="establishment_can_access" locale="es">
        <value>Puede Acceder la Información de Establecimientos</value>
      </configuration>
      <configuration name="establishment_can_edit" locale="es">
        <value>Puede editar la Información de Establecimientos</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="establishment">
          <configuration name="display" values="single" locale="es">
            <value>Establecimiento</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_period">
          <configuration name="display" values="single" locale="es">
            <value>Periodo de establecimiento</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_type">
          <configuration name="display" values="single" locale="es">
            <value>Tipo de establecimiento</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_EstablishmentPeriod">
          <configurationGroup name="fields">
            <configurationGroup name="establishment_type">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Tipo de establecimiento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Añode Aplicabilidad</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Establishment">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="es">
              <value>%s en %s (%s)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="establishment_period">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Periodo de establecimiento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_cadre">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Empleo o Cuadro</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Edificio o Tipo de Edificio</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Cantidad de Empleados de Salud</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>