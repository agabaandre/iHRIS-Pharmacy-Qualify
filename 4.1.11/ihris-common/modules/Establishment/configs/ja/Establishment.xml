<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="establishment" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="establishment" path="/I2CE" locale="ja">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="ja">
      <configuration name="can_edit_database_list_establishment_type" locale="ja">
        <value>世帯種リストを編集できる</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_type" locale="ja">
        <value>世帯種リストを閲覧できる</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment_period" locale="ja">
        <value>世帯期間リストを編集できる</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_period" locale="ja">
        <value>世帯期間リストを閲覧できる</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment" locale="ja">
        <value>世帯情報データベースを編集できる</value>
      </configuration>
      <configuration name="can_view_database_list_establishment" locale="ja">
        <value>世帯情報データベースを閲覧できる</value>
      </configuration>
      <configuration name="establishment_can_access" locale="ja">
        <value>世帯情報にアクセスできる</value>
      </configuration>
      <configuration name="establishment_can_edit" locale="ja">
        <value>世帯情報を編集できる</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="establishment">
          <configuration name="display" values="single" locale="ja">
            <value>世帯</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_period">
          <configuration name="display" values="single" locale="ja">
            <value>世帯期間</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_type">
          <configuration name="display" values="single" locale="ja">
            <value>世帯種類</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_EstablishmentPeriod">
          <configurationGroup name="fields">
            <configurationGroup name="establishment_type">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:世帯種類</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:適用年度</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Establishment">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="ja">
              <value>%s で　%s(%s)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="establishment_period">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:世帯期間</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_cadre">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:職種または職位</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:設備または施設種類</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:医療従事者数</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>