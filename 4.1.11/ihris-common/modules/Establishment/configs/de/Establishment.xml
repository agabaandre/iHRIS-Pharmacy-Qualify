<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="establishment" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="establishment" path="/I2CE" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="de">
      <configuration name="can_edit_database_list_establishment_type" locale="de">
        <value>Kann die Datenbank mit den Betriebsstättenarten bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_type" locale="de">
        <value>Kann die Datenbank mit den Betriebsstättenarten einsehen</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment_period" locale="de">
        <value>Kann die Datenbank mit den Betriebsstättenzeiträumen bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_period" locale="de">
        <value>Kann die Datenbank mit den Betriebsstättenzeiträumen einsehen</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment" locale="de">
        <value>Kann die Datenbank mit den Betriebsstätten bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_establishment" locale="de">
        <value>Kann die Datenbank mit den Betriebsstätten einsehen</value>
      </configuration>
      <configuration name="establishment_can_access" locale="de">
        <value>Kann die Betriebsstätteninformationen einsehen</value>
      </configuration>
      <configuration name="establishment_can_edit" locale="de">
        <value>Kann die Betriebsstätteninformationen bearbeiten</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="establishment">
          <configuration name="display" values="single" locale="de">
            <value>Betriebsstätte</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_period">
          <configuration name="display" values="single" locale="de">
            <value>Betriebsstättenzeitraum</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_type">
          <configuration name="display" values="single" locale="de">
            <value>Betriebsstättenart</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_EstablishmentPeriod">
          <configurationGroup name="fields">
            <configurationGroup name="establishment_type">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Betriebsstättenart</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Jahr der Nutzung</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Establishment">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="de">
              <value>%s bei %s (%s)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="establishment_period">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Betriebsstättenzeitraum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_cadre">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Stelle oder Mitarbeitergruppe</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Einrichtung oder Einrichtungsart</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Anzahl der Arbeitskräfte im Gesundheitswesen</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>