<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="establishment" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="establishment" path="/I2CE" locale="sk">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="sk">
      <configuration name="can_edit_database_list_establishment_type" locale="sk">
        <value>Možno upraviť zoznam typov databáý zariadení</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_type" locale="sk">
        <value>Možno zobraziť zoznam typov databáz zariadení</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment_period" locale="sk">
        <value>Možno upraviť zoznam databáz období administrácie</value>
      </configuration>
      <configuration name="can_view_database_list_establishment_period" locale="sk">
        <value>Možno zobraziť zoznam databáz období administrácie</value>
      </configuration>
      <configuration name="can_edit_database_list_establishment" locale="sk">
        <value>Možno upraviť zoznam databáz administrácií</value>
      </configuration>
      <configuration name="can_view_database_list_establishment" locale="sk">
        <value>Možno zobraziť zoznam databáz administrácií</value>
      </configuration>
      <configuration name="establishment_can_access" locale="sk">
        <value>Možný prístup k informácii administrácie</value>
      </configuration>
      <configuration name="establishment_can_edit" locale="sk">
        <value>Možno upraviť informáciu administrácie</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="establishment">
          <configuration name="display" values="single" locale="sk">
            <value>Administrácia</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_period">
          <configuration name="display" values="single" locale="sk">
            <value>Obdobnie administrácie</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="establishment_type">
          <configuration name="display" values="single" locale="sk">
            <value>Typ administrácie</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_EstablishmentPeriod">
          <configurationGroup name="fields">
            <configurationGroup name="establishment_type">
              <configuration name="headers" type="delimited" locale="sk">
                <value>default:Typ administrácie</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="sk">
                <value>default:Rok platnosti</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Establishment">
          <configurationGroup name="meta" path="meta/list/default">
            <configuration name="display_string" locale="sk">
              <value>%s zo %s (%s)</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="establishment_period">
              <configuration name="headers" type="delimited" locale="sk">
                <value>default:Obdobnie administrácie</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job_cadre">
              <configuration name="headers" type="delimited" locale="sk">
                <value>default:práca alebo funkcia</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="sk"></configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="sk">
                <value>default:Počet pracovníkov v zdravotníctve</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>