<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="csd-data-dow">
  <metadata>
    <displayName>CSD Data Day Of Week</displayName>
    <category>Application Component</category>
    <description>CSD Data - Day Of Week</description>
    <creator>IntraHealth Informatics</creator>
    <email>litlfred@gmail.com</email>
    <link>https://launchpad.net/i2ce</link>
    <version>4.3.0</version>
    <path name="configs">
      <value>./configs</value>
    </path>
  </metadata>
  <configurationGroup name="csd-data-dow" path="/modules/forms">
    <configuration name="storage" path="/modules/forms/forms/day_of_week/storage">
      <value>magicdata</value>
    </configuration>
    <!--REQUIRED VALUES-->
    <configurationGroup name="day_of_week" path="/I2CE/formsData/forms/day_of_week">
      <version>4.2.0.12</version>
      <configurationGroup name="0">
        <configuration name="last_modified">
          <value>2014-01-16 16:20:00</value>
        </configuration>
        <configurationGroup name="fields">
          <configuration name="name" locale="en_US">
            <value>Sunday</value>
          </configuration>
          <configuration name="code" locale="en_US">
            <value>0</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="1">
        <configuration name="last_modified">
          <value>2014-01-16 16:20:00</value>
        </configuration>
