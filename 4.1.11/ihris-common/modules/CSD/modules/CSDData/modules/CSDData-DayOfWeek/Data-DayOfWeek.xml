            <value>Monday</value>
          </configuration>
          <configuration name="code" locale="en_US">
            <value>1</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="2">
        <configuration name="last_modified">
          <value>2014-01-16 16:20:00</value>
