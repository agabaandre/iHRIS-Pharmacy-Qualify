<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="simple-competency" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="simple-competency" path="/I2CE" locale="sv">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_lists_competency" locale="sv">
        <value>Redigera alla listor som är associerade med kompetens</value>
      </configuration>
      <configuration name="can_edit_database_list_competency" locale="sv">
        <value>Redigera kompetenslistan</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_type" locale="sv">
        <value>Redigera kompetenstypslistan</value>
      </configuration>
      <configuration name="can_view_database_lists_competency" locale="sv">
        <value>Visa alla listor som är associerade med kompetens</value>
      </configuration>
      <configuration name="can_view_database_list_competency" locale="sv">
        <value>Visa kompetenslistan</value>
      </configuration>
      <configuration name="can_view_database_list_competency_type" locale="sv">
        <value>Visa kompetenstypslistan</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency">
          <configuration name="display" values="single" locale="sv">
            <value>Kompetens</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="competency_type">
          <configuration name="display" values="single" locale="sv">
            <value>Kompetenstyp</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Competency">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="sv">
                <value>default:Namn</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_type">
              <configuration name="headers" type="delimited" values="many" locale="sv">
                <value>default:Kompetenstyp</value>
                <value>select_competency_type:Välj kompetenstyp, sedan kompetens</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="sv">
                <value>default:Anteckningar</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>