<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="simple-competency" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="simple-competency" path="/I2CE" locale="br">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_lists_competency" locale="br">
        <value>Kemmañ an holl rolloù all kevelet ouzh ar varregezh</value>
      </configuration>
      <configuration name="can_edit_database_list_competency" locale="br">
        <value>Kemmañ ar roll barregezhioù</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_type" locale="br">
        <value>Kemmañ ar roll seurtoù barregezh</value>
      </configuration>
      <configuration name="can_view_database_lists_competency" locale="br">
        <value>Diskwel an holl rolloù kevelet ouzh ar varregezh</value>
      </configuration>
      <configuration name="can_view_database_list_competency" locale="br">
        <value>Diskwel roll ar barregezhioù</value>
      </configuration>
      <configuration name="can_view_database_list_competency_type" locale="br">
        <value>Diskwel roll ar seurtoù barregezhioù</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency">
          <configuration name="display" values="single" locale="br">
            <value>Barregezh</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="competency_type">
          <configuration name="display" values="single" locale="br">
            <value>Seurt barregezh</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Competency">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="br">
                <value>default:Anv</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_type">
              <configuration name="headers" type="delimited" values="many" locale="br">
                <value>default:Seurt barregezh</value>
                <value>select_competency_type:Dibar seurt barregezh, ha barregezh da choude</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="br">
                <value>default:Notennoù</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>