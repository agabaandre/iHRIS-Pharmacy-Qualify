<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="simple-competency" path="/I2CE" locale="gl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_lists_competency" locale="gl">
        <value>Editar todas as listas asociadas รก competencia</value>
      </configuration>
      <configuration name="can_edit_database_list_competency" locale="gl">
        <value>Editar a lista de competencias</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_type" locale="gl">
        <value>Editar a lista dos tipos de competencias</value>
      </configuration>
      <configuration name="can_view_database_lists_competency" locale="gl">
        <value>Ollar todas as listas asociadas รก competencia</value>
      </configuration>
      <configuration name="can_view_database_list_competency" locale="gl">
        <value>Ollar a lista de competencias</value>
      </configuration>
      <configuration name="can_view_database_list_competency_type" locale="gl">
        <value>Ollar a lista dos tipos de competencias</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency">
          <configuration name="display" values="single" locale="gl">
            <value>Competencia</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="competency_type">
          <configuration name="display" values="single" locale="gl">
            <value>Tipo de competencias</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Competency">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Nome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_type">
              <configuration name="headers" type="delimited" values="many" locale="gl">
                <value>default:Tipo de competencias</value>
                <value>select_competency_type:Seleccione o tipo de competencia; logo, a competencia</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Notas</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>