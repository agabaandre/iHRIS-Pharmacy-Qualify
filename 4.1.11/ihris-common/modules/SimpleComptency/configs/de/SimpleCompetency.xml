<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="simple-competency" path="/I2CE" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_lists_competency" locale="de">
        <value>Alle Listen bearbeiten, die mit Fähigkeiten verknüpft sind</value>
      </configuration>
      <configuration name="can_edit_database_list_competency" locale="de">
        <value>Liste der Fähigkeiten bearbeiten</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_type" locale="de">
        <value>Liste der Fähigkeitsarten bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_lists_competency" locale="de">
        <value>Alle Listen einsehen, die mit Fähigkeiten verknüpft sind</value>
      </configuration>
      <configuration name="can_view_database_list_competency" locale="de">
        <value>Liste der Fähigkeiten einsehen</value>
      </configuration>
      <configuration name="can_view_database_list_competency_type" locale="de">
        <value>Liste der Fähigkeitsarten einsehen</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency">
          <configuration name="display" values="single" locale="de">
            <value>Fähigkeiten</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="competency_type">
          <configuration name="display" values="single" locale="de">
            <value>Art der Fähigkeit</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Competency">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
            <configurationGroup name="competency_type">
              <configuration name="headers" type="delimited" values="many" locale="de">
                <value>default:Art der Fähigkeit</value>
                <value>select_competency_type:Zunächst die Fähigkeitsart auswählen und danach die Fähigkeit</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Anmerkungen</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>