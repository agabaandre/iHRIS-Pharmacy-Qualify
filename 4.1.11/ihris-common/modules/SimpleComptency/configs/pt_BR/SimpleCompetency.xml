<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="simple-competency" path="/I2CE" locale="pt_BR">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_lists_competency" locale="pt_BR">
        <value>Editar todas as listas associadas com a competência</value>
      </configuration>
      <configuration name="can_edit_database_list_competency" locale="pt_BR">
        <value>Editar a lista de competências</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_type" locale="pt_BR">
        <value>Editar a lista de tipos de competências</value>
      </configuration>
      <configuration name="can_view_database_lists_competency" locale="pt_BR">
        <value>Visualizar todas as listas associadas com a competência</value>
      </configuration>
      <configuration name="can_view_database_list_competency" locale="pt_BR">
        <value>Visualizar a lista de competências</value>
      </configuration>
      <configuration name="can_view_database_list_competency_type" locale="pt_BR">
        <value>Visualizar a lista de tipos de competência</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency">
          <configuration name="display" values="single" locale="pt_BR">
            <value>Competência</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="competency_type">
          <configuration name="display" values="single" locale="pt_BR">
            <value>Tipo de Competência</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Competency">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Nome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_type">
              <configuration name="headers" type="delimited" values="many" locale="pt_BR">
                <value>default:Tipo de Competência</value>
                <value>select_competency_type:Selecione o Tipo de Competência, e então a Competência</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Notas</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>