<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="simple-competency" path="/I2CE" locale="it">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_lists_competency" locale="it">
        <value>Modifica tutti gli elenchi associati alla competenza</value>
      </configuration>
      <configuration name="can_edit_database_list_competency" locale="it">
        <value>Modifica l'elenco delle competenze</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_type" locale="it">
        <value>Modifica l'elenco delle tipologie di competenze</value>
      </configuration>
      <configuration name="can_view_database_lists_competency" locale="it">
        <value>Visualizza tutti gli elenchi associati alla competenza</value>
      </configuration>
      <configuration name="can_view_database_list_competency" locale="it">
        <value>VIsualizza l'elenco delle competenze</value>
      </configuration>
      <configuration name="can_view_database_list_competency_type" locale="it">
        <value>Visualizza l'elenco delle tipologie di competenze</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency">
          <configuration name="display" values="single" locale="it">
            <value>Competenza</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="competency_type">
          <configuration name="display" values="single" locale="it">
            <value>Tipo di competenza</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Competency">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Nome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_type">
              <configuration name="headers" type="delimited" values="many" locale="it">
                <value>default:Tipo di competenza</value>
                <value>select_competency_type:Seleziona la tipologia di comptenza e poi la competenza</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Note</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>