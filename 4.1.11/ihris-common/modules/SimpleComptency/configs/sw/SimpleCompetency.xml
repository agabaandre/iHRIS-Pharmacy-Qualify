<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="simple-competency" path="/I2CE" locale="sw">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_lists_competency" locale="sw">
        <value>Hariri orodha zote zinazohusiana na uwezo</value>
      </configuration>
      <configuration name="can_edit_database_list_competency" locale="sw">
        <value>Hariri orodha ya uwezo</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_type" locale="sw">
        <value>Hariri orodha ya aina ya uwezo</value>
      </configuration>
      <configuration name="can_view_database_lists_competency" locale="sw">
        <value>Angalia orodha zote zinazohusiana na uwezo</value>
      </configuration>
      <configuration name="can_view_database_list_competency" locale="sw">
        <value>Angalia orodha ya uwezo</value>
      </configuration>
      <configuration name="can_view_database_list_competency_type" locale="sw">
        <value>Angalia orodha ya aina ya uwezo</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency">
          <configuration name="display" values="single" locale="sw">
            <value>Uwezo</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="competency_type">
          <configuration name="display" values="single" locale="sw">
            <value>Aina ya Uwezo</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Competency">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="sw">
                <value>default:Jina</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_type">
              <configuration name="headers" type="delimited" values="many" locale="sw">
                <value>default:Aina ya Uwezo</value>
                <value>select_competency_type:Chagua Aina ya Uwezo halafu Uwezo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="sw">
                <value>default:Maelezo</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>