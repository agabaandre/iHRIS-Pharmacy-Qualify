<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="simple-competency" path="/I2CE" locale="es">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_lists_competency" locale="es">
        <value>Editar todas las listas asociadas con la competencia</value>
      </configuration>
      <configuration name="can_edit_database_list_competency" locale="es">
        <value>Editar la lista de competencia</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_type" locale="es">
        <value>Editar la lista de tipos de competencia</value>
      </configuration>
      <configuration name="can_view_database_lists_competency" locale="es">
        <value>Ver todas las listas asociadas con la competencia</value>
      </configuration>
      <configuration name="can_view_database_list_competency" locale="es">
        <value>Ver la lista de competencia</value>
      </configuration>
      <configuration name="can_view_database_list_competency_type" locale="es">
        <value>Ver la lista de tipos de competencia</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency">
          <configuration name="display" values="single" locale="es">
            <value>Competencia</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="competency_type">
          <configuration name="display" values="single" locale="es">
            <value>Tipo de Competencia</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Competency">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Nombre</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_type">
              <configuration name="headers" type="delimited" values="many" locale="es">
                <value>default:Tipo de Competencia</value>
                <value>select_competency_type:Seleccione Tipo de Competencia despu√©s Competencia</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Notas</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>