<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="simple-competency" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="simple-competency" path="/I2CE" locale="sk">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_lists_competency" locale="sk">
        <value>Uprav celý zoznam priradený ku spôsobilosti</value>
      </configuration>
      <configuration name="can_edit_database_list_competency" locale="sk">
        <value>Uprav zoznam spôsobilost</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_type" locale="sk">
        <value>Uprav zoznam spôsobilosti</value>
      </configuration>
      <configuration name="can_view_database_lists_competency" locale="sk">
        <value>Zobraz celý zoznam priradený ku spôsobilosti</value>
      </configuration>
      <configuration name="can_view_database_list_competency" locale="sk">
        <value>Zobraz zoznam spôsobilostí</value>
      </configuration>
      <configuration name="can_view_database_list_competency_type" locale="sk">
        <value>Zobraz zoznam typov spôsobilostí</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency">
          <configuration name="display" values="single" locale="sk">
            <value>Spôsobilosť</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="competency_type">
          <configuration name="display" values="single" locale="sk">
            <value>Typ spôsobilosti</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Competency">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="sk">
                <value>default:Názov</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_type">
              <configuration name="headers" type="delimited" values="many" locale="sk">
                <value>default:Typ spôsobilosti</value>
                <value>select_competency_type:vyber typ spôsobilosti potom spôsobilosť</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="sk">
                <value>default:Poznámky</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>