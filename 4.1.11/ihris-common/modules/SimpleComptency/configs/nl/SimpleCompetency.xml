<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="simple-competency" path="/I2CE" locale="nl">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_lists_competency" locale="nl">
        <value>Alle lijsten die te maken hebben met competentie bewerken</value>
      </configuration>
      <configuration name="can_edit_database_list_competency" locale="nl">
        <value>Competentielijst bewerken</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_type" locale="nl">
        <value>Competentietypelijst bewerken</value>
      </configuration>
      <configuration name="can_view_database_lists_competency" locale="nl">
        <value>Alle lijsten die te maken hebben met competentie bekijken</value>
      </configuration>
      <configuration name="can_view_database_list_competency" locale="nl">
        <value>Competentielijst bekijken</value>
      </configuration>
      <configuration name="can_view_database_list_competency_type" locale="nl">
        <value>Competentietypelijst bekijken</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency">
          <configuration name="display" values="single" locale="nl">
            <value>Competentie</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="competency_type">
          <configuration name="display" values="single" locale="nl">
            <value>Competentietype</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Competency">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Naam</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_type">
              <configuration name="headers" type="delimited" values="many" locale="nl">
                <value>default:Competentietype</value>
                <value>select_competency_type:Selecteer een competentietype en daarna een competentie</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Opmerkingen</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>