<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="simple-competency" path="/I2CE" locale="cs">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_lists_competency" locale="cs">
        <value>Upravit celý seznam spojený s kompetencí</value>
      </configuration>
      <configuration name="can_edit_database_list_competency" locale="cs">
        <value>Upravit seznam kompetence</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_type" locale="cs">
        <value>Upravit seznam typů kompetence</value>
      </configuration>
      <configuration name="can_view_database_lists_competency" locale="cs">
        <value>Zobrazit celý seznam spojený s kompetencí</value>
      </configuration>
      <configuration name="can_view_database_list_competency" locale="cs">
        <value>Zobrazit seznam kompetence</value>
      </configuration>
      <configuration name="can_view_database_list_competency_type" locale="cs">
        <value>Zobrazit seznam typů kompetence</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency">
          <configuration name="display" values="single" locale="cs">
            <value>Kompetence</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="competency_type">
          <configuration name="display" values="single" locale="cs">
            <value>Typ kompetence</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Competency">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Název</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_type">
              <configuration name="headers" type="delimited" values="many" locale="cs">
                <value>default:Typ kompetence</value>
                <value>select_competency_type:Vyberte typ kompetence pak kompetenci</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Poznámky</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>