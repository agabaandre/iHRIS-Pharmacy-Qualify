<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonEducation" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonEducation" path="/I2CE" locale="pt">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pt">
      <configuration name="person_can_view_child_form_education" locale="pt">
        <value>Pode visualizar os child forms de educação de uma pessoa</value>
      </configuration>
      <configuration name="person_can_edit_child_form_education" locale="pt">
        <value>Pode editar os child forms de educação de uma pessoa</value>
      </configuration>
      <configuration name="can_edit_database_list_edu_type" locale="pt">
        <value>Pode editar as listas de dados de base sobre o tipo de educação</value>
      </configuration>
      <configuration name="can_view_database_list_edu_type" locale="pt">
        <value>Pode visualizar as listas de dados de base sobre o tipo de educação</value>
      </configuration>
      <configuration name="can_edit_database_list_degree" locale="pt">
        <value>Pode editar as listas de dados de base sobre o grau</value>
      </configuration>
      <configuration name="can_view_database_list_degree" locale="pt">
        <value>Pode visualizar as listas de dados de base sobre o grau</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="education">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt">
            <value>Adicionar/Actualizar Histórico da Educação</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="degree">
          <configuration name="display" values="single" locale="pt">
            <value>Grau</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="edu_type">
          <configuration name="display" values="single" locale="pt">
            <value>Tipode de Educação</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="education">
          <configuration name="display" values="single" locale="pt">
            <value>Histórico Educação</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Degree">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Nome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="edu_type">
              <configuration name="headers" type="delimited" values="many" locale="pt">
                <value>default:Tipode de Educação</value>
                <value>select_degree:Seleccionar Tipo de Educação e o Grau</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Education">
          <configurationGroup name="fields">
            <configurationGroup name="degree">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Grau</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="institution">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Nome da Instituição</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Localização da Instituição</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Ano de Graduação (deixar em branco se estiver em curso)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="major">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Especialidade</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>