<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonEducation" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonEducation" path="/I2CE" locale="tl">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="tl">
      <configuration name="person_can_view_child_form_education" locale="tl">
        <value>Matitingnan ang pormularyo sa edukasyon ng bata ng isang tao</value>
      </configuration>
      <configuration name="person_can_edit_child_form_education" locale="tl">
        <value>Mababago ang mga pormularyo ng edukasyon ng bata ng isang tao</value>
      </configuration>
      <configuration name="can_edit_database_list_edu_type" locale="tl">
        <value>Mababago ang mga tala ng kalipunan na para sa uri ng edukasyon</value>
      </configuration>
      <configuration name="can_view_database_list_edu_type" locale="tl">
        <value>Makikita ang mga tala ng kalipunan ng dato para sa uri ng edukasyon</value>
      </configuration>
      <configuration name="can_edit_database_list_degree" locale="tl">
        <value>Mababago ang mga tala ng kalipunan ng dato para sa uri ng degri</value>
      </configuration>
      <configuration name="can_view_database_list_degree" locale="tl">
        <value>Makikita ang mga tala ng kalipunan ng dato para sa uri ng degri</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="education">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="tl">
            <value>Magdagdag/Magsapanahon ng Kasaysayan ng Edukasyon</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="degree">
          <configuration name="display" values="single" locale="tl">
            <value>Antas</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="edu_type">
          <configuration name="display" values="single" locale="tl">
            <value>Uri ng Edukasyon</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="education">
          <configuration name="display" values="single" locale="tl">
            <value>Kasaysayan ng Edukasyon</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Degree">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pangalan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="edu_type">
              <configuration name="headers" type="delimited" values="many" locale="tl">
                <value>default:Uri ng Edukasyon</value>
                <value>select_degree:Pumili ng Uri ng Edukasyon at pagkaraan ay Degri</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Education">
          <configurationGroup name="fields">
            <configurationGroup name="degree">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Antas</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="institution">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pangalan ng Institusyon</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Lokasyon ng Institusyon</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Taon ng Pagtatapos (iwanang walang laman kung Isinasagawa)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="major">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pangunahing Larangan sa Kurso</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>