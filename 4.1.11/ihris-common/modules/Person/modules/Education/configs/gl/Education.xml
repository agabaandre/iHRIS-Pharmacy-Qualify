<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonEducation" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonEducation" path="/I2CE" locale="gl">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="gl">
      <configuration name="person_can_view_child_form_education" locale="gl">
        <value>Pode ollar a ficha de educación dunha persoa</value>
      </configuration>
      <configuration name="person_can_edit_child_form_education" locale="gl">
        <value>Pode editar as fichas de educación dunha persoa</value>
      </configuration>
      <configuration name="can_edit_database_list_edu_type" locale="gl">
        <value>Pode editar as listas da base de datos sobre os tipos de educación</value>
      </configuration>
      <configuration name="can_view_database_list_edu_type" locale="gl">
        <value>Pode ollar as listas da base de datos sobre os tipos de educación</value>
      </configuration>
      <configuration name="can_edit_database_list_degree" locale="gl">
        <value>Pode editar as listas da base de datos sobre os tipos de graos</value>
      </configuration>
      <configuration name="can_view_database_list_degree" locale="gl">
        <value>Pode ollar as listas da base de datos sobre os tipos de graos</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="education">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="gl">
            <value>Engadir/Actualizar o historial educativo</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="degree">
          <configuration name="display" values="single" locale="gl">
            <value>Grao</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="edu_type">
          <configuration name="display" values="single" locale="gl">
            <value>Tipo de educación</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="education">
          <configuration name="display" values="single" locale="gl">
            <value>Historial educativo</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Degree">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Nome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="edu_type">
              <configuration name="headers" type="delimited" values="many" locale="gl">
                <value>default:Tipo de educación</value>
                <value>select_degree:Seleccione o tipo de educación; logo, o grao</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Education">
          <configurationGroup name="fields">
            <configurationGroup name="degree">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Grao</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="institution">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Nome da institución</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Localización da institución</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Ano de graduación (déixeo en branco se aínda non se gradúa)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="major">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Especialidade</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>