<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonEducation" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonEducation" path="/I2CE" locale="lki">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="lki">
      <configuration name="person_can_view_child_form_education" locale="lki"></configuration>
      <configuration name="person_can_edit_child_form_education" locale="lki"></configuration>
      <configuration name="can_edit_database_list_edu_type" locale="lki"></configuration>
      <configuration name="can_view_database_list_edu_type" locale="lki">
        <value>قادر به مشاهدهٔ فهرست‌های پایگاه دادهٔ نوع تحصیل</value>
      </configuration>
      <configuration name="can_edit_database_list_degree" locale="lki">
        <value>قادر به ویرایش فهرست‌های پایگاه دادهٔ نوع درجه</value>
      </configuration>
      <configuration name="can_view_database_list_degree" locale="lki">
        <value>قادر به مشاهدهٔ فهرست‌های پایگاه دادهٔ نوع درجه</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="education">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="lki">
            <value>افزودن/به‌روزرسانی تاریخچهٔ تحصیلات</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="degree">
          <configuration name="display" values="single" locale="lki">
            <value>درجۀ</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="edu_type">
          <configuration name="display" values="single" locale="lki">
            <value>نوع تحصیل</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="education">
          <configuration name="display" values="single" locale="lki">
            <value>تاریخچهٔ تحصیلات</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Degree">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="lki">
                <value>default:نۆم</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="edu_type">
              <configuration name="headers" type="delimited" values="many" locale="lki">
                <value>default:نوع تحصیل</value>
                <value>select_degree:انتخاب نوع تحصیلات و سپس درجه</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Education">
          <configurationGroup name="fields">
            <configurationGroup name="degree">
              <configuration name="headers" type="delimited" locale="lki">
                <value>default:درجۀ</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="institution">
              <configuration name="headers" type="delimited" locale="lki"></configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="lki">
                <value>default:محل مؤسسه</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="lki">
                <value>default:تاریخ فارغ التحصیلی (اگر در حال انجام است، خالی بگذارید)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="major">
              <configuration name="headers" type="delimited" locale="lki">
                <value>default:گرایش</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>