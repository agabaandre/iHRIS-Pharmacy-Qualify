<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonID" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonID" path="/I2CE" locale="gl">
    <version>4.2.1.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="gl">
      <configuration name="exp_date" path="person_id/expiration_date/bad_date" locale="gl">
        <value>A data de caducidade debe ser posterior á data de emisión.</value>
      </configuration>
      <configuration name="wrong_pattern" path="person_id/id_num/wrong_pattern" locale="gl"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="gl">
      <configuration name="person_can_view_child_form_person_id" locale="gl">
        <value>Pode ollar a ficha person_id dunha persoa</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_id" locale="gl">
        <value>Pode editar a ficha person_id dunha persoa</value>
      </configuration>
      <configuration name="can_edit_database_list_id_type" locale="gl">
        <value>Pode editar a lista da base de datos dos id_type</value>
      </configuration>
      <configuration name="can_view_database_list_id_type" locale="gl">
        <value>Pode ollar a lista da base de datos dos id_type</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_id">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="gl">
            <value>Engadir/Actualizar a identificación</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/forms">
      <configurationGroup name="id_type">
        <configuration name="display" values="single" locale="gl">
          <value>Tipo de identificación</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="person_id">
        <configuration name="display" values="single" locale="gl">
          <value>Identificación persoal</value>
        </configuration>
        <configurationGroup name="meta">
          <configuration name="description" locale="gl">
            <value>Este formulario contén información básica sobre a identificación dunha persoa</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/formClasses">
      <configurationGroup name="iHRIS_PersonID">
        <configurationGroup name="fields">
          <configurationGroup name="id_type">
            <configuration name="headers" type="delimited" locale="gl">
              <value>default:Tipo de identificación</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="id_num">
            <configuration name="headers" type="delimited" locale="gl">
              <value>default:Número de identificación</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="issue_date">
            <configuration name="headers" type="delimited" locale="gl">
              <value>default:Data de emisión</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="expiration_date">
            <configuration name="headers" type="delimited" locale="gl">
              <value>default:Data de caducidade</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="country">
            <configuration name="headers" type="delimited" locale="gl">
              <value>default:País de emisión</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="place">
            <configuration name="headers" type="delimited" locale="gl">
              <value>default:Lugar de emisión</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="iHRIS_PersonID_Type">
        <configurationGroup name="fields">
          <configurationGroup name="pattern">
            <configuration name="headers" type="delimited" locale="gl"></configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>