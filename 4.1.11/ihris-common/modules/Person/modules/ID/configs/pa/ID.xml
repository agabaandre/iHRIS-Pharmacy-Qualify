<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonID" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonID" path="/I2CE" locale="pa">
    <version>4.2.1.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="pa">
      <configuration name="exp_date" path="person_id/expiration_date/bad_date" locale="pa"></configuration>
      <configuration name="wrong_pattern" path="person_id/id_num/wrong_pattern" locale="pa"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pa">
      <configuration name="person_can_view_child_form_person_id" locale="pa"></configuration>
      <configuration name="person_can_edit_child_form_person_id" locale="pa"></configuration>
      <configuration name="can_edit_database_list_id_type" locale="pa"></configuration>
      <configuration name="can_view_database_list_id_type" locale="pa"></configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_id">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pa">
            <value>ਸ਼ਨਾਖ਼ਤ ਜੋੜੋ/ਨਵਿਆਉ</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/forms">
      <configurationGroup name="id_type">
        <configuration name="display" values="single" locale="pa">
          <value>ਸ਼ਨਾਖ਼ਤ ਦੀ ਕਿਸਮ</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="person_id">
        <configuration name="display" values="single" locale="pa">
          <value>ਨਿੱਜੀ ਸ਼ਨਾਖ਼ਤ</value>
        </configuration>
        <configurationGroup name="meta">
          <configuration name="description" locale="pa"></configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/formClasses">
      <configurationGroup name="iHRIS_PersonID">
        <configurationGroup name="fields">
          <configurationGroup name="id_type">
            <configuration name="headers" type="delimited" locale="pa">
              <value>default:ਸ਼ਨਾਖ਼ਤ ਦੀ ਕਿਸਮ</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="id_num">
            <configuration name="headers" type="delimited" locale="pa">
              <value>default:ਸ਼ਨਾਖ਼ਤੀ ਨੰਬਰ</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="issue_date">
            <configuration name="headers" type="delimited" locale="pa">
              <value>default:ਕੱਢਣ ਦੀ ਮਿਤੀ</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="expiration_date">
            <configuration name="headers" type="delimited" locale="pa">
              <value>default:ਮਿਆਦ ਮੁੱਕਣ ਦੀ ਮਿਤੀ</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="country">
            <configuration name="headers" type="delimited" locale="pa">
              <value>default:ਕੱਢਣ ਵਾਲ਼ਾ ਮੁਲਕ</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="place">
            <configuration name="headers" type="delimited" locale="pa">
              <value>default:ਕੱਢਣ ਦਾ ਟਿਕਾਣਾ</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="iHRIS_PersonID_Type">
        <configurationGroup name="fields">
          <configurationGroup name="pattern">
            <configuration name="headers" type="delimited" locale="pa"></configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>