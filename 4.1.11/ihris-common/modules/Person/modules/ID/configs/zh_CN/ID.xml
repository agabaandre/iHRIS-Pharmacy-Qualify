<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonID" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonID" path="/I2CE" locale="zh_CN">
    <version>4.2.1.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="zh_CN">
      <configuration name="exp_date" path="person_id/expiration_date/bad_date" locale="zh_CN">
        <value>届满日期必须在发行日期之后。</value>
      </configuration>
      <configuration name="wrong_pattern" path="person_id/id_num/wrong_pattern" locale="zh_CN"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="zh_CN">
      <configuration name="person_can_view_child_form_person_id" locale="zh_CN">
        <value>可以查看该人的个人ID的子表单</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_id" locale="zh_CN">
        <value>可以编辑该人的个人ID的子表单</value>
      </configuration>
      <configuration name="can_edit_database_list_id_type" locale="zh_CN">
        <value>可以编辑效益类型数据库列表</value>
      </configuration>
      <configuration name="can_view_database_list_id_type" locale="zh_CN">
        <value>可以查看效益类型数据库列表</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_id">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="zh_CN">
            <value>添加/更新识别</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/forms">
      <configurationGroup name="id_type">
        <configuration name="display" values="single" locale="zh_CN">
          <value>识别类型</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="person_id">
        <configuration name="display" values="single" locale="zh_CN">
          <value>人的识别</value>
        </configuration>
        <configurationGroup name="meta">
          <configuration name="description" locale="zh_CN">
            <value>这种形式持有的鉴定人的基本信息</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/formClasses">
      <configurationGroup name="iHRIS_PersonID">
        <configurationGroup name="fields">
          <configurationGroup name="id_type">
            <configuration name="headers" type="delimited" locale="zh_CN">
              <value>default:识别类型</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="id_num">
            <configuration name="headers" type="delimited" locale="zh_CN">
              <value>default:识别号码</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="issue_date">
            <configuration name="headers" type="delimited" locale="zh_CN">
              <value>default:发行日期</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="expiration_date">
            <configuration name="headers" type="delimited" locale="zh_CN">
              <value>default:届满日期</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="country">
            <configuration name="headers" type="delimited" locale="zh_CN">
              <value>default:签发国家</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="place">
            <configuration name="headers" type="delimited" locale="zh_CN">
              <value>default:签发地点</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="iHRIS_PersonID_Type">
        <configurationGroup name="fields">
          <configurationGroup name="pattern">
            <configuration name="headers" type="delimited" locale="zh_CN"></configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>