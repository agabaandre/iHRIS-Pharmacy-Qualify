<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonID" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonID" path="/I2CE" locale="uk">
    <version>4.2.1.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="uk">
      <configuration name="exp_date" path="person_id/expiration_date/bad_date" locale="uk">
        <value>Дата закінчення терміну дії повинна бути після дати видачі.</value>
      </configuration>
      <configuration name="wrong_pattern" path="person_id/id_num/wrong_pattern" locale="uk"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="uk">
      <configuration name="person_can_view_child_form_person_id" locale="uk">
        <value>Можна переглянути дочірню форму особи про ідентифікатор</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_id" locale="uk">
        <value>Можна редагувати дочірню форму особи про ідентифікатор</value>
      </configuration>
      <configuration name="can_edit_database_list_id_type" locale="uk">
        <value>Можна редагувати список типів ідентифікаторів бази даних</value>
      </configuration>
      <configuration name="can_view_database_list_id_type" locale="uk">
        <value>Можна переглянути список типів ідентифікаторів бази даних</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_id">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="uk">
            <value>Додати/оновити ідентифікацію</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/forms">
      <configurationGroup name="id_type">
        <configuration name="display" values="single" locale="uk">
          <value>Тип ідентифікації</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="person_id">
        <configuration name="display" values="single" locale="uk">
          <value>Ідентифікації особи</value>
        </configuration>
        <configurationGroup name="meta">
          <configuration name="description" locale="uk">
            <value>Ця форма містить основні відомості про ідентифікацію людини</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/formClasses">
      <configurationGroup name="iHRIS_PersonID">
        <configurationGroup name="fields">
          <configurationGroup name="id_type">
            <configuration name="headers" type="delimited" locale="uk">
              <value>default:Тип ідентифікації</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="id_num">
            <configuration name="headers" type="delimited" locale="uk">
              <value>default:Ідентифікаційний номер</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="issue_date">
            <configuration name="headers" type="delimited" locale="uk">
              <value>default:Дата видачі</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="expiration_date">
            <configuration name="headers" type="delimited" locale="uk">
              <value>default:Дата закінчення</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="country">
            <configuration name="headers" type="delimited" locale="uk">
              <value>default:Країна видачі</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="place">
            <configuration name="headers" type="delimited" locale="uk">
              <value>default:Місце видачі</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="iHRIS_PersonID_Type">
        <configurationGroup name="fields">
          <configurationGroup name="pattern">
            <configuration name="headers" type="delimited" locale="uk"></configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>