<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonID" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonID" path="/I2CE" locale="sw">
    <version>4.2.1.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="sw">
      <configuration name="exp_date" path="person_id/expiration_date/bad_date" locale="sw"></configuration>
      <configuration name="wrong_pattern" path="person_id/id_num/wrong_pattern" locale="sw"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="sw">
      <configuration name="person_can_view_child_form_person_id" locale="sw">
        <value>Weza kuangalia mtu_fomu ya utambulisho ya mtoto wa mtu</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_id" locale="sw">
        <value>Weza kuhariri mtu_fomu za utambulisho za mtoto wa mtu</value>
      </configuration>
      <configuration name="can_edit_database_list_id_type" locale="sw">
        <value>Weza kuhariri orodha ya hazinadata ya aina ya _utambulisho</value>
      </configuration>
      <configuration name="can_view_database_list_id_type" locale="sw">
        <value>Weza kuangalia orodha ya hazinadata ya aina ya _utambulisho</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_id">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="sw">
            <value>Ongeza/Sasisha Utambulisho</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/forms">
      <configurationGroup name="id_type">
        <configuration name="display" values="single" locale="sw">
          <value>Aina ya Utambulisho</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="person_id">
        <configuration name="display" values="single" locale="sw">
          <value>Utambulisho wa Mtu</value>
        </configuration>
        <configurationGroup name="meta">
          <configuration name="description" locale="sw">
            <value>Fomu hii ina taarifa ya msingi kuhusu utambulisho kwa ajili ya mtu</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/formClasses">
      <configurationGroup name="iHRIS_PersonID">
        <configurationGroup name="fields">
          <configurationGroup name="id_type">
            <configuration name="headers" type="delimited" locale="sw">
              <value>default:Aina ya Utambulisho</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="id_num">
            <configuration name="headers" type="delimited" locale="sw">
              <value>default:Namba ya Utambulisho</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="issue_date">
            <configuration name="headers" type="delimited" locale="sw"></configuration>
          </configurationGroup>
          <configurationGroup name="expiration_date">
            <configuration name="headers" type="delimited" locale="sw"></configuration>
          </configurationGroup>
          <configurationGroup name="country">
            <configuration name="headers" type="delimited" locale="sw"></configuration>
          </configurationGroup>
          <configurationGroup name="place">
            <configuration name="headers" type="delimited" locale="sw"></configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="iHRIS_PersonID_Type">
        <configurationGroup name="fields">
          <configurationGroup name="pattern">
            <configuration name="headers" type="delimited" locale="sw"></configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>