<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonID" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonID" path="/I2CE" locale="tl">
    <version>4.2.1.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="tl">
      <configuration name="exp_date" path="person_id/expiration_date/bad_date" locale="tl">
        <value>Ang petsa ng pagkawalang bisa ay dapat na maging pagkalipas ng petsa ng pagkakaloob.</value>
      </configuration>
      <configuration name="wrong_pattern" path="person_id/id_num/wrong_pattern" locale="tl"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="tl">
      <configuration name="person_can_view_child_form_person_id" locale="tl">
        <value>Matitingnan ang ID_ng_tao ng pormularyong pambata ng isang tao</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_id" locale="tl">
        <value>Mababago ang ID_ng_tao ng mga pormularyong pambata ng isang tao</value>
      </configuration>
      <configuration name="can_edit_database_list_id_type" locale="tl">
        <value>Mababago ang tala ng uri_ng_ID ng kalipunan ng dato</value>
      </configuration>
      <configuration name="can_view_database_list_id_type" locale="tl">
        <value>Matitingnan ang tala ng uri_ng_ID ng kalipunan ng dato</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_id">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="tl">
            <value>Magdagdag/Magsapanahon ng Pagkakakilanlan</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/forms">
      <configurationGroup name="id_type">
        <configuration name="display" values="single" locale="tl">
          <value>Uri ng Pagkakakilanlan</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="person_id">
        <configuration name="display" values="single" locale="tl">
          <value>Pagkakakilanlan ng Tao</value>
        </configuration>
        <configurationGroup name="meta">
          <configuration name="description" locale="tl">
            <value>Ang pormularyong ito ay humahawak ng saligang kabatiran tungkol sa pagkakakilanlan ng isang tao</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/formClasses">
      <configurationGroup name="iHRIS_PersonID">
        <configurationGroup name="fields">
          <configurationGroup name="id_type">
            <configuration name="headers" type="delimited" locale="tl">
              <value>default:Uri ng Pagkakakilanlan</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="id_num">
            <configuration name="headers" type="delimited" locale="tl">
              <value>default:Bilang ng Pagkakakilanlan</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="issue_date">
            <configuration name="headers" type="delimited" locale="tl">
              <value>default:Petsa ng Pagkakaloob</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="expiration_date">
            <configuration name="headers" type="delimited" locale="tl">
              <value>default:Petsa ng Pagkawalang Bisa</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="country">
            <configuration name="headers" type="delimited" locale="tl">
              <value>default:Bansa ng Pagkakaloob</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="place">
            <configuration name="headers" type="delimited" locale="tl">
              <value>default:Pook ng Pagkakaloob</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="iHRIS_PersonID_Type">
        <configurationGroup name="fields">
          <configurationGroup name="pattern">
            <configuration name="headers" type="delimited" locale="tl"></configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>