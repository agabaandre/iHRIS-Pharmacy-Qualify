<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonEmployment" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonEmployment" path="/I2CE" locale="pt">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pt">
      <configuration name="person_can_view_child_form_employment" locale="pt">
        <value>Pode visualizar o child form de Contacto de Emprego de uma pessoa</value>
      </configuration>
      <configuration name="person_can_edit_child_form_employment" locale="pt">
        <value>Pode editar o child form de Contacto de Emprego de uma pessoa</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="employment">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt">
            <value>Adicionar/Actualizar Informação de Emprego</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="employment">
          <configuration name="display" values="single" locale="pt">
            <value>Historial de Emprego</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Employment">
          <configurationGroup name="fields">
            <configurationGroup name="company_name">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Nome da Empresa</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="company_address">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Endereço da Empresa</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="company_phone">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Telefone da Empresa</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Data de Início</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_wage">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Salário Inicial</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_position">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Cargo Inicial</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Data de Cessação (deixar em branco se ainda estiver a trabalhar na empresa)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_wage">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Salário à Saída</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_position">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Cargo à Saída</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="supervisor">
              <configuration name="headers" type="delimited" locale="pt"></configuration>
            </configurationGroup>
            <configurationGroup name="contact_ok">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Podemos Contacta-lo?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="responsibilities">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Responsabilidades</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="reason_for_leaving">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Motivo de Saída</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>