<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonEmployment" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonEmployment" path="/I2CE" locale="tl">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="tl">
      <configuration name="person_can_view_child_form_employment" locale="tl">
        <value>Matitingnan ang pormularyo ng hanapbuhay ng bata ng isang tao</value>
      </configuration>
      <configuration name="person_can_edit_child_form_employment" locale="tl">
        <value>Mababago ang mga pormularyo sa paghahanapbuhay ng bata ng isang tao</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="employment">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="tl">
            <value>Magdagdag/Magsapanahon ng Kasaysayan ng Paghahanapbuhay</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="employment">
          <configuration name="display" values="single" locale="tl">
            <value>Kasaysayan ng Paghahanapbuhay</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Employment">
          <configurationGroup name="fields">
            <configurationGroup name="company_name">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pangalan ng Kumpanya</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="company_address">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Tirahan ng Kumpanya</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="company_phone">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Telepono ng Kumpanya</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Petsa ng Pagsisimula</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_wage">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pansimulang Pasahod</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_position">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pansimulang Posisyon</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Petsa ng Pagwawakas (iwanang walang laman kung naghahanapbuhay pa)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_wage">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Panghuling Sahod</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_position">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Panghuling Posisyon</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="supervisor">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Superbisor</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="contact_ok">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Maaari bang Tawagan?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="responsibilities">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Mga Pananagutan sa Hanapbuhay</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="reason_for_leaving">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Dahilan ng Paglisan</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>