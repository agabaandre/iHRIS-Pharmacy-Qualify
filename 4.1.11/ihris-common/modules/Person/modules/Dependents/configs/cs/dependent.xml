<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="dependents" path="/">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="dependents" path="/" locale="cs">
    <version>4.3.0.1</version>
    <configurationGroup name="dep_class" path="/modules/forms/formClasses/iHRIS_Dependent">
      <configurationGroup name="fields">
        <configurationGroup name="gender">
          <configuration name="headers" type="delimited" values="many" locale="cs">
            <value>default:Pohlaví</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="date_of_birth">
          <configuration name="headers" type="delimited" values="many" locale="cs">
            <value>default:Datum narození</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="name">
          <configuration name="headers" type="delimited" values="many" locale="cs">
            <value>default:Jméno</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="dep_form" path="/modules/forms/forms/dependent">
      <configuration name="display" locale="cs">
        <value>Zavislý</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="cs">
      <configuration name="person_can_view_child_form_dependent" locale="cs">
        <value>Může zobrazit závislé osoby</value>
      </configuration>
      <configuration name="person_can_edit_child_form_dependent" locale="cs">
        <value>Může upravit závislé osoby</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="dependent_page" path="/I2CE/page/dependent">
      <configurationGroup name="args">
        <configuration name="title" values="single" locale="cs">
          <value>Přidat/Aktualizovat informace o závislých</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>