<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="dependents" path="/">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="dependents" path="/" locale="ar">
    <version>4.3.0.1</version>
    <configurationGroup name="dep_class" path="/modules/forms/formClasses/iHRIS_Dependent">
      <configurationGroup name="fields">
        <configurationGroup name="gender">
          <configuration name="headers" type="delimited" values="many" locale="ar">
            <value>default:الجنس</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="date_of_birth">
          <configuration name="headers" type="delimited" values="many" locale="ar">
            <value>default:تاريخ&#160;الميلاد</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="name">
          <configuration name="headers" type="delimited" values="many" locale="ar">
            <value>default:اسم</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="dep_form" path="/modules/forms/forms/dependent">
      <configuration name="display" locale="ar">
        <value>تابع</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="ar">
      <configuration name="person_can_view_child_form_dependent" locale="ar">
        <value>يمكن عرض&#160;أقرباء شخص</value>
      </configuration>
      <configuration name="person_can_edit_child_form_dependent" locale="ar">
        <value>يمكن تعديل&#160;أقرباء شخص</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="dependent_page" path="/I2CE/page/dependent">
      <configurationGroup name="args">
        <configuration name="title" values="single" locale="ar">
          <value>إضافة / تحديث&#160;بيانات تابع</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>