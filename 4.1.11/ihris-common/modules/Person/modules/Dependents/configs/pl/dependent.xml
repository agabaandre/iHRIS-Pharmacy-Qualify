<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="dependents" path="/">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="dependents" path="/" locale="pl">
    <version>4.2.0.1</version>
    <configurationGroup name="dep_class" path="/modules/forms/formClasses/iHRIS_Dependent">
      <configurationGroup name="fields">
        <configurationGroup name="gender">
          <configuration name="headers" type="delimited" values="many" locale="pl">
            <value>default:Płeć</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="date_of_birth">
          <configuration name="headers" type="delimited" values="many" locale="pl">
            <value>default:Data urodzenia</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="name">
          <configuration name="headers" type="delimited" values="many" locale="pl">
            <value>default:Nazwa</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="dep_form" path="/modules/forms/forms/dependent">
      <configuration name="display" locale="pl">
        <value>Osoby na utrzymaniu</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pl">
      <configuration name="person_can_view_child_form_dependent" locale="pl">
        <value>Może przeglądać osoby na utrzymaniu</value>
      </configuration>
      <configuration name="person_can_edit_child_form_dependent" locale="pl">
        <value>Może zmieniać osoby na utrzymaniu</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="dependent_page" path="/I2CE/page/dependent">
      <configurationGroup name="args">
        <configuration name="title" values="single" locale="pl">
          <value>Dodaj/zmień informacje o osobach na utrzymaniu</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>