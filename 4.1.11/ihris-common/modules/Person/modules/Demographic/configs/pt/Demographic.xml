<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonDemographic" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonDemographic" path="/I2CE" locale="pt">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pt">
      <configuration name="person_can_view_child_form_demographic" locale="pt">
        <value>Pode visualizar os child forms demográficos de uma pessoa</value>
      </configuration>
      <configuration name="person_can_edit_child_form_demographic" locale="pt">
        <value>Pode editar os child forms demográficos de uma pessoa</value>
      </configuration>
      <configuration name="can_edit_database_list_marital_status" locale="pt">
        <value>Pode editar a lista de informação do estado civil do departamento</value>
      </configuration>
      <configuration name="can_view_database_list_marital_status" locale="pt">
        <value>Pode visualizar a lista de informação do estado civil do departamento</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="gender">
        <configurationGroup name="F">
          <configuration name="fields" value="many" type="delimited" locale="pt">
            <value>name:Feminino</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="M">
          <configuration name="fields" value="many" type="delimited" locale="pt">
            <value>name:Masculino</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="demographic">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt">
            <value>Adicionar/Actualizar Informação</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="demographic">
          <configuration name="display" values="single" locale="pt">
            <value>Informação Demográfica</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="marital_status">
          <configuration name="display" values="single" locale="pt">
            <value>Estado Civil</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="gender">
          <configuration name="display" values="single" locale="pt">
            <value>Género</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Demographic">
          <configurationGroup name="fields">
            <configurationGroup name="birth_date">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Data de Nascimento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="gender">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Género</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="marital_status">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Estado Civil</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>