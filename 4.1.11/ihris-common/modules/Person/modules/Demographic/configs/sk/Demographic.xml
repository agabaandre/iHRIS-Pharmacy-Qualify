<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonDemographic" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonDemographic" path="/I2CE" locale="sk">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="sk">
      <configuration name="person_can_view_child_form_demographic" locale="sk">
        <value>Možno zobraziť demografický osobný formulár dieťaťa</value>
      </configuration>
      <configuration name="person_can_edit_child_form_demographic" locale="sk">
        <value>Možno upraviť demografický osobný formulár dieťaťa</value>
      </configuration>
      <configuration name="can_edit_database_list_marital_status" locale="sk">
        <value>Možno upraviť zoznam časti rodinný stav</value>
      </configuration>
      <configuration name="can_view_database_list_marital_status" locale="sk">
        <value>Možno zobraziť zoznam časti rodinný stav</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="gender">
        <configurationGroup name="F">
          <configuration name="fields" value="many" type="delimited" locale="sk">
            <value>name:ženské</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="M">
          <configuration name="fields" value="many" type="delimited" locale="sk">
            <value>name:mužské</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="demographic">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="sk">
            <value>Pridať/upraviť demografický údaj</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="demographic">
          <configuration name="display" values="single" locale="sk">
            <value>Demografický údaj</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="marital_status">
          <configuration name="display" values="single" locale="sk">
            <value>rodinný stav</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="gender">
          <configuration name="display" values="single" locale="sk">
            <value>Pohlavie</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Demographic">
          <configurationGroup name="fields">
            <configurationGroup name="birth_date">
              <configuration name="headers" type="delimited" locale="sk">
                <value>default:Dátum narodenia</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="gender">
              <configuration name="headers" type="delimited" locale="sk">
                <value>default:Pohlavie</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="marital_status">
              <configuration name="headers" type="delimited" locale="sk">
                <value>default:rodinný stav</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>