<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonDemographic" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonDemographic" path="/I2CE" locale="tl">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="tl">
      <configuration name="person_can_view_child_form_demographic" locale="tl">
        <value>Matitingnan ang pormularyong pangdemograpiya ng bata ng isang tao</value>
      </configuration>
      <configuration name="person_can_edit_child_form_demographic" locale="tl">
        <value>Mababago ang mga pormularyong pangdemograpiya ng bata ng isang tao</value>
      </configuration>
      <configuration name="can_edit_database_list_marital_status" locale="tl">
        <value>Mababago ang tala ng kagawaran sa katayuan ng pag-aasawa.</value>
      </configuration>
      <configuration name="can_view_database_list_marital_status" locale="tl">
        <value>Matitingnan ang tala ng katayuan sa pag-aasawa ng kagawaran.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="gender">
        <configurationGroup name="F">
          <configuration name="fields" value="many" type="delimited" locale="tl">
            <value>name:Babae</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="M">
          <configuration name="fields" value="many" type="delimited" locale="tl">
            <value>name:Lalaki</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="demographic">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="tl">
            <value>Magdagdag/Magsapanahon ng Kabatirang Demograpiko</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="demographic">
          <configuration name="display" values="single" locale="tl">
            <value>Kabatirang Demograpiko</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="marital_status">
          <configuration name="display" values="single" locale="tl">
            <value>Katayuan sa Pag-aasawa</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="gender">
          <configuration name="display" values="single" locale="tl">
            <value>Kasarian</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Demographic">
          <configurationGroup name="fields">
            <configurationGroup name="birth_date">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Petsa ng Kapanganakan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="gender">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Kasarian</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="marital_status">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Katayuan sa Pag-aasawa</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>