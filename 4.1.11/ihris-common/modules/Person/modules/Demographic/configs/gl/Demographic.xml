<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonDemographic" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonDemographic" path="/I2CE" locale="gl">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="gl">
      <configuration name="person_can_view_child_form_demographic" locale="gl">
        <value>Pode ollar a ficha demográfica dunha persoa</value>
      </configuration>
      <configuration name="person_can_edit_child_form_demographic" locale="gl">
        <value>Pode editar as fichas demográficas dunha persoa</value>
      </configuration>
      <configuration name="can_edit_database_list_marital_status" locale="gl">
        <value>Pode editar a lista de estados civís do departamento.</value>
      </configuration>
      <configuration name="can_view_database_list_marital_status" locale="gl">
        <value>Pode ollar a lista de estados civís do departamento.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="gender">
        <configurationGroup name="F">
          <configuration name="fields" value="many" type="delimited" locale="gl">
            <value>name:Muller</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="M">
          <configuration name="fields" value="many" type="delimited" locale="gl">
            <value>name:Home</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="demographic">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="gl">
            <value>Engadir/Actualizar a información demográfica</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="demographic">
          <configuration name="display" values="single" locale="gl">
            <value>Información demográfica</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="marital_status">
          <configuration name="display" values="single" locale="gl">
            <value>Estado civil</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="gender">
          <configuration name="display" values="single" locale="gl">
            <value>Sexo</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Demographic">
          <configurationGroup name="fields">
            <configurationGroup name="birth_date">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Data de nacemento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="gender">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Sexo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="marital_status">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Estado civil</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>