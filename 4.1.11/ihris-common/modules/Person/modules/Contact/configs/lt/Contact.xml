<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonContact" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonContact" path="/I2CE" locale="lt">
    <version>4.2.0.1</version>
    <configurationGroup name="invalid_messages" path="/modules/forms/formfield_messages" locale="lt">
      <configuration name="work" path="work/email/invalid_email"></configuration>
      <configuration name="personal" path="personal/email/invalid_email"></configuration>
      <configuration name="other" path="other/email/invalid_email"></configuration>
      <configuration name="emergency" path="emergency/email/invalid_email"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="lt">
      <configuration name="person_can_view_child_form_person_contact_work" locale="lt"></configuration>
      <configuration name="person_can_edit_child_form_person_contact_work" locale="lt"></configuration>
      <configuration name="person_can_view_child_form_person_contact_emergency" locale="lt"></configuration>
      <configuration name="person_can_edit_child_form_person_contact_emergency" locale="lt"></configuration>
      <configuration name="person_can_view_child_form_person_contact_personal" locale="lt"></configuration>
      <configuration name="person_can_edit_child_form_person_contact_personal" locale="lt"></configuration>
      <configuration name="person_can_view_child_form_person_contact_other" locale="lt"></configuration>
      <configuration name="person_can_edit_child_form_person_contact_other" locale="lt"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="person_contact_work">
          <configuration name="display" values="single" locale="lt">
            <value>Darbo kontaktai</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_contact_emergency">
          <configuration name="display" values="single" locale="lt">
            <value>Kontaktai nelaimės atveju</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_contact_other">
          <configuration name="display" values="single" locale="lt">
            <value>Kiti Kontaktai</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_contact_personal">
          <configuration name="display" values="single" locale="lt">
            <value>Asmeninis Kontaktas</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="contact">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="lt">
            <value>Pridėti/atnaujinti kontaktinę informaciją</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>