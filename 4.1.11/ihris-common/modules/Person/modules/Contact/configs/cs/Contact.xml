        </configurationGroup>
        <configurationGroup name="person_contact_personal">
          <configuration name="display" values="single" locale="cs">
            <value>Osobní kontakt</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="contact">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="cs">
            <value>Přidat/Aktualizovat informace o kontaktu</value>
