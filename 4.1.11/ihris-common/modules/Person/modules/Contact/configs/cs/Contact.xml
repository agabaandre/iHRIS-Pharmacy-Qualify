<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonContact" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
