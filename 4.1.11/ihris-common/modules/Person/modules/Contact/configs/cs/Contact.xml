          <configuration name="display" values="single" locale="cs">
            <value>Kontakt v práci</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_contact_emergency">
          <configuration name="display" values="single" locale="cs">
            <value>Kontakt pro případ nutnosti</value>
