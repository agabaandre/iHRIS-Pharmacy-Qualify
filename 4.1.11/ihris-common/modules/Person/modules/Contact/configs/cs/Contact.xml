      <configuration name="personal" path="personal/email/invalid_email">
        <value>Zadejte platnou e-mailovou adresu</value>
      </configuration>
      <configuration name="other" path="other/email/invalid_email">
        <value>Zadejte platnou e-mailovou adresu</value>
      </configuration>
      <configuration name="emergency" path="emergency/email/invalid_email">
        <value>Zadejte platnou e-mailovou adresu</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="cs">
      <configuration name="person_can_view_child_form_person_contact_work" locale="cs">
        <value>Může zobrazit podřazený formulář kontaktu v práci osoby</value>
