        <value>Může upravit podřazený formulář osobního kontaktu osoby</value>
      </configuration>
      <configuration name="person_can_view_child_form_person_contact_other" locale="cs">
        <value>Může zobrazit podřazený formulář jiného kontaktu osoby</value>
