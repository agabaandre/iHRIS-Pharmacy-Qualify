<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="SelfService" path="/">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="SelfService" path="/" locale="fr">
    <version>4.2.0.1.1</version>
    <configurationGroup name="SS" path="/modules/SelfService">
      <configurationGroup name="page">
        <configurationGroup name="view">
          <configurationGroup name="args">
            <configuration name="title" locale="fr">
              <value>Afficher</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="person_can_view_child_form_user_map" locale="fr">
        <value>Peut afficher les formulaires de la carte d'utilisateur d'une personne</value>
      </configuration>
      <configuration name="person_can_edit_child_form_user_map" locale="fr">
        <value>Peut editer les formulaires de la carte d'utilisateur d'une personne</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="user_map">
          <configuration name="display" values="single" locale="fr">
            <value>Carte d'utilisteur</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_UserMap">
          <configurationGroup name="fields">
            <configurationGroup name="username">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Nom d'utilisateur</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page" path="/modules/SelfService/page">
      <configurationGroup name="userAccount">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Ajouter/mettre a jour le compte d'utilisateur</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>