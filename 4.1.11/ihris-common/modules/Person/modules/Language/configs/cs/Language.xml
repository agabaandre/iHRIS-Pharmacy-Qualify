<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonLanguage" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonLanguage" path="/I2CE" locale="cs">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="cs">
      <configuration name="person_can_view_child_form_person_language" locale="cs">
        <value>Může zobrazit podřazený formulář person_language osoby</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_language" locale="cs">
        <value>Můžeupravit podřazený formulář person_language osoby</value>
      </configuration>
      <configuration name="can_edit_database_list_language" locale="cs">
        <value>Může upravit databázový seznam jazyků</value>
      </configuration>
      <configuration name="can_view_database_list_language" locale="cs">
        <value>Může zobrazit databázový seznam jazyků</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="language_proficiency">
        <configurationGroup name="elementary">
          <configuration name="fields" value="many" type="delimited" locale="cs">
            <value>name:Základní</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="limited">
          <configuration name="fields" value="many" type="delimited" locale="cs">
            <value>name:Omezené na práci</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="professional">
          <configuration name="fields" value="many" type="delimited" locale="cs">
            <value>name:Pracující profesionál</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="full_professional">
          <configuration name="fields" value="many" type="delimited" locale="cs">
            <value>name:Plně profesionální</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="fluent">
          <configuration name="fields" value="many" type="delimited" locale="cs">
            <value>name:Plynně</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_language">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="cs">
            <value>Přidat/aktualizovat jazyk</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="language">
          <configuration name="display" values="single" locale="cs">
            <value>Jazyk</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="language_proficiency">
          <configuration name="display" values="single" locale="cs">
            <value>Jazykové znalosti</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_language">
          <configuration name="display" values="single" locale="cs">
            <value>Jazykové znalosti osoby</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonLanguage">
          <configurationGroup name="fields">
            <configurationGroup name="language">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Jazyk</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="speaking">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Plynný</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="reading">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Jazykové znalosti čtení</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="writing">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Jazykové znalosti psaní</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>