<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonLanguage" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonLanguage" path="/I2CE" locale="nl">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="nl">
      <configuration name="person_can_view_child_form_person_language" locale="nl">
        <value>Kan "person_language"-kindformulier van een persoon bekijken</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_language" locale="nl">
        <value>Kan "person_language"-kindformulieren van een persoon bewerken</value>
      </configuration>
      <configuration name="can_edit_database_list_language" locale="nl">
        <value>Kan de databaselijst language bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_language" locale="nl">
        <value>Kan de databaselijst language bekijken</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="language_proficiency">
        <configurationGroup name="elementary">
          <configuration name="fields" value="many" type="delimited" locale="nl">
            <value>name:Elementair</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="limited">
          <configuration name="fields" value="many" type="delimited" locale="nl">
            <value>name:Beperkte ervaring in werk</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="professional">
          <configuration name="fields" value="many" type="delimited" locale="nl">
            <value>name:Professioneel gebruikt</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="full_professional">
          <configuration name="fields" value="many" type="delimited" locale="nl">
            <value>name:Uitgebreid professioneel gebruikt</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="fluent">
          <configuration name="fields" value="many" type="delimited" locale="nl">
            <value>name:Vloeiend</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_language">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Taal toevoegen of bijwerken</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="language">
          <configuration name="display" values="single" locale="nl">
            <value>Taal</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="language_proficiency">
          <configuration name="display" values="single" locale="nl">
            <value>Taalvaardigheid</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_language">
          <configuration name="display" values="single" locale="nl">
            <value>Taalvaardigheid voor persoon</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonLanguage">
          <configurationGroup name="fields">
            <configurationGroup name="language">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Taal</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="speaking">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Spreekvaardigheid</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="reading">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Leesvaardigheid</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="writing">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Schrijfvaardigheid</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>