<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonLanguage" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonLanguage" path="/I2CE" locale="fr_ML">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr_ML">
      <configuration name="person_can_view_child_form_person_language" locale="fr_ML">
        <value>Peut afficher le formulaire enfant de la langue d’une personne</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_language" locale="fr_ML">
        <value>Peut éditer les formulaires enfants de la langue d’une personne</value>
      </configuration>
      <configuration name="can_edit_database_list_language" locale="fr_ML">
        <value>Peut éditer la liste de la base de données de la langue</value>
      </configuration>
      <configuration name="can_view_database_list_language" locale="fr_ML">
        <value>Peut afficher la liste de la base de données d’une langue</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="language_proficiency">
        <configurationGroup name="elementary">
          <configuration name="fields" value="many" type="delimited" locale="fr_ML">
            <value>name:Débutant</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="limited">
          <configuration name="fields" value="many" type="delimited" locale="fr_ML">
            <value>name:Compétence limitée</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="professional">
          <configuration name="fields" value="many" type="delimited" locale="fr_ML">
            <value>name:Compétence au niveau de travail professionnel</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="full_professional">
          <configuration name="fields" value="many" type="delimited" locale="fr_ML">
            <value>name:Compétence profesisionnelle</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="fluent">
          <configuration name="fields" value="many" type="delimited" locale="fr_ML">
            <value>name:Avoir la parole facile.</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_language">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr_ML">
            <value>Ajouter/Mettre à jour la langue</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="language">
          <configuration name="display" values="single" locale="fr_ML">
            <value>Langue</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="language_proficiency">
          <configuration name="display" values="single" locale="fr_ML">
            <value>Compétence en la langue</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_language">
          <configuration name="display" values="single" locale="fr_ML">
            <value>Compétence en la langue d'une personne</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonLanguage">
          <configurationGroup name="fields">
            <configurationGroup name="language">
              <configuration name="headers" type="delimited" locale="fr_ML">
                <value>default:Langue</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="speaking">
              <configuration name="headers" type="delimited" locale="fr_ML">
                <value>default:Compétence en le parlant</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="reading">
              <configuration name="headers" type="delimited" locale="fr_ML">
                <value>default:Compétence en la lecture</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="writing">
              <configuration name="headers" type="delimited" locale="fr_ML">
                <value>default:Compétence en l'écriture</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>