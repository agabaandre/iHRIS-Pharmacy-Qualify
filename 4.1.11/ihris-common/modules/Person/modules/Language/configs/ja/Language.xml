<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonLanguage" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PersonLanguage" path="/I2CE" locale="ja">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="ja">
      <configuration name="person_can_view_child_form_person_language" locale="ja">
        <value>使用言語の子フォームを閲覧できる</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_language" locale="ja">
        <value>使用言語の子フォームを編集できる</value>
      </configuration>
      <configuration name="can_edit_database_list_language" locale="ja">
        <value>使用言語のデータベースリストを編集できる</value>
      </configuration>
      <configuration name="can_view_database_list_language" locale="ja">
        <value>使用言語のデータベースリストを閲覧できる</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="language_proficiency">
        <configurationGroup name="elementary">
          <configuration name="fields" value="many" type="delimited" locale="ja">
            <value>name:基本職</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="limited">
          <configuration name="fields" value="many" type="delimited" locale="ja">
            <value>name:限定職</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="professional">
          <configuration name="fields" value="many" type="delimited" locale="ja">
            <value>name:専門職</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="full_professional">
          <configuration name="fields" value="many" type="delimited" locale="ja">
            <value>name:高度専門職</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="fluent">
          <configuration name="fields" value="many" type="delimited" locale="ja">
            <value>name:堪能です</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_language">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ja">
            <value>言語を追加/更新</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="language">
          <configuration name="display" values="single" locale="ja">
            <value>言語</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="language_proficiency">
          <configuration name="display" values="single" locale="ja">
            <value>言語能力</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_language">
          <configuration name="display" values="single" locale="ja">
            <value>個人の言語能力</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonLanguage">
          <configurationGroup name="fields">
            <configurationGroup name="language">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:言語</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="speaking">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:会話能力</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="reading">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:読解能力</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="writing">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:記述能力</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>