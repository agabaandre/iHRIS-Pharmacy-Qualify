          <configuration name="fields" value="many" type="delimited" locale="sk">
            <value>name:Základné</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="limited">
          <configuration name="fields" value="many" type="delimited" locale="sk">
            <value>name:Obmedzená práca</value>
