      <configuration name="can_view_database_list_language" locale="sk">
        <value>Možno zobraziť zoznam databázy jazyka</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="language_proficiency">
