        <configurationGroup name="fluent">
          <configuration name="fields" value="many" type="delimited" locale="sk">
            <value>name:Plynule</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
