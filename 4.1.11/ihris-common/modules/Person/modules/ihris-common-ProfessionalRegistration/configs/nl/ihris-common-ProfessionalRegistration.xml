<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-common-ProfessionalRegistration" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="ihris-common-ProfessionalRegistration" path="/I2CE" locale="nl">
    <version>4.2.0.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="nl">
      <configuration name="person_can_view_child_form_registration" locale="nl">
        <value>Kan registratiekindformulier van een persoon bekijken</value>
      </configuration>
      <configuration name="person_can_edit_child_form_registration" locale="nl">
        <value>Kan registratiekindformulier van een persoon bewerken</value>
      </configuration>
      <configuration name="can_edit_database_list_council" locale="nl">
        <value>Kan de lijst met beroepsverenigingen bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_council" locale="nl">
        <value>Kan de lijst met beroepsverenigingen bekijken</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="registration">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Registratie toevoegen of bijwerken</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="council">
          <configuration name="display" values="single" locale="nl">
            <value>Beroepsvereniging</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="registration">
          <configuration name="display" values="single" locale="nl">
            <value>Registratie</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Registration">
          <configurationGroup name="fields">
            <configurationGroup name="council">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Beroepsvereniging</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_number">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Registratienummer</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_number">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Licentienummer</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Registratiedatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_expiration">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Vervaldatum licentie</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>