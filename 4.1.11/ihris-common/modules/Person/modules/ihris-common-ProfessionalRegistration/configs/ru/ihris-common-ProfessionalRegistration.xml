<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-common-ProfessionalRegistration" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="ihris-common-ProfessionalRegistration" path="/I2CE" locale="ru">
    <version>4.2.0.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="ru">
      <configuration name="person_can_view_child_form_registration" locale="ru"></configuration>
      <configuration name="person_can_edit_child_form_registration" locale="ru"></configuration>
      <configuration name="can_edit_database_list_council" locale="ru"></configuration>
      <configuration name="can_view_database_list_council" locale="ru"></configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="registration">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ru"></configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="council">
          <configuration name="display" values="single" locale="ru"></configuration>
        </configurationGroup>
        <configurationGroup name="registration">
          <configuration name="display" values="single" locale="ru">
            <value>Регистрация</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Registration">
          <configurationGroup name="fields">
            <configurationGroup name="council">
              <configuration name="headers" type="delimited" locale="ru"></configuration>
            </configurationGroup>
            <configurationGroup name="registration_number">
              <configuration name="headers" type="delimited" locale="ru"></configuration>
            </configurationGroup>
            <configurationGroup name="license_number">
              <configuration name="headers" type="delimited" locale="ru">
                <value>default:Номер лицензии</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_date">
              <configuration name="headers" type="delimited" locale="ru">
                <value>default:Дата регистрации</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_expiration">
              <configuration name="headers" type="delimited" locale="ru">
                <value>default:Срок действия лицензии</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>