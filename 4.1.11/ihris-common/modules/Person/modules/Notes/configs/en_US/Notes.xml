<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonNotes" path="/I2CE">
  <metadata>
    <version>4.3.0</version>
  </metadata>
  <configurationGroup name="PersonNotes" path="/I2CE" locale="en_US">
    <version>4.3.0</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="en_US">
      <configuration name="person_can_view_child_form_notes" locale="en_US">
        <value>Can view notes child form of a person</value>
      </configuration>
      <configuration name="person_can_edit_child_form_notes" locale="en_US">
        <value>Can edit notes child forms of a person</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="notes">
          <configuration name="display" values="single" locale="en_US">
            <value>Notes</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Notes">
          <configurationGroup name="fields">
            <configurationGroup name="note">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Note</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date_added">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Date Added</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="notes">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="en_US">
            <value>Add/Update Notes</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>