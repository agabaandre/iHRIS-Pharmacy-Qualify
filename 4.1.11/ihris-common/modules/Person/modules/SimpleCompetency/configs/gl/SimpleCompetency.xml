<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="person-simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="person-simple-competency" path="/I2CE" locale="gl">
    <version>4.3.0.1</version>
    <configurationGroup name="formsdata" path="/I2CE/formsData/forms">
      <configurationGroup name="competency_evaluation">
        <configurationGroup name="not_evaluated">
          <configurationGroup name="fields">
            <configuration name="name" locale="gl">
              <value>Sen avaliar</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="gl">
              <value>Competente</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="not_competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="gl">
              <value>Non competente</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="person_tasks" path="/I2CE/tasks/task_description" locale="gl">
      <configuration name="person_can_view_child_form_person_competency" locale="gl">
        <value>Pode ollar a ficha person_competency dunha persoa</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_competency" locale="gl">
        <value>Pode editar a ficha person_competency dunha persoa</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation_history" locale="gl">
        <value>Pode ollar o historial de avaliación das competencias dunha persoa</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation" locale="gl">
        <value>Pode ollar a avaliación das competencias dunha persoa</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_evaluation" locale="gl">
        <value>Editar a lista de avaliación das competencias</value>
      </configuration>
      <configuration name="can_view_database_list_competency_evaluation" locale="gl">
        <value>Ollar a lista de avaliación das competencias</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency_evaluation">
          <configuration name="display" values="single" locale="gl">
            <value>Avaliación de competencias</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_competency">
          <configuration name="display" values="single" locale="gl">
            <value>Competencia persoal</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonCompetency">
          <configurationGroup name="fields">
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Última avaliación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Avaliación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency">
              <configuration name="headers" type="delimited" locale="gl">
                <value>default:Competencia</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_competency_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="gl">
            <value>Historial de avaliación das competencias</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_competency">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="gl">
            <value>Engadir/Actualizar as competencias</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>