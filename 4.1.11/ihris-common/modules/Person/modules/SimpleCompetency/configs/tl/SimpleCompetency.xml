<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="person-simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="person-simple-competency" path="/I2CE" locale="tl">
    <version>4.3.0.1</version>
    <configurationGroup name="formsdata" path="/I2CE/formsData/forms">
      <configurationGroup name="competency_evaluation">
        <configurationGroup name="not_evaluated">
          <configurationGroup name="fields">
            <configuration name="name" locale="tl">
              <value>Hindi Nasuri</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="tl">
              <value>May Kakayahang Gumanap</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="not_competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="tl">
              <value>Walang Kakayahang Gumanap</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="person_tasks" path="/I2CE/tasks/task_description" locale="tl">
      <configuration name="person_can_view_child_form_person_competency" locale="tl">
        <value>Matitingnan ang pormularyong pambata sa kakayahang gumanap ng isang tao</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_competency" locale="tl">
        <value>Mababago ang mga pormularyong pambata sa kakayahang gumanap ng isang tao</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation_history" locale="tl">
        <value>Matitingnan ang kasaysayan ng pagsusuri sa kakayahang gumanap ng isang tao</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation" locale="tl">
        <value>Matitingnan ang pagsusuri sa kakayahang gumanap ng isang tao</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_evaluation" locale="tl">
        <value>Baguhin ang tala ng pagsusuri sa kakayahang gumanap</value>
      </configuration>
      <configuration name="can_view_database_list_competency_evaluation" locale="tl">
        <value>Tingnan ang tala ng pagsusuri sa kakayahan sa pagganap</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency_evaluation">
          <configuration name="display" values="single" locale="tl">
            <value>Pagsusuri ng Kakayahang Gumanap</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_competency">
          <configuration name="display" values="single" locale="tl">
            <value>Kakayahang Gumanap ng Tao</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonCompetency">
          <configurationGroup name="fields">
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Huling Pagsusuri</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pagsusuri</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Kakayahang Gumanap</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_competency_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="tl">
            <value>Kasaysayan ng Pagsusuri ng Kakayahan sa Pagganap</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_competency">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="tl">
            <value>Magdagdag/Magsapanahon ng Kakayahang Gumanap</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>