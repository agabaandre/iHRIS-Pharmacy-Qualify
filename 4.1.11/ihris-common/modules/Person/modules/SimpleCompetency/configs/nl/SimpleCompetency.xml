<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="person-simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="person-simple-competency" path="/I2CE" locale="nl">
    <version>4.3.0.1</version>
    <configurationGroup name="formsdata" path="/I2CE/formsData/forms">
      <configurationGroup name="competency_evaluation">
        <configurationGroup name="not_evaluated">
          <configurationGroup name="fields">
            <configuration name="name" locale="nl">
              <value>Niet beoordeeld</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="nl">
              <value>Bevoegd</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="not_competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="nl">
              <value>Niet bevoegd</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="person_tasks" path="/I2CE/tasks/task_description" locale="nl">
      <configuration name="person_can_view_child_form_person_competency" locale="nl">
        <value>Kan "person_competency"-kindformulier van een persoon bekijken</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_competency" locale="nl">
        <value>Kan "person_competency"-kindformulieren van een persoon bewerken</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation_history" locale="nl">
        <value>Kan de competentie-evaluatiegeschiedenis van een persoon bekijken</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation" locale="nl">
        <value>Kan de competentie-evaluatie van een persoon bekijken</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_evaluation" locale="nl">
        <value>Competentie-evaluatielijst bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_competency_evaluation" locale="nl">
        <value>Competentie-evaluatielijst bekijken</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency_evaluation">
          <configuration name="display" values="single" locale="nl">
            <value>Competentieevaluatie</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_competency">
          <configuration name="display" values="single" locale="nl">
            <value>Persoonscompetentie</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonCompetency">
          <configurationGroup name="fields">
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Laatste beoordeling</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Evaluatie</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Competentie</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_competency_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Competentie-evaluatiegeschiedenis</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_competency">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Competentie toevoegen of bijwerken</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>