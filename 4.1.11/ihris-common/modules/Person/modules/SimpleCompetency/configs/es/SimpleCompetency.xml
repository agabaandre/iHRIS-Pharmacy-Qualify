<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="person-simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="person-simple-competency" path="/I2CE" locale="es">
    <version>4.3.0.1</version>
    <configurationGroup name="formsdata" path="/I2CE/formsData/forms">
      <configurationGroup name="competency_evaluation">
        <configurationGroup name="not_evaluated">
          <configurationGroup name="fields">
            <configuration name="name" locale="es">
              <value>Sin Evaluación</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="es">
              <value>Competente</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="not_competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="es">
              <value>Incompetente</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="person_tasks" path="/I2CE/tasks/task_description" locale="es">
      <configuration name="person_can_view_child_form_person_competency" locale="es">
        <value>Puede ver la forma hija de competencia_de_la_persona</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_competency" locale="es">
        <value>Puede modificar la forma hija de competencia_de_la_persona</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation_history" locale="es">
        <value>Puede ver la historia de la evaluación de las compentencias</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation" locale="es">
        <value>Puedes ver la evaluación de competencias de una persona</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_evaluation" locale="es">
        <value>Modificar la lista de evaluación de competencias</value>
      </configuration>
      <configuration name="can_view_database_list_competency_evaluation" locale="es">
        <value>Ver la lista de evaluación de competencias</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency_evaluation">
          <configuration name="display" values="single" locale="es">
            <value>Evaluación de Competencia</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_competency">
          <configuration name="display" values="single" locale="es">
            <value>Competencias</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonCompetency">
          <configurationGroup name="fields">
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Última evaluación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Evaluación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Competencia</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_competency_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="es">
            <value>Histórico de evaluaciones de competencias</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_competency">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="es">
            <value>Añadir/modificar competencia</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>