<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="person-simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="person-simple-competency" path="/I2CE" locale="uk">
    <version>4.3.0.1</version>
    <configurationGroup name="formsdata" path="/I2CE/formsData/forms">
      <configurationGroup name="competency_evaluation">
        <configurationGroup name="not_evaluated">
          <configurationGroup name="fields">
            <configuration name="name" locale="uk">
              <value>Не оцінено</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="uk">
              <value>Компетентний</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="not_competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="uk">
              <value>Не компетентний</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="person_tasks" path="/I2CE/tasks/task_description" locale="uk">
      <configuration name="person_can_view_child_form_person_competency" locale="uk">
        <value>Можна переглядати персональну кваліфікацію дочірньої форми особи</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_competency" locale="uk">
        <value>Можна редагувати персональну кваліфікацію дочірньої форми особи</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation_history" locale="uk">
        <value>Можна переглядати історію оцінок кваліфікації особи</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation" locale="uk">
        <value>Можна переглянути оцінки кваліфікації людини</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_evaluation" locale="uk">
        <value>Редагувати список оцінок кваліфікації</value>
      </configuration>
      <configuration name="can_view_database_list_competency_evaluation" locale="uk">
        <value>Переглянути список оцінок кваліфікації</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency_evaluation">
          <configuration name="display" values="single" locale="uk">
            <value>Оцінка кваліфікації</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_competency">
          <configuration name="display" values="single" locale="uk">
            <value>Кваліфікації особи</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonCompetency">
          <configurationGroup name="fields">
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Останнє випробування</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Випробування</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency">
              <configuration name="headers" type="delimited" locale="uk">
                <value>default:Кваліфікація</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_competency_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="uk">
            <value>Історія оцінок кваліфікації</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_competency">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="uk">
            <value>Додати/оновити кваліфікацію</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>