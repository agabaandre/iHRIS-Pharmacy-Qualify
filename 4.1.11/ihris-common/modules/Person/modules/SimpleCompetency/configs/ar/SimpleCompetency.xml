<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="person-simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="person-simple-competency" path="/I2CE" locale="ar">
    <version>4.3.0.1</version>
    <configurationGroup name="formsdata" path="/I2CE/formsData/forms">
      <configurationGroup name="competency_evaluation">
        <configurationGroup name="not_evaluated">
          <configurationGroup name="fields">
            <configuration name="name" locale="ar">
              <value>لم يتم التقييم</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="ar">
              <value>كفء</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="not_competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="ar">
              <value>غير كفء</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="person_tasks" path="/I2CE/tasks/task_description" locale="ar">
      <configuration name="person_can_view_child_form_person_competency" locale="ar">
        <value>يمكن عرض&#160;نموذج الكفاءة الفرعي لشخص</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_competency" locale="ar">
        <value>يمكن تعديل&#160;نموذج الكفاءة الفرعي لشخص</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation_history" locale="ar">
        <value>يمكن&#160;عرض سجل تقييم شخص</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation" locale="ar">
        <value>يمكن&#160;عرض تقييم كفاءة شحص</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_evaluation" locale="ar">
        <value>تعديل قائمة&#160;تقييم&#160;الكفاءة</value>
      </configuration>
      <configuration name="can_view_database_list_competency_evaluation" locale="ar">
        <value>عرض قائمة&#160;تقييم&#160;الكفاءة</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency_evaluation">
          <configuration name="display" values="single" locale="ar">
            <value>تقييم&#160;الكفاءة</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_competency">
          <configuration name="display" values="single" locale="ar">
            <value>كفاءة شخص</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonCompetency">
          <configurationGroup name="fields">
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:أحر تقييم</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:تقييم</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:الكفاءة</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_competency_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ar">
            <value>سجل تقييم&#160;المهارات</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_competency">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ar">
            <value>إضافة / تحديث &#160;كفاءة</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>