<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="person-simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="person-simple-competency" path="/I2CE" locale="it">
    <version>4.3.0.1</version>
    <configurationGroup name="formsdata" path="/I2CE/formsData/forms">
      <configurationGroup name="competency_evaluation">
        <configurationGroup name="not_evaluated">
          <configurationGroup name="fields">
            <configuration name="name" locale="it">
              <value>Non valutato</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="it">
              <value>Competente</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="not_competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="it">
              <value>Non competente</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="person_tasks" path="/I2CE/tasks/task_description" locale="it">
      <configuration name="person_can_view_child_form_person_competency" locale="it">
        <value>Visualizza il sottomodulo delle competenze di una persona</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_competency" locale="it">
        <value>Modifica il sottomodulo delle competenze di una persona</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation_history" locale="it">
        <value>Visualizza lo storico della valutazione delle competenze di una persona</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation" locale="it">
        <value>Visualizza la valutazione delle competenze di una persona</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_evaluation" locale="it">
        <value>Modifica l'elenco delle valutazioni delle competenze</value>
      </configuration>
      <configuration name="can_view_database_list_competency_evaluation" locale="it">
        <value>Visualizza l'elenco delle valutazioni delle competenze</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency_evaluation">
          <configuration name="display" values="single" locale="it">
            <value>Valutazione delle competenze</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_competency">
          <configuration name="display" values="single" locale="it">
            <value>Competenze personali</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonCompetency">
          <configurationGroup name="fields">
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Ultima valutata</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Valutazione</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Competenza</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_competency_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="it">
            <value>Storico della valutazione delle competenze</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_competency">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="it">
            <value>Aggiungi/aggiorna competenza</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>