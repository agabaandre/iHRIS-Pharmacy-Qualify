<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="person-simple-competency" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="person-simple-competency" path="/I2CE" locale="zh_CN">
    <version>4.2.0.1</version>
    <configurationGroup name="formsdata" path="/I2CE/formsData/forms">
      <configurationGroup name="competency_evaluation">
        <configurationGroup name="not_evaluated">
          <configurationGroup name="fields">
            <configuration name="name" locale="zh_CN">
              <value>不计算</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="zh_CN">
              <value>主管</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="not_competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="zh_CN">
              <value>不合格</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="person_tasks" path="/I2CE/tasks/task_description" locale="zh_CN">
      <configuration name="person_can_view_child_form_person_competency" locale="zh_CN">
        <value>可以查看该人的个人能力的子窗体</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_competency" locale="zh_CN">
        <value>可以编辑该人的个人能力的子窗体</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation_history" locale="zh_CN">
        <value>看一个人的能力评价历史</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation" locale="zh_CN">
        <value>看一个人的能力评价</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_evaluation" locale="zh_CN">
        <value>编辑能力评价列表</value>
      </configuration>
      <configuration name="can_view_database_list_competency_evaluation" locale="zh_CN">
        <value>查看能力评价列表</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency_evaluation">
          <configuration name="display" values="single" locale="zh_CN">
            <value>能力评价</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_competency">
          <configuration name="display" values="single" locale="zh_CN">
            <value>人的能力</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonCompetency">
          <configurationGroup name="fields">
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:最后评估</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:评价</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:能力</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_competency_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="zh_CN">
            <value>资格评价历史</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_competency">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="zh_CN">
            <value>添加更新能力</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>