<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="person-simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="person-simple-competency" path="/I2CE" locale="fr">
    <version>4.3.0.1</version>
    <configurationGroup name="formsdata" path="/I2CE/formsData/forms">
      <configurationGroup name="competency_evaluation">
        <configurationGroup name="not_evaluated">
          <configurationGroup name="fields">
            <configuration name="name" locale="fr">
              <value>Non évalué</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="fr">
              <value>Compétent</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="not_competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="fr">
              <value>Non compétent</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="person_tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="person_can_view_child_form_person_competency" locale="fr">
        <value>Peut afficher le formulaire-enfant personnel d’une personne</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_competency" locale="fr">
        <value>Peu éditer les formulaires-enfant personnels d’une personne</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation_history" locale="fr">
        <value>Peut afficher l’historique de l’évaluation de la compétence d’une personne</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation" locale="fr">
        <value>Impossible d'afficher l'évaluation des compétences de cette personne</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_evaluation" locale="fr">
        <value>Editer la liste d’évaluation de la compétence</value>
      </configuration>
      <configuration name="can_view_database_list_competency_evaluation" locale="fr">
        <value>Afficher la liste d’évaluation de la compétence</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency_evaluation">
          <configuration name="display" values="single" locale="fr">
            <value>Évaluation de compétence</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_competency">
          <configuration name="display" values="single" locale="fr">
            <value>Compétence personnelle</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonCompetency">
          <configurationGroup name="fields">
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Dernière évaluation</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="fr"></configuration>
            </configurationGroup>
            <configurationGroup name="competency">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Compétences</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_competency_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Historique d’évaluation de la compétence</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_competency">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Ajouter/Actualiser la compétence</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>