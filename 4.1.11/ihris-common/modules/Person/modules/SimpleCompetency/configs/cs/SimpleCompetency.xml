<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="person-simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="person-simple-competency" path="/I2CE" locale="cs">
    <version>4.3.0.1</version>
    <configurationGroup name="formsdata" path="/I2CE/formsData/forms">
      <configurationGroup name="competency_evaluation">
        <configurationGroup name="not_evaluated">
          <configurationGroup name="fields">
            <configuration name="name" locale="cs">
              <value>Nehodnoceno</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="cs">
              <value>Kompetentní</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="not_competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="cs">
              <value>Nekompetentní</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="person_tasks" path="/I2CE/tasks/task_description" locale="cs">
      <configuration name="person_can_view_child_form_person_competency" locale="cs">
        <value>Může zobrazit podřazený formulář person_competency osoby</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_competency" locale="cs">
        <value>Může upravit podřazený formulář person_competency osoby</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation_history" locale="cs">
        <value>Může zobrazit historii vyhodnocení kompetence osoby</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation" locale="cs">
        <value>Může zobrazit vyhodnocení kompetence osoby</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_evaluation" locale="cs">
        <value>Upravit seznam ohodnocení kompetence</value>
      </configuration>
      <configuration name="can_view_database_list_competency_evaluation" locale="cs">
        <value>Zobrazit seznam ohodnocení kompetence</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency_evaluation">
          <configuration name="display" values="single" locale="cs">
            <value>Vyhodnocení kompetence</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_competency">
          <configuration name="display" values="single" locale="cs">
            <value>Kompetence osoby</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonCompetency">
          <configurationGroup name="fields">
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Naposledy vyhodnoceno</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Vyhodnocení</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency">
              <configuration name="headers" type="delimited" locale="cs">
                <value>default:Kompetence</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_competency_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="cs">
            <value>Historie vyhodnocení kompetence</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_competency">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="cs">
            <value>Přidat/Aktualizovat kompetenci</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>