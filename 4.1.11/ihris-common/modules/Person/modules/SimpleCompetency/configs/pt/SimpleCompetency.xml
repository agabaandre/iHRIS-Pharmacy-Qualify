<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="person-simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="person-simple-competency" path="/I2CE" locale="pt">
    <version>4.3.0.1</version>
    <configurationGroup name="formsdata" path="/I2CE/formsData/forms">
      <configurationGroup name="competency_evaluation">
        <configurationGroup name="not_evaluated">
          <configurationGroup name="fields">
            <configuration name="name" locale="pt">
              <value>Não Avaliado</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="pt">
              <value>Competente</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="not_competent">
          <configurationGroup name="fields">
            <configuration name="name" locale="pt">
              <value>Não é Competente</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="person_tasks" path="/I2CE/tasks/task_description" locale="pt">
      <configuration name="person_can_view_child_form_person_competency" locale="pt">
        <value>Pode visualizar os child forms sobre a competência de uma pessoa</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_competency" locale="pt">
        <value>Pode editar os child forms sobre a competência de uma pessoa</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation_history" locale="pt">
        <value>Pode visualizar o histórico de avaliação da competência de uma pessoa</value>
      </configuration>
      <configuration name="person_competency_can_view_evaluation" locale="pt">
        <value>Pode visualizar a avaliação da competência de uma pessoa</value>
      </configuration>
      <configuration name="can_edit_database_list_competency_evaluation" locale="pt">
        <value>Editar a lista de avaliação da competência</value>
      </configuration>
      <configuration name="can_view_database_list_competency_evaluation" locale="pt">
        <value>Visualizar a lista de avaliação da competência</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="competency_evaluation">
          <configuration name="display" values="single" locale="pt">
            <value>Avaliação da Competência</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="person_competency">
          <configuration name="display" values="single" locale="pt">
            <value>Competência da Pessoa</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonCompetency">
          <configurationGroup name="fields">
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Última Avaliação</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Avaliação</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Competência</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_competency_history">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt">
            <value>Histórico de Avaliação da Competência</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_competency">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt">
            <value>Adicionar/Actualizar Competência</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>