<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Person" path="/I2CE">
  <metadata>
    <version>4.1.10.0.1</version>
  </metadata>
  <configurationGroup name="Person" path="/I2CE" locale="pt_BR">
    <version>4.1.9.0.1</version>
    <configurationGroup name="page_feedback_messages" path="/modules/forms/page_feedback_messages">
      <configuration name="person_save" locale="pt_BR">
        <value>Este registro foi salvo</value>
      </configuration>
      <configuration name="person_child_save" locale="pt_BR">
        <value>Este registro foi salvo</value>
      </configuration>
      <configuration name="person_child_delete_success" locale="pt_BR"></configuration>
      <configuration name="person_child_delete_fail" locale="pt_BR"></configuration>
      <configuration name="person_child_delete_not_permitted" locale="pt_BR"></configuration>
      <configuration name="person_child_delete_not_allowed" locale="pt_BR"></configuration>
    </configurationGroup>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="pt_BR">
      <configuration name="person_surname" path="person/surname/unique">
        <value>Registros duplicados coincidem com o nome deste registro:</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pt_BR">
      <configuration name="person_can_view" locale="pt_BR">
        <value>Pode visualizar a página da pessoa</value>
      </configuration>
      <configuration name="person_can_edit" locale="pt_BR">
        <value>Pode editar a página da pessoa</value>
      </configuration>
      <configuration name="person_can_view_child_forms" locale="pt_BR"></configuration>
      <configuration name="person_can_edit_child_forms" locale="pt_BR"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="person">
          <configuration name="display" values="single" locale="pt_BR">
            <value>Pessoa</value>
          </configuration>
          <configurationGroup name="meta">
            <configuration name="description" locale="pt_BR">
              <value>Este formulário abrange informações básicas de uma pessoa, tais como nome e residência</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Person">
          <configurationGroup name="fields">
            <configurationGroup name="surname">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Sobrenome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="firstname">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Primeiro Nome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="othername">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Outros Nomes</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="nationality">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Nacionalidade</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="residence">
              <configuration name="headers" type="delimited" values="many" locale="pt_BR">
                <value>default:Residência</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt_BR">
            <value>Adicionar/Atualizar Pessoa</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt_BR">
            <value>Visualizar Pessoa</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>