<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Geography" path="/I2CE">
  <metadata>
    <version>4.1.11.0.1</version>
  </metadata>
  <configurationGroup name="Geography" path="/I2CE" locale="nl">
    <version>4.1.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="nl">
      <configuration name="can_edit_database_list_country" locale="nl">
        <value>Kan de landenlijst bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_country" locale="nl">
        <value>Kan de landenlijst bekijken</value>
      </configuration>
      <configuration name="can_edit_database_list_region" locale="nl">
        <value>Kan de regiolijst bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_region" locale="nl">
        <value>Kan de regiolijst bekijken</value>
      </configuration>
      <configuration name="can_edit_database_list_district" locale="nl">
        <value>Kan de districtenlijst bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_district" locale="nl">
        <value>Kan de districtenlijst bekijken</value>
      </configuration>
      <configuration name="can_edit_database_list_county" locale="nl">
        <value>Kan de landenlijst bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_county" locale="nl">
        <value>Kan de landenlijst bekijken</value>
      </configuration>
      <configuration name="can_edit_geographic_database_lists" locale="nl">
        <value>Kan enige geografische gegevens bewerken</value>
      </configuration>
      <configuration name="can_view_geographic_database_lists" locale="nl">
        <value>Kan enige geografische gegevens bekijken</value>
      </configuration>
      <configuration name="can_edit_all_geographic_database_lists" locale="nl">
        <value>Kan alle geografische gegevens bewerken</value>
      </configuration>
      <configuration name="can_view_all_geographic_database_lists" locale="nl">
        <value>Kan alle geografische gegevens bekijken</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="country">
          <configuration name="display" values="single" locale="nl">
            <value>Land</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="county">
          <configuration name="display" values="single" locale="nl">
            <value>District</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="district">
          <configuration name="display" values="single" locale="nl"></configuration>
        </configurationGroup>
        <configurationGroup name="region">
          <configuration name="display" values="single" locale="nl">
            <value>Regio</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ListByCountry">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Naam</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Locatie</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Country">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" values="many" locale="nl">
                <value>default:Naam</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="alpha_two">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:2-teken alphacode</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:ISO numerieke code</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="primary">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Primair land</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Gebruiken voor locatieselectie</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_County">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Naam</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="district">
              <configuration name="headers" type="delimited" locale="nl"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_District">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Naam</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="region">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Regio</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="nl"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Region">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Naam</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="country">
              <configuration name="headers" type="delimited" values="many" locale="nl">
                <value>default:Land</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="nl"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>