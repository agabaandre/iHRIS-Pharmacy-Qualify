<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Geography" path="/I2CE">
  <metadata>
    <version>4.1.11.0.1</version>
  </metadata>
  <configurationGroup name="Geography" path="/I2CE" locale="pt">
    <version>4.1.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pt">
      <configuration name="can_edit_database_list_country" locale="pt">
        <value>Pode editar lista de países</value>
      </configuration>
      <configuration name="can_view_database_list_country" locale="pt">
        <value>Pode visualizar lista de países</value>
      </configuration>
      <configuration name="can_edit_database_list_region" locale="pt">
        <value>Pode editar lista de regiões</value>
      </configuration>
      <configuration name="can_view_database_list_region" locale="pt">
        <value>Pode visualizar lista de regiões</value>
      </configuration>
      <configuration name="can_edit_database_list_district" locale="pt">
        <value>Pode editar lista de distritos</value>
      </configuration>
      <configuration name="can_view_database_list_district" locale="pt">
        <value>Pode visualizar lista de distritos</value>
      </configuration>
      <configuration name="can_edit_database_list_county" locale="pt">
        <value>Pode editar lista de países</value>
      </configuration>
      <configuration name="can_view_database_list_county" locale="pt">
        <value>Pode visualizar lista de países</value>
      </configuration>
      <configuration name="can_edit_geographic_database_lists" locale="pt">
        <value>Pode editar alguns dados geográficos</value>
      </configuration>
      <configuration name="can_view_geographic_database_lists" locale="pt">
        <value>Pode visualizar alguns dados geográficos</value>
      </configuration>
      <configuration name="can_edit_all_geographic_database_lists" locale="pt">
        <value>Pode editar todos os dados geográficos</value>
      </configuration>
      <configuration name="can_view_all_geographic_database_lists" locale="pt">
        <value>Pode visualizar todos os dados geográficos</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="country">
          <configuration name="display" values="single" locale="pt">
            <value>País</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="county">
          <configuration name="display" values="single" locale="pt">
            <value>País</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="district">
          <configuration name="display" values="single" locale="pt">
            <value>Distrito</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="region">
          <configuration name="display" values="single" locale="pt">
            <value>Região</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ListByCountry">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Nome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Local</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Country">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" values="many" locale="pt">
                <value>default:Nome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="alpha_two">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Código Alfa de 2 Caracteres</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Códio Numérico ISO</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="primary">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:País Primário</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Usar para Seleção do Local</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_County">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Nome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="district">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Distrito</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_District">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Nome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="region">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Região</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Código</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Region">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Nome</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="country">
              <configuration name="headers" type="delimited" values="many" locale="pt">
                <value>default:País</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="pt">
                <value>default:Código</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>