<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="training-course" path="/I2CE">
  <metadata>
    <version>4.1.11.0</version>
  </metadata>
  <configurationGroup name="training-course" path="/I2CE" locale="en_US">
    <version>4.1.9.1</version>
    <configurationGroup name="tranlatable_strings" locale="en_US" path="/modules/training-course/translatable-strings" type="delimited">
      <value>add_student:Add Student</value>
      <value>edit_student:Edit Student</value>
      <value>remove_student:Remove Student</value>
      <value>no_edit_students:You do not have permission to edit students for this course instance.</value>
    </configurationGroup>
    <configurationGroup name="invalid_messages" path="/modules/forms/formfield_messages" locale="en_US">
      <configuration name="work" path="scheduled_training_course/end_date/bad_date">
        <value>The end date must be after the start date.</value>
      </configuration>
      <configuration name="work" path="person_scheduled_training_course/training_course_mod/bad_mod">
        <value>You have selected at least one module that is not available for the course:</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsdata" path="/I2CE/formsData/forms">
      <configurationGroup name="training_course_status">
        <configurationGroup name="open">
          <configurationGroup name="fields">
            <configuration name="name" locale="en_US">
              <value>Open</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="closed">
          <configurationGroup name="fields">
            <configuration name="name" locale="en_US">
              <value>Closed</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="training_course_evaluation">
        <configurationGroup name="pass">
          <configurationGroup name="fields">
            <configuration name="name" locale="en_US">
              <value>Pass</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="fail">
          <configurationGroup name="fields">
            <configuration name="name" locale="en_US">
              <value>Fail</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="incomplete">
          <configurationGroup name="fields">
            <configuration name="name" locale="en_US">
              <value>Incomplete</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="not_evaluated">
          <configurationGroup name="fields">
            <configuration name="name" locale="en_US">
              <value>Not Evaluated</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="roles" path="roles/names">
      <configurationGroup name="training_manager">
        <configuration name="display_name" values="single" locale="en_US">
          <value>Training Manager</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_training_course_status" locale="en_US">
        <value>Edit the training course status list</value>
      </configuration>
      <configuration name="can_view_database_list_training_course_status" locale="en_US">
        <value>View the training course status list</value>
      </configuration>
      <configuration name="can_edit_database_list_training_course_mod" locale="en_US">
        <value>Edit the training course modules list</value>
      </configuration>
      <configuration name="can_view_database_list_training_course_mod" locale="en_US">
        <value>View the training course modules list</value>
      </configuration>
      <configuration name="can_edit_database_list_training_course_status" locale="en_US">
