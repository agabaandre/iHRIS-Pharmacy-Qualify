      <configurationGroup name="editstudents">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="en_US">
            <value>Edit Students</value>
          </configuration>
          <configuration name="action_header" values="single" locale="en_US">
            <value>Add/Remove Student</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="student_training_participation" path="/modules/CustomReports/relationships/student_training_participation">
        <configuration name="display_name" locale="en_US">
          <value>Scheduled Course Traniing Participation</value>
        </configuration>
        <configuration name="description" locale="en_US">
          <value>List of all training partipipation By Scheduled Course</value>
        </configuration>
        <configurationGroup name="joins">
          <configurationGroup name="person_scheduled_training_course">
            <configuration name="display_name" locale="en_US">
              <value>Person Scheduled Course</value>
            </configuration>
            <configurationGroup name="joins">
              <configurationGroup name="person">
                <configuration name="display_name" locale="en_US">
                  <value>person</value>
                </configuration>
                <configurationGroup name="joins">
                  <configurationGroup name="person_position">
                    <configuration name="display_name" locale="en_US">
                      <value>Current Position</value>
                    </configuration>
                    <configurationGroup name="joins">
                      <configurationGroup name="position">
                        <configuration name="display_name" locale="en_US">
                          <value>Position</value>
                        </configuration>
                        <configurationGroup name="joins">
                          <configurationGroup name="facility">
                            <configuration name="display_name" locale="en_US">
                              <value>facility</value>
                            </configuration>
                            <configurationGroup name="joins">
                              <configurationGroup name="facility_contact">
                                <configuration name="display_name" locale="en_US">
                                  <value>Facility Contact</value>
                                </configuration>
                              </configurationGroup>
                            </configurationGroup>
                          </configurationGroup>
                        </configurationGroup>
                      </configurationGroup>
                    </configurationGroup>
                  </configurationGroup>
                  <configurationGroup name="person_contact">
                    <configuration name="display_name" locale="en_US">
                      <value>Person Contact</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
