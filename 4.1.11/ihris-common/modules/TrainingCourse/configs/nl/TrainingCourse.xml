                    </configurationGroup>
                  </configurationGroup>
                  <configurationGroup name="person_contact">
                    <configuration name="display_name" locale="nl">
                      <value>Persoonlijk contactpersoon</value>
                    </configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
            </configurationGroup>
