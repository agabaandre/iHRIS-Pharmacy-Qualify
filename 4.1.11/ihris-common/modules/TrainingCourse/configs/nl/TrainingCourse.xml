      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Training_Course_Evaluation">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Naam</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Competentieevaluatie</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Training_Course">
          <configurationGroup name="fields">
            <configurationGroup name="training_course_mod">
              <configuration name="headers" type="delimited" locale="nl"></configuration>
            </configurationGroup>
            <configurationGroup name="has_certification_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Certificeringsdatum gebruiker</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="certificate">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Certificaeringssjabloon</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="topic">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Onderwerp</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Opmerkingen</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training_institution">
