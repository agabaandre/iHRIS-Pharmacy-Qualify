              </configuration>
            </configurationGroup>
            <configurationGroup name="continuing_education_course">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Aantal CUE's</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training_course_status">
              <configuration name="headers" type="delimited" locale="nl"></configuration>
            </configurationGroup>
            <configurationGroup name="training_course_category">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Categorie</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Person_Scheduled_Training_Course">
          <configurationGroup name="fields">
            <configurationGroup name="training_course_mod">
              <configuration name="headers" type="delimited" locale="nl"></configuration>
            </configurationGroup>
            <configurationGroup name="certification_date">
              <configuration name="headers" locale="nl" type="delimited">
                <value>default:Certificeringsdatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="attending">
              <configuration name="headers" locale="nl" type="delimited">
                <value>default:Neemt deel</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="is_retraining">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Omscholing</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="completed">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Voltooid</value>
