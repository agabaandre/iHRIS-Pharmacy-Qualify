            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Begindatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Einddatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" values="many" locale="nl">
                <value>default:Locatie</value>
              </configuration>
            </configurationGroup>
