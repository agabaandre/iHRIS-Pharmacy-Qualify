          <configuration name="action_header" values="single" locale="nl">
            <value>Student toevoegen of verwijderen</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="student_training_participation" path="/modules/CustomReports/relationships/student_training_participation">
        <configuration name="display_name" locale="nl"></configuration>
        <configuration name="description" locale="nl">
          <value>Lijst met alle trainingsdeelnames per geplande cursus</value>
        </configuration>
        <configurationGroup name="joins">
          <configurationGroup name="person_scheduled_training_course">
            <configuration name="display_name" locale="nl">
              <value>Geplande cursus van persoon</value>
            </configuration>
            <configurationGroup name="joins">
              <configurationGroup name="person">
                <configuration name="display_name" locale="nl">
                  <value>persoon</value>
                </configuration>
                <configurationGroup name="joins">
                  <configurationGroup name="person_position">
                    <configuration name="display_name" locale="nl">
                      <value>Huidige positie</value>
                    </configuration>
                    <configurationGroup name="joins">
                      <configurationGroup name="position">
                        <configuration name="display_name" locale="nl">
                          <value>Positie</value>
                        </configuration>
                        <configurationGroup name="joins">
                          <configurationGroup name="facility">
                            <configuration name="display_name" locale="nl">
                              <value>instelling</value>
                            </configuration>
                            <configurationGroup name="joins">
