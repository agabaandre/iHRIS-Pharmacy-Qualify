              </configuration>
            </configurationGroup>
            <configurationGroup name="training_course_requestor">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Hiniling Ni</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="scheduled_training_course">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pagkakataon</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
