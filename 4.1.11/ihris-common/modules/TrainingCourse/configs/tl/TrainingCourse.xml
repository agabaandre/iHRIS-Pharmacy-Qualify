        <configurationGroup name="training_course_mod">
          <configuration name="display" values="single" locale="tl"></configuration>
        </configurationGroup>
        <configurationGroup name="training_course_status">
          <configuration name="display" values="single" locale="tl">
            <value>Katayuan ng Kurso ng Pagsasanay</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="training_course_category">
          <configuration name="display" values="single" locale="tl">
            <value>Kategorya ng Kurso ng Pagsasanay</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="training_course">
          <configuration name="display" values="single" locale="tl">
            <value>Kurso ng Pagsasanay</value>
          </configuration>
          <configurationGroup name="meta">
            <configuration name="description" locale="tl">
              <value>Ang pormularyong ito ay humahawak ng saligang kabatiran tungkol sa isang kurso ng pagsasanay</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="scheduled_training_course">
