              <configuration name="headers" type="delimited" values="many" locale="tl">
                <value>default:Kurso ng Pagsasanay</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Sityo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="num_students">
              <configuration name="headers" type="delimited" locale="tl">
