            <configuration name="name" locale="tl">
              <value>Buksan</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="closed">
          <configurationGroup name="fields">
            <configuration name="name" locale="tl">
              <value>Nakasara na</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="training_course_evaluation">
        <configurationGroup name="pass">
          <configurationGroup name="fields">
            <configuration name="name" locale="tl">
              <value>Pumasa</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="fail">
          <configurationGroup name="fields">
            <configuration name="name" locale="tl">
              <value>Nabigo</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="incomplete">
          <configurationGroup name="fields">
            <configuration name="name" locale="tl">
              <value>Kulang</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="not_evaluated">
          <configurationGroup name="fields">
            <configuration name="name" locale="tl">
              <value>Hindi Nasuri</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="roles" path="roles/names">
