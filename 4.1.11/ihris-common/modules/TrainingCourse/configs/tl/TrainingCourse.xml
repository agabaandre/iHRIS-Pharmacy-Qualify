              <configuration name="headers" type="delimited" locale="tl"></configuration>
            </configurationGroup>
            <configurationGroup name="certification_date">
              <configuration name="headers" locale="tl" type="delimited"></configuration>
            </configurationGroup>
            <configurationGroup name="attending">
              <configuration name="headers" locale="tl" type="delimited"></configuration>
