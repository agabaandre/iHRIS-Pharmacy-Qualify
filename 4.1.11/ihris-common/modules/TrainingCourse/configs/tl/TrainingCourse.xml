                            <configuration name="display_name" locale="tl"></configuration>
                            <configurationGroup name="joins">
                              <configurationGroup name="facility_contact">
                                <configuration name="display_name" locale="tl"></configuration>
                              </configurationGroup>
                            </configurationGroup>
                          </configurationGroup>
