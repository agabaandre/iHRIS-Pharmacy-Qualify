          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pangalan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Pagsusuri ng Kakayahan sa Pagganap</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Training_Course">
          <configurationGroup name="fields">
            <configurationGroup name="training_course_mod">
              <configuration name="headers" type="delimited" locale="tl"></configuration>
            </configurationGroup>
            <configurationGroup name="has_certification_date">
              <configuration name="headers" type="delimited" locale="tl"></configuration>
            </configurationGroup>
            <configurationGroup name="certificate">
              <configuration name="headers" type="delimited" locale="tl"></configuration>
            </configurationGroup>
            <configurationGroup name="topic">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Paksa</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="notes">
