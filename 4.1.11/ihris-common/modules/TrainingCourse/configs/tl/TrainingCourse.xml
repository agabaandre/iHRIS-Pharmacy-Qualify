      <configuration name="can_edit_database_list_training_course_status" locale="tl">
        <value>Baguhin ang tala ng katayuan ng kurso ng pagsasanay</value>
      </configuration>
      <configuration name="can_view_database_list_training_course_status" locale="tl">
        <value>Tingnan ang tala ng katayuan ng kurso ng pagsasanay</value>
      </configuration>
      <configuration name="can_edit_database_list_training_course_evaluation" locale="tl">
        <value>Baguhin ang tala ng pagsusuri ng kurso ng pagsasanay</value>
