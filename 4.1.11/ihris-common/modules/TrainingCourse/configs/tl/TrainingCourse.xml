            <configurationGroup name="training_course_status">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Katayuan</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training_course_category">
              <configuration name="headers" type="delimited" locale="tl">
                <value>default:Kategorya</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
