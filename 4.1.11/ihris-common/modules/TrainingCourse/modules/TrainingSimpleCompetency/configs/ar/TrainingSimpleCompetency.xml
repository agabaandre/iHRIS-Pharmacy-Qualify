<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="training-simple-competency" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="training-simple-competency" path="/I2CE" locale="ar">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_scheduled_training_course_can_edit_evaluation" locale="ar">
        <value>قم بتقييم دورة تدريبية مجدولة لشخص</value>
      </configuration>
      <configuration name="person_scheduled_training_course_evaluation_can_view_evaluation" locale="ar">
        <value>عرض تقييم دورة تدريبية مجدولة لشخص</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="training_course_competency_evaluation">
          <configuration name="display" values="single" locale="ar">
            <value>تقييم كفاءة الدورة التدريبية</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Training_Course_Competency_Evaluation">
          <configurationGroup name="fields">
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:ملاحظات</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:تاريخ التقييم</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency_evaluation">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:تقييم</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="competency">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:كفاءة</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Training_Course">
          <configurationGroup name="fields">
            <configurationGroup name="competency">
              <configurationGroup name="meta">
                <configurationGroup name="list">
                  <configurationGroup name="default">
                    <configuration name="display_string" locale="ar"></configuration>
                  </configurationGroup>
                </configurationGroup>
              </configurationGroup>
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:كفاءات ممنوحة</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="evaluate-competencies" path="/modules/training-course/page/evaluate-competencies">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ar">
            <value>قم بتقييم كفاءات الدورة</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>