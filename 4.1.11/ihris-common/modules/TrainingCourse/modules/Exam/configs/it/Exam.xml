<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="training-exam" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="training-exam" path="/I2CE" locale="it">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_scheduled_training_course_can_edit_exam" locale="it">
        <value>Valuta il corso di formazione in programma per una persona</value>
      </configuration>
      <configuration name="person_scheduled_training_course_can_view_exam" locale="it">
        <value>Visualizza la valutazione di un corso di formazione in programma per una persona</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="training_course_exam_type">
          <configuration name="display" values="single" locale="it">
            <value>Tipo di esame del corso di formazione</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="training_course_exam">
          <configuration name="display" values="single" locale="it">
            <value>Esame del corso di formazione</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Training_Course_Exam">
          <configurationGroup name="fields">
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Note</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Data di valutazione</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training_course_exam_type">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Tipo di esame</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="score">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Punteggio</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Training_Course">
          <configurationGroup name="fields">
            <configurationGroup name="passing_score">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Punteggio per il superamento dell'esame finale</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="exam" path="/modules/training-course/page/exam">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="it">
            <value>Risultati dell'esame del corso di formazione</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>