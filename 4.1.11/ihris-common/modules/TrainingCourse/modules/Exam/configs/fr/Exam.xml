<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="training-exam" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="training-exam" path="/I2CE" locale="fr">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_scheduled_training_course_can_edit_exam" locale="fr">
        <value>Evaluer les compétences d’une personne à une formation programmée</value>
      </configuration>
      <configuration name="person_scheduled_training_course_can_view_exam" locale="fr">
        <value>Afficher l’évaluation d'une personne à une formation programmée</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="training_course_exam_type">
          <configuration name="display" values="single" locale="fr">
            <value>Type d'examen à une formation</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="training_course_exam">
          <configuration name="display" values="single" locale="fr">
            <value>Test d'Evaluation à une formation</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Training_Course_Exam">
          <configurationGroup name="fields">
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="fr"></configuration>
            </configurationGroup>
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date d’évaluation</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training_course_exam_type">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Type d'Examen</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="score">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Résultat</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Training_Course">
          <configurationGroup name="fields">
            <configurationGroup name="passing_score">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Résultat de passage à l’examen final</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="exam" path="/modules/training-course/page/exam">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Résultats à l'examen d'une formation</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>