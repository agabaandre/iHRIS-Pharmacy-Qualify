<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="training-exam" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="training-exam" path="/I2CE" locale="ja">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_scheduled_training_course_can_edit_exam" locale="ja">
        <value>トレーニング コース参加者の評価</value>
      </configuration>
      <configuration name="person_scheduled_training_course_can_view_exam" locale="ja">
        <value>トレーニング コース参加者の評価を閲覧する。</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="training_course_exam_type">
          <configuration name="display" values="single" locale="ja">
            <value>トレーニング コースの試験タイプ</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="training_course_exam">
          <configuration name="display" values="single" locale="ja">
            <value>トレーニング コースの試験</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Training_Course_Exam">
          <configurationGroup name="fields">
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:注釈</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:評価日</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training_course_exam_type">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:試験タイプ</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="score">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:スコア</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Training_Course">
          <configurationGroup name="fields">
            <configurationGroup name="passing_score">
              <configuration name="headers" type="delimited" locale="ja">
                <value>default:最終試験の合格スコア</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="exam" path="/modules/training-course/page/exam">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ja">
            <value>トレーニング コース試験の結果</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>