<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="training-exam" path="/I2CE">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="training-exam" path="/I2CE" locale="zh_CN">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="person_scheduled_training_course_can_edit_exam" locale="zh_CN">
        <value>评价一个人的预定培训课程</value>
      </configuration>
      <configuration name="person_scheduled_training_course_can_view_exam" locale="zh_CN">
        <value>查看一个人的另作安排训练的课程评价</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="training_course_exam_type">
          <configuration name="display" values="single" locale="zh_CN">
            <value>培训课程考试类型</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="training_course_exam">
          <configuration name="display" values="single" locale="zh_CN">
            <value>培训课程考试</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Training_Course_Exam">
          <configurationGroup name="fields">
            <configurationGroup name="notes">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:备注</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="evaluation_date">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:评估日期</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training_course_exam_type">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:考试类型</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="score">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:分数</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Training_Course">
          <configurationGroup name="fields">
            <configurationGroup name="passing_score">
              <configuration name="headers" type="delimited" locale="zh_CN">
                <value>default:通过为期末考试的分数</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="exam" path="/modules/training-course/page/exam">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="zh_CN">
            <value>培训课程的考试成绩</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>