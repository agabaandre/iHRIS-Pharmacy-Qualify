<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PersonAddQualification" path="/I2CE">
  <metadata>
    <version>4.0.9.0</version>
  </metadata>
  <configurationGroup name="PersonAddQualification" path="/I2CE" locale="en_US">
    <version>4.0.9.0</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="en_US">
      <configuration name="person_can_view_child_form_addqualification" locale="en_US">
        <value>Can view addqualification child form of a person</value>
      </configuration>
      <configuration name="person_can_edit_child_form_addqualification" locale="en_US">
        <value>Can edit addqualification child forms of a person</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="addqualification">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="en_US">
            <value>Add/Update Additional Qualification History</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="addqualification">
          <configuration name="display" values="single" locale="en_US">
            <value>Additional Qualification History</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        
        <configurationGroup name="iHRIS_AddQualification">
          <configurationGroup name="fields">
            <configurationGroup name="certificate">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Qualification</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="institution">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Institution Name</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Institution Location</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="year">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Year of Graduation</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="speciality">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Speciality(Where Applicable)</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>
