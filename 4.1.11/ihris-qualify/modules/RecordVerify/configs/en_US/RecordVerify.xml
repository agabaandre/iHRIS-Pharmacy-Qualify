<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="RecordVerify" path="/I2CE">
  <metadata>
    <version>4.2.2.0</version>
  </metadata>
  <configurationGroup name="RecordVerify" path="/I2CE" locale="en_US">
    <version>4.2.0</version>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="record_verify">
          <configuration name="display" values="single" locale="en_US">
            <value>Record Verification</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="verify_change">
          <configuration name="display" values="single" locale="en_US">
            <value>Verification Change</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_RecordVerify">
          <configurationGroup name="fields">
            <configurationGroup name="verify_date">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Verification Date</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="verify_change">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Changes Made</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="record_verify">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="en_US">
            <value>Verify Record</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>