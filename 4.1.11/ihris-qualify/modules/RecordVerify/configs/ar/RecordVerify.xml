<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="RecordVerify" path="/I2CE">
  <metadata>
    <version>4.1.0.1</version>
  </metadata>
  <configurationGroup name="RecordVerify" path="/I2CE" locale="ar">
    <version>4.1.0.1</version>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="record_verify">
          <configuration name="display" values="single" locale="ar">
            <value>سجل&#160;للتدقيق</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="verify_change">
          <configuration name="display" values="single" locale="ar">
            <value>تدقيق&#160;التغيير</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_RecordVerify">
          <configurationGroup name="fields">
            <configurationGroup name="verify_date">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:التدقيق في&#160;تاريخ</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="verify_change">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:التغييرات التي تمت</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="record_verify">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ar">
            <value>دقق في&#160;سجل</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>