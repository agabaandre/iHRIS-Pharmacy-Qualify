              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="training">
          <configurationGroup name="fields">
            <configurationGroup name="cadre">
              <configuration name="header" locale="nl">
                <value>Opleidingkader-ID</value>
              </configuration>
