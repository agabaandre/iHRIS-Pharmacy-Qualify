<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-qualify-CustomReports-search-people" path="/modules/CustomReports">
  <metadata>
    <version>4.1.0.1</version>
  </metadata>
  <configurationGroup name="ihris-qualify-CustomReports-search-people" path="/modules/CustomReports" locale="es">
    <version>4.1.0.1</version>
    <configurationGroup name="search_people" path="relationships/search_people">
      <configuration name="display_name" locale="es">
        <value>Buscar Personas</value>
      </configuration>
      <configuration name="description" locale="es">
        <value>La relación usada para la pagina de búsqueda de la persona.</value>
      </configuration>
      <configurationGroup name="joins">
        <configurationGroup name="last_training">
          <configuration name="description" locale="es">
            <value>El entrenamiento mas reciente que esta persona ha tomado.</value>
          </configuration>
          <configuration name="display_name" locale="es">
            <value>Ultimo entrenamiento</value>
          </configuration>
          <configurationGroup name="joins">
            <configurationGroup name="cadre">
              <configuration name="description" locale="es">
                <value>El puesto del entrenamiento mas reciente</value>
              </configuration>
              <configuration name="display_name" locale="es">
                <value>Puesto</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="demographic">
          <configuration name="description" locale="es">
            <value>Detalles demográficos de esta persona.</value>
          </configuration>
          <configuration name="display_name" locale="es">
            <value>Demográfico</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="SearchPeopleReport" path="reports/search_people">
      <configurationGroup name="meta">
        <configuration name="display_name" locale="es">
          <value>Buscar Personas</value>
        </configuration>
        <configuration name="description" locale="es">
          <value>Buscar cualquier persona en el sistema con el entrenamiento más reciente.</value>
        </configuration>
        <configuration name="category" locale="es">
          <value>Reportes de Búsqueda</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="reporting_forms">
        <configurationGroup name="primary_form">
          <configurationGroup name="fields">
            <configurationGroup name="surname">
              <configurationGroup name="limits">
                <configurationGroup name="contains">
                  <configuration name="header" locale="es">
                    <value>Apellido</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="es">
                <value>Apellido</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="firstname">
              <configuration name="header" locale="es">
                <value>Nombre</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="othername">
              <configuration name="header" locale="es">
                <value>Otros Nombres</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="last_training">
          <configurationGroup name="fields">
            <configurationGroup name="index_num">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="es">
                    <value>Número de índice</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="es">
                <value>Número de índice</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="cadre">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="es">
                    <value>Puesto</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="es">
                <value>ID de Puesto de Entrenamiento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="intake_date">
              <configuration name="header" locale="es">
                <value>Fecha de Admisión</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="cadre">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="es">
                <value>Puesto</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="SearchPeopleView" path="reportViews/search_people">
      <configuration name="description" locale="es">
        <value>Buscar personas en el sistema</value>
      </configuration>
      <configuration name="display_name" locale="es">
        <value>Buscar Personas</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>