<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-qualify-CustomReports-disruption" path="//modules/CustomReports">
  <metadata>
    <version>4.1.0.1</version>
