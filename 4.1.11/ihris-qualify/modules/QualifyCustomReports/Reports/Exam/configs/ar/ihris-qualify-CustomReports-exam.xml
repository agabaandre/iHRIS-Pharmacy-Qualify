              </configurationGroup>
              <configurationGroup name="StackedColumn">
                <configuration name="display_name" locale="ar">
                  <value>رسم بياني عمودي (بسيط)</value>
                </configuration>
              </configurationGroup>
              <configurationGroup name="Scatter">
                <configuration name="display_name" locale="ar">
                  <value>رسم بياني بالنقاط المبعثرة</value>
                </configuration>
              </configurationGroup>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>