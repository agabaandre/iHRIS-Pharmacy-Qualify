<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-qualify-CustomReports-institution" path="/modules/CustomReports">
  <metadata>
    <version>4.1.0.1</version>
  </metadata>
  <configurationGroup name="ihris-qualify-CustomReports-institution" path="/modules/CustomReports" locale="pt_BR">
    <version>4.1.0.1</version>
    <configurationGroup name="relationship-institution" path="relationships/institution">
      <configuration name="display_name" locale="pt_BR">
        <value>Instituição de Formação</value>
      </configuration>
      <configuration name="description" locale="pt_BR">
        <value>Relação pra todas as instituições de treinamento no sistema</value>
      </configuration>
      <configurationGroup name="joins">
        <configurationGroup name="facility_contact">
          <configuration name="description" locale="pt_BR">
            <value>Detalhes de contato pra este instituição</value>
          </configuration>
          <configuration name="display_name" locale="pt_BR">
            <value>Contato</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="facility_agent">
          <configuration name="description" locale="pt_BR">
            <value>O agente de Instalação para esta instituição</value>
          </configuration>
          <configuration name="display_name" locale="pt_BR">
            <value>Agente de Instalação</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="district">
          <configuration name="description" locale="pt_BR">
            <value>Distrito onde se situa a instituição</value>
          </configuration>
          <configuration name="display_name" locale="pt_BR">
            <value>Distrito</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="report-institution" path="reports/institution">
      <configurationGroup name="meta">
        <configuration name="display_name" locale="pt_BR">
          <value>Relatório da Instituição de Treinamento</value>
        </configuration>
        <configuration name="description" locale="pt_BR">
          <value>Uma lista de todas as instituições de treinamento.</value>
        </configuration>
        <configuration name="category" locale="pt_BR">
          <value>Instituições de Treinamento</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="reporting_forms">
        <configurationGroup name="primary_form">
          <configurationGroup name="fields">
            <configurationGroup name="facility_agent">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="pt_BR">
                    <value>Agente de Instalação</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="pt_BR">
                <value>ID do Agente de Instalação da Instituição de Treinamento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="header" locale="pt_BR">
                <value>Nome da Instituição de treinamento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="pt_BR">
                    <value>Local da Instituição de Treinamento</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="pt_BR">
                <value>ID do Local da Instituição de Treinamento</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="district">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="pt_BR">
                <value>Distrito</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="facility_agent">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="pt_BR">
                <value>Agente de Instalação</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="facility_contact">
          <configurationGroup name="fields">
            <configurationGroup name="address">
              <configuration name="header" locale="pt_BR">
                <value>Endereço de Correspondência</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportView-institution" path="reportViews/institution">
      <configuration name="description" locale="pt_BR">
        <value>Uma lista de todas as instituições de treinamento.</value>
      </configuration>
      <configuration name="display_name" locale="pt_BR">
        <value>Instituições de Treinamento</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>