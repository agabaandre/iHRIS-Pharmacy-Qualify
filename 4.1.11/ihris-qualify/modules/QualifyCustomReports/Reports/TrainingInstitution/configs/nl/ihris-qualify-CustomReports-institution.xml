<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-qualify-CustomReports-institution" path="/modules/CustomReports">
  <metadata>
    <version>4.1.0.1</version>
  </metadata>
  <configurationGroup name="ihris-qualify-CustomReports-institution" path="/modules/CustomReports" locale="nl">
    <version>4.1.0.1</version>
    <configurationGroup name="relationship-institution" path="relationships/institution">
      <configuration name="display_name" locale="nl">
        <value>Opleidingsinstituut</value>
      </configuration>
      <configuration name="description" locale="nl">
        <value>Relatie voor alle opleidingsinstituten in het systeem.</value>
      </configuration>
      <configurationGroup name="joins">
        <configurationGroup name="facility_contact">
          <configuration name="description" locale="nl">
            <value>De contactgegevens voor deze instelling.</value>
          </configuration>
          <configuration name="display_name" locale="nl">
            <value>Contactpersoon</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="facility_agent">
          <configuration name="description" locale="nl">
            <value>De faciliteitsagent voor deze instelling.</value>
          </configuration>
          <configuration name="display_name" locale="nl">
            <value>Faciliteitsagent</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="district">
          <configuration name="description" locale="nl">
            <value>Het district waar deze instelling is gelegen.</value>
          </configuration>
          <configuration name="display_name" locale="nl"></configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="report-institution" path="reports/institution">
      <configurationGroup name="meta">
        <configuration name="display_name" locale="nl">
          <value>Opleidingsinstellingenrapportages</value>
        </configuration>
        <configuration name="description" locale="nl">
          <value>Een lijst van alle opleidingsinstituten.</value>
        </configuration>
        <configuration name="category" locale="nl">
          <value>Opleidingsinstituten</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="reporting_forms">
        <configurationGroup name="primary_form">
          <configurationGroup name="fields">
            <configurationGroup name="facility_agent">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="nl">
                    <value>Faciliteitsagent</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="nl">
                <value>Faciliteitsagent-ID van de opleidingsinstelling</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="header" locale="nl">
                <value>Naam opleidingsinstituut</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="nl">
                    <value>Locatie opleidingsinstituut</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="nl">
                <value>Locatie-ID opleidingsinstituut</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="district">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="nl"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="facility_agent">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="nl">
                <value>Faciliteitsagent</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="facility_contact">
          <configurationGroup name="fields">
            <configurationGroup name="address">
              <configuration name="header" locale="nl">
                <value>Postadres</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportView-institution" path="reportViews/institution">
      <configuration name="description" locale="nl">
        <value>Een lijst van alle opleidingsinstituten.</value>
      </configuration>
      <configuration name="display_name" locale="nl">
        <value>Opleidingsinstituten</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>