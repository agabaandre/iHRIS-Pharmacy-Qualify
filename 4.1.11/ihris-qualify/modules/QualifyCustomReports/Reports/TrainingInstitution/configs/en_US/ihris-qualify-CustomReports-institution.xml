<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-qualify-CustomReports-institution" path="/modules/CustomReports">
  <metadata>
    <version>4.2.2.0</version>
  </metadata>
  <configurationGroup name="ihris-qualify-CustomReports-institution" path="/modules/CustomReports" locale="en_US">
    <version>4.2.0</version>
    <configurationGroup name="relationship-institution" path="relationships/institution">
      <configuration name="display_name" locale="en_US">
        <value>Training Institution</value>
      </configuration>
      <configuration name="description" locale="en_US">
        <value>Relationship for all training institutions in the system.</value>
      </configuration>
      <configurationGroup name="joins">
        <configurationGroup name="facility_contact">
          <configuration name="description" locale="en_US">
            <value>The contact details for this institution.</value>
          </configuration>
          <configuration name="display_name" locale="en_US">
            <value>Contact</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="facility_agent">
          <configuration name="description" locale="en_US">
            <value>The facility agent for this institution.</value>
          </configuration>
          <configuration name="display_name" locale="en_US">
            <value>Facility Agent</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="district">
          <configuration name="description" locale="en_US">
            <value>The district where this institution is located.</value>
          </configuration>
          <configuration name="display_name" locale="en_US">
            <value>District</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="report-institution" path="reports/institution">
      <configurationGroup name="meta">
        <configuration name="display_name" locale="en_US">
          <value>Training Institution Reports</value>
        </configuration>
        <configuration name="description" locale="en_US">
          <value>A list of all training institutions.</value>
        </configuration>
        <configuration name="category" locale="en_US">
          <value>Training Institutions</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="reporting_forms">
        <configurationGroup name="primary_form">
          <configurationGroup name="fields">
            <configurationGroup name="facility_agent">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="en_US">
                    <value>Facility Agent</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="en_US">
                <value>Training Institution Facility Agent ID</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="name">
              <configuration name="header" locale="en_US">
                <value>Training Institution Name</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configurationGroup name="limits">
                <configurationGroup name="equals">
                  <configuration name="header" locale="en_US">
                    <value>Training Institution Location</value>
                  </configuration>
                </configurationGroup>
              </configurationGroup>
              <configuration name="header" locale="en_US">
                <value>Training Institution Location ID</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="district">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="en_US">
                <value>District</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="facility_agent">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="header" locale="en_US">
                <value>Facility Agent</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="facility_contact">
          <configurationGroup name="fields">
            <configurationGroup name="address">
              <configuration name="header" locale="en_US">
                <value>Mailing Address</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="reportView-institution" path="reportViews/institution">
      <configuration name="description" locale="en_US">
        <value>A list of all training institutions.</value>
      </configuration>
      <configuration name="display_name" locale="en_US">
        <value>Training Institutions</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>