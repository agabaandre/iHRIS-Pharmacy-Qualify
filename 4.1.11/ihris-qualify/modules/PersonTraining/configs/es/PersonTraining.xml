<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-qualify-PersonTraining" path="/I2CE">
  <metadata>
    <version>4.1.7.0.1</version>
  </metadata>
  <configurationGroup name="ihris-qualify-PersonTraining" path="/I2CE" locale="es">
    <version>4.1.7.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="es">
      <configuration name="person_can_view_child_form_person_training" locale="es">
        <value>Puede ver el formulario secundario de capacitación de una persona</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_training" locale="es">
        <value>Puede editar el formulario secundario de capacitación de una persona</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="person_training">
          <configuration name="display" values="single" locale="es">
            <value>Educación continua</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonTraining">
          <configurationGroup name="fields">
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Fecha de inicio</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Fecha de finalización</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Curso de capacitación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="provider">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Proveedor de capacitación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Lugar de capacitación</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="cpd_credit">
              <configuration name="headers" type="delimited" locale="es">
                <value>default:Crédito CPD</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_training">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="es">
            <value>Educación continua</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>